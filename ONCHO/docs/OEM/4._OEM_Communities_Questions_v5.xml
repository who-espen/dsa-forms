<h:html xmlns="http://www.w3.org/2002/xforms"
  xmlns:h="http://www.w3.org/1999/xhtml"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns:jr="http://openrosa.org/javarosa">
  <h:head>
    <h:title>4. (English) OEM Breeding site survey - Community questions v5 - Mozambique</h:title>
    <model>
      <instance>
        <data id="OEMCommunityQuestionsv5_Mozambique">
          <meta>
            <instanceID/>
          </meta>
          <q_StartTime/>
          <q_EndTime/>
          <q_RecorderID/>
          <q_DistrictID/>
          <q_SiteName/>

          <q_Consent/>
          <q_Sex/>
          <q_AgeYrs/>
          <q_occupation/>
          <q_HowLongLived/>

          <q_BitesProblems/>
          <q_EndSurveyNote/>
          <q_BitesPerDay/>
          <q_KnowBlackflySeason/>
          <q_BlackflySeason/>

          <q_Notes/>
        </data>
      </instance>
      <itext>
        <translation lang="English">
          <text id="/data/q_RecorderID:label">
            <value>Enter Recorder ID</value>
          </text>
          <text id="/data/q_RecorderID:hint">
            <value>2 digit code assigned to you</value>
          </text>
          <!-- District label begin -->
          <text id="/data/q_DistrictID:label">
            <value>Select District</value>
          </text>

          <text id="/data/q_DistrictID:option0">
            <value>Cahora Bassa</value>
          </text>
          <text id="/data/q_DistrictID:option1">
            <value>Lago</value>
          </text>
          <text id="/data/q_DistrictID:option2">
            <value>Maravia</value>
          </text>
          <text id="/data/q_DistrictID:option3">
            <value>Mechanhelas</value>
          </text>
          <text id="/data/q_DistrictID:option4">
            <value>Milange </value>
          </text>
          <text id="/data/q_DistrictID:option5">
            <value>Morrumbala</value>
          </text>
          <text id="/data/q_DistrictID:option6">
            <value>Sanga</value>
          </text>
          <text id="/data/q_DistrictID:option7">
            <value>Other district</value>
          </text>

          <!-- District label end -->

          <text id="/data/q_SiteName:label">
            <value>Enter the name of breeding site surveyed </value>
          </text>
          <text id="/data/q_SiteName:hint">
            <value>The name of the community you are in</value>
          </text>
          <text id="/data/q_Consent:label">
            <value>Does this person consent to take part in the study?</value>
          </text>
          <text id="/data/q_Consent:hint">
            <value>No response will end survey</value>
          </text>
          <text id="/data/q_Consent:option0">
            <value>Yes</value>
          </text>
          <text id="/data/q_Consent:option1">
            <value>No</value>
          </text>

          <text id="/data/q_Sex:label">
            <value>What gender is this person</value>
          </text>
          <text id="/data/q_Sex:option0">
            <value>Female</value>
          </text>
          <text id="/data/q_Sex:option1">
            <value>Male</value>
          </text>
          <text id="/data/q_AgeYrs:label">
            <value>Enter age in years</value>
          </text>
          <text id="/data/q_occupation:label">
            <value>What is their occupation</value>
          </text>
          <text id="/data/q_HowLongLived:label">
            <value>How many years has the respondent lived in the area?</value>
          </text>

          <!-- blackfly problem begin -->
          <text id="/data/q_BitesProblems:label">
            <value>Are blackfly bites a problem in this community? </value>
          </text>
          <text id="/data/q_BitesProblems:hint">
            <value>check local name for blackfly, show picture or actual black fly in a glass container </value>
          </text>
          <text id="/data/q_BitesProblems:option0">
            <value>Yes</value>
          </text>
          <text id="/data/q_BitesProblems:option1">
            <value>No</value>
          </text>
          <text id="/data/q_BitesProblems:option2">
            <value>Don't know</value>
          </text>
          <!-- blackfly problem end -->

          <text id="/data/q_EndSurveyNote:label">
            <value>You just entered a value that will end the survey</value>
          </text>

          <!-- Bites per day begin -->

          <text id="/data/q_BitesPerDay:label">
            <value>How many bites do you receive each day? </value>
          </text>
          <text id="/data/q_BitesPerDay:option0">
            <value>None</value>
          </text>
          <text id="/data/q_BitesPerDay:option1">
            <value>A few (&lt;=10)</value>
          </text>
          <text id="/data/q_BitesPerDay:option2">
            <value>Some (&gt;10 and &lt;=50)</value>
          </text>
          <text id="/data/q_BitesPerDay:option3">
            <value>Many (&gt;50)</value>
          </text>
          <text id="/data/q_BitesPerDay:option4">
            <value>Don't know</value>
          </text>
          <!-- Bite per day end -->

          <text id="/data/q_KnowBlackflySeason:label">
            <value>Do you know the time of year when there are the most black fly bites? </value>
          </text>
          <text id="/data/q_KnowBlackflySeason:option0">
            <value>Yes</value>
          </text>
          <text id="/data/q_KnowBlackflySeason:option1">
            <value>No</value>
          </text>

          <!-- Black fly season begin -->
          <text id="/data/q_BlackflySeason:label">
            <value>If Yes, select the month(s)</value>
          </text>
          <text id="/data/q_BlackflySeason:option0">
            <value>January</value>
          </text>
          <text id="/data/q_BlackflySeason:option1">
            <value>February</value>
          </text>
          <text id="/data/q_BlackflySeason:option2">
            <value>March</value>
          </text>
          <text id="/data/q_BlackflySeason:option3">
            <value>April</value>
          </text>
          <text id="/data/q_BlackflySeason:option4">
            <value>May</value>
          </text>
          <text id="/data/q_BlackflySeason:option5">
            <value>June</value>
          </text>
          <text id="/data/q_BlackflySeason:option6">
            <value>July</value>
          </text>
          <text id="/data/q_BlackflySeason:option7">
            <value>August</value>
          </text>
          <text id="/data/q_BlackflySeason:option8">
            <value>September</value>
          </text>
          <text id="/data/q_BlackflySeason:option9">
            <value>October</value>
          </text>
          <text id="/data/q_BlackflySeason:option10">
            <value>November</value>
          </text>
          <text id="/data/q_BlackflySeason:option11">
            <value>December</value>
          </text>

          <!-- Black fly season end -->

          <text id="/data/q_Notes:label">
            <value>Additional notes to add?</value>
          </text>
        </translation>
      </itext>

      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
      <bind nodeset="/data/q_StartTime" type="dateTime" jr:preload="timestamp" jr:preloadParams="start"/>
      <bind nodeset="/data/q_EndTime" type="dateTime" jr:preload="timestamp" jr:preloadParams="end"/>
      <bind nodeset="/data/q_RecorderID" type="int" required="true()" maxLength="2" constraint="regex(., '^[0-9]{2}$')" jr:constraintMsg="Invalid. Recorder ID code is 2 numbers long."/>
      <bind nodeset="/data/q_DistrictID" type="select1" required="true()" db_add_col_1="."/>
      <bind nodeset="/data/q_SiteName" type="string" required="true()" />
      <bind nodeset="/data/q_Consent" type="select1" required="true()"/>
      <bind nodeset="/data/q_Sex" type="select1" required="true()" relevant="(selected(/data/q_Consent, 'Yes')) " />
      <bind nodeset="/data/q_AgeYrs" type="int" required="true()" relevant="(selected(/data/q_Consent, 'Yes'))" constraint=".  &#60; 121" jr:constraintMsg="Enter a valid age" />
      <bind nodeset="/data/q_occupation" required="true()" relevant="(selected(/data/q_Consent, 'Yes'))" type="string" />
      <bind nodeset="/data/q_HowLongLived" type="int" relevant="(selected(/data/q_Consent, 'Yes'))" constraint=". &#60;= /data/q_AgeYrs" jr:constraintMsg="The value less than or equal to the age" />
      <bind nodeset="/data/q_BitesProblems" type="select1" required="true()" relevant="(selected(/data/q_Consent, 'Yes'))" />
      <bind nodeset="/data/q_BitesPerDay" type="select1" required="true()" relevant="(selected(/data/q_BitesProblems, 'Yes')) and (selected(/data/q_Consent, 'Yes'))" />
      <bind nodeset="/data/q_KnowBlackflySeason" type="select1" required="true()" relevant="(selected(/data/q_BitesProblems, 'Yes')) and (selected(/data/q_Consent, 'Yes'))" />
      <bind nodeset="/data/q_BlackflySeason" type="select" required="true()" relevant="(selected(/data/q_BitesProblems, 'Yes')) and (selected(/data/q_KnowBlackflySeason, 'Yes')) and (selected(/data/q_Consent, 'Yes'))" />
      <bind nodeset="/data/q_EndSurveyNote" readonly="true()" type="string" relevant="(selected(/data/q_Consent, 'No'))" />
      <bind nodeset="/data/q_Notes" type="string" />
    </model>
  </h:head>
  <h:body>
    <input ref="/data/q_RecorderID">
      <label ref="jr:itext('/data/q_RecorderID:label')"/>
      <hint ref="jr:itext('/data/q_RecorderID:hint')"/>
    </input>
    <!-- District Begin -->
    <select1 ref="/data/q_DistrictID">
      <label ref="jr:itext('/data/q_DistrictID:label')"/>
      <item>
        <label ref="jr:itext('/data/q_DistrictID:option0')"/>
        <value>Cahora Bassa; 33224</value>
      </item>
      <item>
        <label ref="jr:itext('/data/q_DistrictID:option1')"/>
        <value>Lago; 33196</value>
      </item>
      <item>
        <label ref="jr:itext('/data/q_DistrictID:option2')"/>
        <value>Maravia; 33233</value>
      </item>
      <item>
        <label ref="jr:itext('/data/q_DistrictID:option3')"/>
        <value>Mechanhelas; 33203</value>
      </item>
      <item>
        <label ref="jr:itext('/data/q_DistrictID:option4')"/>
        <value>Milange ; 33248</value>
      </item>
      <item>
        <label ref="jr:itext('/data/q_DistrictID:option5')"/>
        <value>Morrumbala; 33254</value>
      </item>
      <item>
        <label ref="jr:itext('/data/q_DistrictID:option6')"/>
        <value>Sanga; 33209</value>
      </item>
      <item>
        <label ref="jr:itext('/data/q_DistrictID:option7')"/>
        <value>Other district</value>
      </item>
    </select1>
    <!-- District label End -->
    <input ref="/data/q_SiteName">
      <label ref="jr:itext('/data/q_SiteName:label')"/>
      <hint ref="jr:itext('/data/q_SiteName:hint')"/>
    </input>

    <select1 ref="/data/q_Consent">
      <label ref="jr:itext('/data/q_Consent:label')"/>
      <hint ref="jr:itext('/data/q_Consent:hint')"/>
      <item>
        <label ref="jr:itext('/data/q_Consent:option0')"/>
        <hint ref="jr:itext('/data/q_Consent:hint')"/>
        <value>Yes</value>
      </item>
      <item>
        <label ref="jr:itext('/data/q_Consent:option1')"/>
        <value>No</value>
      </item>
    </select1>

    <select1 ref="/data/q_Sex">
      <label ref="jr:itext('/data/q_Sex:label')"/>
      <item>
        <label ref="jr:itext('/data/q_Sex:option0')"/>
        <value>Female</value>
      </item>
      <item>
        <label ref="jr:itext('/data/q_Sex:option1')"/>
        <value>Male</value>
      </item>
    </select1>
    <input ref="/data/q_AgeYrs">
      <label ref="jr:itext('/data/q_AgeYrs:label')"/>
    </input>
    <input ref="/data/q_occupation">
      <label ref="jr:itext('/data/q_occupation:label')"/>
    </input>
    <input ref="/data/q_HowLongLived">
      <label ref="jr:itext('/data/q_HowLongLived:label')"/>
    </input>

    <!-- blackfly problem begin -->
    <select1 ref="/data/q_BitesProblems">
      <label ref="jr:itext('/data/q_BitesProblems:label')"/>
      <hint ref="jr:itext('/data/q_BitesProblems:hint')"/>
      <item>
        <label ref="jr:itext('/data/q_BitesProblems:option0')"/>
        <value>Yes</value>
      </item>
      <item>
        <label ref="jr:itext('/data/q_BitesProblems:option1')"/>
        <value>No</value>
      </item>
      <item>
        <label ref="jr:itext('/data/q_BitesProblems:option2')"/>
        <value>Don't know</value>
      </item>
    </select1>
    <!-- blackfly problem end -->

    <input ref="/data/q_EndSurveyNote">
      <label ref="jr:itext('/data/q_EndSurveyNote:label')"/>
    </input>

    <!-- Bites per day begin -->
    <select1 ref="/data/q_BitesPerDay">
      <label ref="jr:itext('/data/q_BitesPerDay:label')"/>
      <item>
        <label ref="jr:itext('/data/q_BitesPerDay:option0')"/>
        <value>None</value>
      </item>
      <item>
        <label ref="jr:itext('/data/q_BitesPerDay:option1')"/>
        <value>A few (&lt;=10)o</value>
      </item>
      <item>
        <label ref="jr:itext('/data/q_BitesPerDay:option2')"/>
        <value>Some (&gt;10 and &lt;=50)</value>
      </item>
      <item>
        <label ref="jr:itext('/data/q_BitesPerDay:option3')"/>
        <value>Many (&gt;50)</value>
      </item>
      <item>
        <label ref="jr:itext('/data/q_BitesPerDay:option4')"/>
        <value>Don't know</value>
      </item>
    </select1>
    <!-- Bites per day end -->

    <select1 ref="/data/q_KnowBlackflySeason">
      <label ref="jr:itext('/data/q_KnowBlackflySeason:label')"/>
      <item>
        <label ref="jr:itext('/data/q_KnowBlackflySeason:option0')"/>
        <value>Yes</value>
      </item>
      <item>
        <label ref="jr:itext('/data/q_KnowBlackflySeason:option1')"/>
        <value>No</value>
      </item>
    </select1>

    <!-- Black fly season begin -->
    <select ref="/data/q_BlackflySeason">
      <label ref="jr:itext('/data/q_BlackflySeason:label')"/>
      <item>
        <label ref="jr:itext('/data/q_BlackflySeason:option0')"/>
        <value>January</value>
      </item>
      <item>
        <label ref="jr:itext('/data/q_BlackflySeason:option1')"/>
        <value>February</value>
      </item>
      <item>
        <label ref="jr:itext('/data/q_BlackflySeason:option2')"/>
        <value>March</value>
      </item>
      <item>
        <label ref="jr:itext('/data/q_BlackflySeason:option3')"/>
        <value>April</value>
      </item>
      <item>
        <label ref="jr:itext('/data/q_BlackflySeason:option4')"/>
        <value>May</value>
      </item>
      <item>
        <label ref="jr:itext('/data/q_BlackflySeason:option5')"/>
        <value>June</value>
      </item>
      <item>
        <label ref="jr:itext('/data/q_BlackflySeason:option6')"/>
        <value>July</value>
      </item>
      <item>
        <label ref="jr:itext('/data/q_BlackflySeason:option7')"/>
        <value>August</value>
      </item>
      <item>
        <label ref="jr:itext('/data/q_BlackflySeason:option8')"/>
        <value>September</value>
      </item>
      <item>
        <label ref="jr:itext('/data/q_BlackflySeason:option9')"/>
        <value>October</value>
      </item>
      <item>
        <label ref="jr:itext('/data/q_BlackflySeason:option10')"/>
        <value>November</value>
      </item>
      <item>
        <label ref="jr:itext('/data/q_BlackflySeason:option11')"/>
        <value>December</value>
      </item>
    </select>
    <!-- Black fly season end -->

    <input ref="/data/q_Notes">
      <label ref="jr:itext('/data/q_Notes:label')"/>
    </input>

  </h:body>
</h:html>
