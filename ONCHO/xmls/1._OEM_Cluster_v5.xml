      <bind nodeset="/data/c_ClusterID2" type="int" required="true()" maxLength="3" constraint=". =/data/c_ClusterID1" db_add_col_1="/data/c_DistrictID" db_add_col_2="/data/c_EU" db_add_col_3="."/>
