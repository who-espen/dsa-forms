<h:html xmlns="http://www.w3.org/2002/xforms" 
  xmlns:h="http://www.w3.org/1999/xhtml" 
  xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
  xmlns:jr="http://openrosa.org/javarosa">
  <h:head>
    <h:title>3. OEM : Pesquisa do local de reprodução - Avaliação do rio v5 - Moçambique</h:title>
    <model>
      <instance>
        <data id="OEMRiverAssessmentv5_Mozambique.pt">
          <meta>
            <instanceID/>
          </meta>
          <r_StartTime/>
          <r_EndTime/>
          <r_DistrictID/>
          <r_SiteName/>
          <r_Month/>
          <r_Year/>
          <r_GPS/>
          <r_RiverName/>
          <r_RiverBasin/>
          <r_SiteSuitable/>
          <r_EvidenceOfLarvae/>
          <r_LarvaeRate/>
          <r_Notes/>
        </data>
      </instance>
      <itext>
        <translation lang="English">
          <text id="/data/r_DistrictID:label">
            <value>Seleccione o nome do distrito</value>
          </text>

          <!-- District label begin -->

          <text id="/data/r_DistrictID:option0">
            <value>Buzi</value>
          </text>
          <text id="/data/r_DistrictID:option1">
            <value>Gôndola</value>
          </text>
          <text id="/data/r_DistrictID:option2">
            <value>Lago</value>
          </text>
          <text id="/data/r_DistrictID:option3">
            <value>Machaze</value>
          </text>
          <text id="/data/r_DistrictID:option4">
            <value>Manica</value>
          </text>
          <text id="/data/r_DistrictID:option5">
            <value>Maravia</value>
          </text>
          <text id="/data/r_DistrictID:option6">
            <value>Sanga</value>
          </text>
          <text id="/data/r_DistrictID:option7">
            <value>Spared District</value>
          </text>

          <!-- District label end -->

          <text id="/data/r_SiteName:label">
            <value>Nome da comunidade pesquisada</value>
          </text>
          <text id="/data/r_SiteName:hint">
            <value>A comunidade mais próxima do local de reprodução</value>
          </text>
          <text id="/data/r_Month:label">
            <value>Mês de pesquisa</value>
          </text>

          <!-- Month begin -->

          <text id="/data/r_Month:option0">
            <value>Janeiro</value>
          </text>
          <text id="/data/r_Month:option1">
            <value>Fevereiro</value>
          </text>
          <text id="/data/r_Month:option2">
            <value>Março</value>
          </text>
          <text id="/data/r_Month:option3">
            <value>Abril</value>
          </text>
          <text id="/data/r_Month:option4">
            <value>Maio</value>
          </text>
          <text id="/data/r_Month:option5">
            <value>Junho</value>
          </text>
          <text id="/data/r_Month:option6">
            <value>Julho</value>
          </text>
          <text id="/data/r_Month:option7">
            <value>Agosto</value>
          </text>
          <text id="/data/r_Month:option8">
            <value>Setembro</value>
          </text>
          <text id="/data/r_Month:option9">
            <value>Outubro</value>
          </text>
          <text id="/data/r_Month:option10">
            <value>Novembro</value>
          </text>
          <text id="/data/r_Month:option11">
            <value>Dezembro</value>
          </text>

          <!-- Month end -->

          <text id="/data/r_Year:label">
            <value>Ano de pesquisa</value>
          </text>
          <text id="/data/r_GPS:label">
            <value>Coordenação GPS do local de reprodução suspeito</value>
          </text>
          <text id="/data/r_GPS:hint">
            <value>Funciona melhor fora dos edifícios</value>
          </text>
          <text id="/data/r_RiverName:label">
            <value>Nome do rio ou riacho</value>
          </text>
          <text id="/data/r_RiverBasin:label">
            <value>Nome da bacia do rio</value>
          </text>
          <text id="/data/r_SiteSuitable:label">
            <value>No momento da inspecção, o local pareceu adequado para a reprodução de mosca preta</value>
          </text>
          <text id="/data/r_SiteSuitable:option0">
            <value>Sim</value>
          </text>
          <text id="/data/r_SiteSuitable:option1">
            <value>Não</value>
          </text>
          <text id="/data/r_EvidenceOfLarvae:label">
            <value>No momento da inspecção, o local forneceu evidências de larvas</value>
          </text>
          <text id="/data/r_EvidenceOfLarvae:option0">
            <value>Sim</value>
          </text>
          <text id="/data/r_EvidenceOfLarvae:option1">
            <value>Não</value>
          </text>
          <text id="/data/r_LarvaeRate:label">
            <value>Se SIM às larvas, classifique a abundância como poucas, algumas ou muitas</value>
          </text>
          <!-- Larvae Rate Begin -->
          <text id="/data/r_LarvaeRate:option0">
            <value>-</value>
          </text>
          <text id="/data/r_LarvaeRate:option1">
            <value>+</value>
          </text>
          <text id="/data/r_LarvaeRate:option2">
            <value>++</value>
          </text>
          <text id="/data/r_LarvaeRate:option3">
            <value>+++</value>
          </text>
          <!-- Larvae Rate End -->
          <text id="/data/r_Notes:label">
            <value>Notas Adicionais</value>
          </text>
        </translation>
      </itext>

      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
      <bind nodeset="/data/r_StartTime" type="dateTime" jr:preload="timestamp" jr:preloadParams="start"/>
      <bind nodeset="/data/r_EndTime" type="dateTime" jr:preload="timestamp" jr:preloadParams="end"/>
      <bind nodeset="/data/r_DistrictID" type="select1" required="true()" db_add_col_1="."/>
      <bind nodeset="/data/r_SiteName" type="string" required="true()" />
      <bind nodeset="/data/r_Month" type="select1" required="true()"/>
      <bind nodeset="/data/r_Year" type="int" required="true()" maxLength="4" constraint="regex(., '^20[0-9]{2}$')" jr:constraintMsg="The year is not correct" default="2019" />
      <bind nodeset="/data/r_GPS" type="geopoint" required="true()"/>
      <bind nodeset="/data/r_RiverName" type="string" required="true()" />
      <bind nodeset="/data/r_RiverBasin" type="string" required="true()" />
      <bind nodeset="/data/r_SiteSuitable" type="select1" required="true()"/>
      <bind nodeset="/data/r_EvidenceOfLarvae" type="select1" required="true()"/>
      <bind nodeset="/data/r_LarvaeRate" type="select1" required="true()" relevant="(selected(/data/r_EvidenceOfLarvae, 'Yes'))"/>
      <bind nodeset="/data/r_Notes" type="string"/>
    </model>
  </h:head>
  <h:body>
    <!-- District Begin -->
    <select1 ref="/data/r_DistrictID">
      <label ref="jr:itext('/data/r_DistrictID:label')"/>
      <item>
        <label ref="jr:itext('/data/r_DistrictID:option0')"/>
        <value>Buzi; 33210</value>
      </item>
      <item>
        <label ref="jr:itext('/data/r_DistrictID:option1')"/>
        <value>Gôndola; 33152</value>
      </item>
      <item>
        <label ref="jr:itext('/data/r_DistrictID:option2')"/>
        <value>Lago; 33196</value>
      </item>
      <item>
        <label ref="jr:itext('/data/r_DistrictID:option3')"/>
        <value>Machaze; 33155</value>
      </item>
      <item>
        <label ref="jr:itext('/data/r_DistrictID:option4')"/>
        <value>Manica; 3315</value>
      </item>
      <item>
        <label ref="jr:itext('/data/r_DistrictID:option5')"/>
        <value>Maravia; 33233</value>
      </item>
      <item>
        <label ref="jr:itext('/data/r_DistrictID:option6')"/>
        <value>Sanga; 33209</value>
      </item>
      <item>
        <label ref="jr:itext('/data/r_DistrictID:option7')"/>
        <value>Spared District</value>
      </item>
    </select1>
    <!-- District label End -->
   <input ref="/data/r_SiteName">
      <label ref="jr:itext('/data/r_SiteName:label')"/>
      <hint ref="jr:itext('/data/r_SiteName:hint')"/>
    </input>

    <!-- Month begin -->
    <select1 ref="/data/r_Month">
      <label ref="jr:itext('/data/r_Month:label')"/>
      <item>
        <label ref="jr:itext('/data/r_Month:option0')"/>
        <value>January</value>
      </item>
      <item>
        <label ref="jr:itext('/data/r_Month:option1')"/>
        <value>February</value>
      </item>
      <item>
        <label ref="jr:itext('/data/r_Month:option2')"/>
        <value>March</value>
      </item>
      <item>
        <label ref="jr:itext('/data/r_Month:option3')"/>
        <value>April</value>
      </item>
      <item>
        <label ref="jr:itext('/data/r_Month:option4')"/>
        <value>May</value>
      </item>
      <item>
        <label ref="jr:itext('/data/r_Month:option5')"/>
        <value>June</value>
      </item>
      <item>
        <label ref="jr:itext('/data/r_Month:option6')"/>
        <value>July</value>
      </item>
      <item>
        <label ref="jr:itext('/data/r_Month:option7')"/>
        <value>August</value>
      </item>
      <item>
        <label ref="jr:itext('/data/r_Month:option8')"/>
        <value>September</value>
      </item>
      <item>
        <label ref="jr:itext('/data/r_Month:option9')"/>
        <value>October</value>
      </item>
      <item>
        <label ref="jr:itext('/data/r_Month:option10')"/>
        <value>November</value>
      </item>
      <item>
        <label ref="jr:itext('/data/r_Month:option11')"/>
        <value>December</value>
      </item>
    </select1>
    <!-- Month end -->

   <input ref="/data/r_Year">
      <label ref="jr:itext('/data/r_Year:label')"/>
    </input>
    <input ref="/data/r_GPS">
      <label ref="jr:itext('/data/r_GPS:label')"/>
      <hint ref="jr:itext('/data/r_GPS:hint')"/>
    </input>
    <input ref="/data/r_RiverName">
      <label ref="jr:itext('/data/r_RiverName:label')"/>
    </input>
    <input ref="/data/r_RiverBasin">
      <label ref="jr:itext('/data/r_RiverBasin:label')"/>
    </input>
    <select1 ref="/data/r_SiteSuitable">
      <label ref="jr:itext('/data/r_SiteSuitable:label')"/>
      <item>
        <label ref="jr:itext('/data/r_SiteSuitable:option0')"/>
        <value>Yes</value>
      </item>
      <item>
        <label ref="jr:itext('/data/r_SiteSuitable:option1')"/>
        <value>No</value>
      </item>
    </select1>
    <select1 ref="/data/r_EvidenceOfLarvae">
      <label ref="jr:itext('/data/r_EvidenceOfLarvae:label')"/>
      <item>
        <label ref="jr:itext('/data/r_EvidenceOfLarvae:option0')"/>
        <value>Yes</value>
      </item>
      <item>
        <label ref="jr:itext('/data/r_EvidenceOfLarvae:option1')"/>
        <value>No</value>
      </item>
    </select1>
    <select1 ref="/data/r_LarvaeRate">
      <label ref="jr:itext('/data/r_LarvaeRate:label')"/>
      <item>
        <label ref="jr:itext('/data/r_LarvaeRate:option0')"/>
        <value>-</value>
      </item>
      <item>
        <label ref="jr:itext('/data/r_LarvaeRate:option1')"/>
        <value>+</value>
      </item>
      <item>
        <label ref="jr:itext('/data/r_LarvaeRate:option2')"/>
        <value>++</value>
      </item>
      <item>
        <label ref="jr:itext('/data/r_LarvaeRate:option3')"/>
        <value>+++</value>
      </item>
    </select1>

    <input ref="/data/r_Notes">
      <label ref="jr:itext('/data/r_Notes:label')"/>
    </input>
   
  </h:body>
</h:html>
