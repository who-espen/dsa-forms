<?xml version="1.0" encoding="UTF-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms"
  xmlns:ev="http://www.w3.org/2001/xml-events"
  xmlns:h="http://www.w3.org/1999/xhtml"
  xmlns:jr="http://openrosa.org/javarosa"
  xmlns:odk="http://www.opendatakit.org/xforms"
  xmlns:orx="http://openrosa.org/xforms"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>(November 2022) - 1. vector community questionnaire V1</h:title>
    <model >

      <instance>
        <data id="mz_oncho_bsa_1_vector_community_question_202211_v1">
          <c_recorder_id/>
          <c_district/>
          <c_site/>
          <c_consent/>
          <c_gender/>
          <c_age/>
          <c_occupation/>
          <c_how_lng_lived/>
          <c_black_fly_problem/>
          <c_bites_number/>
          <c_month/>
          <c_note/>
          <c_end_survey_note/>
          <c_start/>
          <c_end/>
          <meta>
            <instanceID/>
          </meta>
        </data>
      </instance>

      <itext>
        <translation default="true()" lang="English">
          <text id="/data/c_recorder_id:jr:constraintMsg">
            <value>Must be two digit number</value>
          </text>
          <text id="/data/c_recorder_id:label">
            <value>1. Enter recorder ID</value>
          </text>
          <text id="/data/c_recorder_id:hint">
            <value>Enter 2-digit recorder code provided to each recorder (person doing the data entry)</value>
          </text>
          <text id="/data/c_district:label">
            <value>2. District</value>
          </text>
          <text id="/data/c_district:hint">
            <value>Select the appropriate district</value>
          </text>
          <text id="/data/c_site:label">
            <value>3. Community name</value>
          </text>
          <text id="/data/c_consent:label">
            <value>4. Does this person consent to take part in the study?</value>
          </text>
          <text id="/data/c_consent/Yes:label">
            <value>Yes</value>
          </text>
          <text id="/data/c_consent/No:label">
            <value>No</value>
          </text>
          <text id="/data/c_gender:label">
            <value>5. What gender is this person?</value>
          </text>
          <text id="/data/c_gender/Male:label">
            <value>Male</value>
          </text>
          <text id="/data/c_gender/Female:label">
            <value>Female</value>
          </text>
          <text id="/data/c_age:jr:constraintMsg">
            <value>Please enter a correct age</value>
          </text>
          <text id="/data/c_age:label">
            <value>6. What age is this person?</value>
          </text>
          <text id="/data/c_occupation:label">
            <value>7. What is their occupation?</value>
          </text>
          <text id="/data/c_how_lng_lived:jr:constraintMsg">
            <value>The value must be less than the age</value>
          </text>
          <text id="/data/c_how_lng_lived:label">
            <value>8. How long has this person lived in this community?</value>
          </text>
          <text id="/data/c_black_fly_problem:label">
            <value>9. Are black fly bites a problem in the community?</value>
          </text>
          <text id="/data/c_black_fly_problem/Yes:label">
            <value>Yes</value>
          </text>
          <text id="/data/c_black_fly_problem/No:label">
            <value>No</value>
          </text>
          <text id="/data/c_black_fly_problem/Do not know:label">
            <value>Do not know</value>
          </text>
          <text id="/data/c_bites_number:label">
            <value>10. Bites number</value>
          </text>
          <text id="/data/c_bites_number:hint">
            <value>If yes, how many bites do you receive each day?</value>
          </text>
          <text id="/data/c_bites_number/1 – 10:label">
            <value>1 – 10</value>
          </text>
          <text id="/data/c_bites_number/11 – 50:label">
            <value>11 – 50</value>
          </text>
          <text id="/data/c_bites_number/51 – 100:label">
            <value>51 – 100</value>
          </text>
          <text id="/data/c_bites_number/101 – 150:label">
            <value>101 – 150</value>
          </text>
          <text id="/data/c_bites_number/151 – 200:label">
            <value>151 – 200</value>
          </text>
          <text id="/data/c_bites_number/More than 201:label">
            <value>More than 201</value>
          </text>
          <text id="/data/c_month:label">
            <value>11. Black fly month</value>
          </text>
          <text id="/data/c_month:hint">
            <value>Select the month or months when the respondent reports blackfly biting is most frequent</value>
          </text>
          <text id="/data/c_month/January:label">
            <value>January</value>
          </text>
          <text id="/data/c_month/February:label">
            <value>February</value>
          </text>
          <text id="/data/c_month/March:label">
            <value>March</value>
          </text>
          <text id="/data/c_month/April:label">
            <value>April</value>
          </text>
          <text id="/data/c_month/May:label">
            <value>May</value>
          </text>
          <text id="/data/c_month/June:label">
            <value>June</value>
          </text>
          <text id="/data/c_month/July:label">
            <value>July</value>
          </text>
          <text id="/data/c_month/August:label">
            <value>August</value>
          </text>
          <text id="/data/c_month/September:label">
            <value>September</value>
          </text>
          <text id="/data/c_month/October:label">
            <value>October</value>
          </text>
          <text id="/data/c_month/November:label">
            <value>November</value>
          </text>
          <text id="/data/c_month/December:label">
            <value>December</value>
          </text>
          <text id="/data/c_note:label">
            <value>Note</value>
          </text>
          <text id="/data/c_end_survey_note:label">
            <value>You just entered a value that will end the survey</value>
          </text>
        </translation>
      </itext>

      <bind constraint=". &gt;= 10 and . &lt;= 99" jr:constraintMsg="jr:itext('/data/c_recorder_id:jr:constraintMsg')" nodeset="/data/c_recorder_id" required="true()" type="int"/>
      <bind nodeset="/data/c_district" required="true()" type="string"/>
      <bind nodeset="/data/c_site" relevant="if(selected( /data/c_district , 'Other'), false(),true())" required="true()" type="string"/>
      <bind nodeset="/data/c_consent" required="true()" type="string"/>
      <bind nodeset="/data/c_gender" relevant=" /data/c_consent  = 'Yes'" required="true()" type="string"/>
      <bind constraint=". &gt; 0 and . &lt;=120" jr:constraintMsg="jr:itext('/data/c_age:jr:constraintMsg')" nodeset="/data/c_age" relevant=" /data/c_consent  = 'Yes'" required="true()" type="int"/>
      <bind nodeset="/data/c_occupation" relevant=" /data/c_consent  = 'Yes'" required="true()" type="string"/>
      <bind constraint=". &lt;=  /data/c_age " jr:constraintMsg="jr:itext('/data/c_how_lng_lived:jr:constraintMsg')" nodeset="/data/c_how_lng_lived" relevant=" /data/c_consent  = 'Yes'" required="true()" type="int"/>
      <bind nodeset="/data/c_black_fly_problem" relevant=" /data/c_consent  = 'Yes'" required="true()" type="string"/>
      <bind nodeset="/data/c_bites_number" relevant=" /data/c_consent  = 'Yes' and  /data/c_black_fly_problem  = 'Yes'" required="true()" type="string"/>
      <bind nodeset="/data/c_month" relevant=" /data/c_consent  = 'Yes' and  /data/c_black_fly_problem  = 'Yes'" required="true()" type="string"/>
      <bind nodeset="/data/c_note" type="string"/>
      <bind nodeset="/data/c_end_survey_note" readonly="true()" relevant=" /data/c_consent  = 'No'" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/c_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/c_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/data/c_recorder_id">
      <label ref="jr:itext('/data/c_recorder_id:label')"/>
      <hint ref="jr:itext('/data/c_recorder_id:hint')"/>
    </input>
    <input ref="/data/c_district">
      <label ref="jr:itext('/data/c_district:label')"/>
      <hint ref="jr:itext('/data/c_district:hint')"/>
    </input>
    <input ref="/data/c_site">
      <label ref="jr:itext('/data/c_site:label')"/>
    </input>
    <select1 ref="/data/c_consent">
      <label ref="jr:itext('/data/c_consent:label')"/>
      <item>
        <label ref="jr:itext('/data/c_consent/Yes:label')"/>
        <value>Yes</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_consent/No:label')"/>
        <value>No</value>
      </item>
    </select1>
    <select1 ref="/data/c_gender">
      <label ref="jr:itext('/data/c_gender:label')"/>
      <item>
        <label ref="jr:itext('/data/c_gender/Male:label')"/>
        <value>Male</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_gender/Female:label')"/>
        <value>Female</value>
      </item>
    </select1>
    <input ref="/data/c_age">
      <label ref="jr:itext('/data/c_age:label')"/>
    </input>
    <input ref="/data/c_occupation">
      <label ref="jr:itext('/data/c_occupation:label')"/>
    </input>
    <input ref="/data/c_how_lng_lived">
      <label ref="jr:itext('/data/c_how_lng_lived:label')"/>
    </input>
    <select1 ref="/data/c_black_fly_problem">
      <label ref="jr:itext('/data/c_black_fly_problem:label')"/>
      <item>
        <label ref="jr:itext('/data/c_black_fly_problem/Yes:label')"/>
        <value>Yes</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_black_fly_problem/No:label')"/>
        <value>No</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_black_fly_problem/Do not know:label')"/>
        <value>Do not know</value>
      </item>
    </select1>
    <select1 ref="/data/c_bites_number">
      <label ref="jr:itext('/data/c_bites_number:label')"/>
      <hint ref="jr:itext('/data/c_bites_number:hint')"/>
      <item>
        <label ref="jr:itext('/data/c_bites_number/1 – 10:label')"/>
        <value>1 – 10</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_bites_number/11 – 50:label')"/>
        <value>11 – 50</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_bites_number/51 – 100:label')"/>
        <value>51 – 100</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_bites_number/101 – 150:label')"/>
        <value>101 – 150</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_bites_number/151 – 200:label')"/>
        <value>151 – 200</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_bites_number/More than 201:label')"/>
        <value>More than 201</value>
      </item>
    </select1>
    <select ref="/data/c_month">
      <label ref="jr:itext('/data/c_month:label')"/>
      <hint ref="jr:itext('/data/c_month:hint')"/>
      <item>
        <label ref="jr:itext('/data/c_month/January:label')"/>
        <value>January</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_month/February:label')"/>
        <value>February</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_month/March:label')"/>
        <value>March</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_month/April:label')"/>
        <value>April</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_month/May:label')"/>
        <value>May</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_month/June:label')"/>
        <value>June</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_month/July:label')"/>
        <value>July</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_month/August:label')"/>
        <value>August</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_month/September:label')"/>
        <value>September</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_month/October:label')"/>
        <value>October</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_month/November:label')"/>
        <value>November</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_month/December:label')"/>
        <value>December</value>
      </item>
    </select>
    <input ref="/data/c_note">
      <label ref="jr:itext('/data/c_note:label')"/>
    </input>
    <input ref="/data/c_end_survey_note">
      <label ref="jr:itext('/data/c_end_survey_note:label')"/>
    </input>
  </h:body>
</h:html>
