  <h:body>
    <input ref="/data/d_recorder_id">
      <label ref="jr:itext('/data/d_recorder_id:label')"/>
      <hint ref="jr:itext('/data/d_recorder_id:hint')"/>
    </input>
    <select1 ref="/data/d_region">
      <label ref="jr:itext('/data/d_region:label')"/>
      <item>
        <label ref="jr:itext('/data/d_region/BAUCHI:label')"/>
        <value>BAUCHI</value>
      </item>
    </select1>
    <select1 ref="/data/d_District">
      <label ref="jr:itext('/data/d_District:label')"/>
      <itemset nodeset="instance('district_list')/root/item[ /data/d_region  = region_list]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <select1 ref="/data/d_Cluster_Name">
      <label ref="jr:itext('/data/d_Cluster_Name:label')"/>
      <itemset nodeset="instance('community')/root/item[ /data/d_District  = district_list]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <select1 ref="/data/d_cluster_id">
      <label ref="jr:itext('/data/d_cluster_id:label')"/>
      <itemset nodeset="instance('community_id')/root/item[ /data/d_Cluster_Name  = community]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <input ref="/data/d_serial_number1">
      <label ref="jr:itext('/data/d_serial_number1:label')"/>
    </input>
    <input ref="/data/d_serial_number2">
      <label ref="jr:itext('/data/d_serial_number2:label')"/>
    </input>
    <input ref="/data/d_code_id">
      <label ref="jr:itext('/data/d_code_id:label')"/>
    </input>
    <input ref="/data/d_elisa_value_od">
      <label ref="jr:itext('/data/d_elisa_value_od:label')"/>
    </input>
    <select1 ref="/data/d_elisa_result">
      <label ref="jr:itext('/data/d_elisa_result:label')"/>
      <item>
        <label ref="jr:itext('/data/d_elisa_result/Positive:label')"/>
        <value>Positive</value>
      </item>
      <item>
        <label ref="jr:itext('/data/d_elisa_result/Negative:label')"/>
        <value>Negative</value>
      </item>
      <item>
        <label ref="jr:itext('/data/d_elisa_result/Invalid:label')"/>
        <value>Invalid</value>
      </item>
    </select1>
    <input ref="/data/d_Notes">
      <label ref="jr:itext('/data/d_Notes:label')"/>
    </input>
  </h:body>
