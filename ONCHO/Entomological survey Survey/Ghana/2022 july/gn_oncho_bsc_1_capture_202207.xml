  <h:body>
    <select1 ref="/data/s_transmission_zone">
      <label ref="jr:itext('/data/s_transmission_zone:label')"/>
      <item>
        <label ref="jr:itext('/data/s_transmission_zone/Dayi_Asukawkaw:label')"/>
        <value>Dayi_Asukawkaw</value>
      </item>
      <item>
        <label ref="jr:itext('/data/s_transmission_zone/Pra-Offin:label')"/>
        <value>Pra-Offin</value>
      </item>
    </select1>
    <select1 ref="/data/s_region">
      <label ref="jr:itext('/data/s_region:label')"/>
      <itemset nodeset="instance('region')/root/item[transmission_zone =  /data/s_transmission_zone ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <select1 ref="/data/s_district">
      <label ref="jr:itext('/data/s_district:label')"/>
      <itemset nodeset="instance('district')/root/item[region =  /data/s_region ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <select1 ref="/data/s_community">
      <label ref="jr:itext('/data/s_community:label')"/>
      <itemset nodeset="instance('community')/root/item[district =  /data/s_district ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <input ref="/data/s_site">
      <label ref="jr:itext('/data/s_site:label')"/>
    </input>
    <input ref="/data/s_site_id">
      <label ref="jr:itext('/data/s_site_id:label')"/>
    </input>
    <input ref="/data/s_gps">
      <label ref="jr:itext('/data/s_gps:label')"/>
      <hint ref="jr:itext('/data/s_gps:hint')"/>
    </input>
    <input ref="/data/s_river">
      <label ref="jr:itext('/data/s_river:label')"/>
    </input>
    <input ref="/data/s_collector_a">
      <label ref="jr:itext('/data/s_collector_a:label')"/>
    </input>
    <input ref="/data/s_collector_b">
      <label ref="jr:itext('/data/s_collector_b:label')"/>
    </input>
    <input ref="/data/s_date">
      <label ref="jr:itext('/data/s_date:label')"/>
    </input>
    <group ref="/data/s_">
      <label ref="jr:itext('/data/s_:label')"/>
      <repeat nodeset="/data/s_">
        <select1 ref="/data/s_/add_fly">
          <label ref="jr:itext('/data/s_/add_fly:label')"/>
