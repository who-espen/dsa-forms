          </item>
        </select1>
        <input ref="/data/s_/nb_collected">
          <label ref="jr:itext('/data/s_/nb_collected:label')"/>
        </input>
        <select1 ref="/data/s_/weather">
          <label ref="jr:itext('/data/s_/weather:label')"/>
