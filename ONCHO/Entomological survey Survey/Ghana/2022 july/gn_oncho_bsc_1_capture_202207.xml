          </item>
          <item>
            <label ref="jr:itext('/data/s_/weather/Cloudy:label')"/>
            <value>Cloudy</value>
          </item>
          <item>
            <label ref="jr:itext('/data/s_/weather/Windy:label')"/>
            <value>Windy</value>
          </item>
        </select1>
        <input ref="/data/s_/remark">
          <label ref="jr:itext('/data/s_/remark:label')"/>
        </input>
      </repeat>
    </group>
  </h:body>
