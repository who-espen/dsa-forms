<?xml version="1.0" encoding="UTF-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms"
  xmlns:ev="http://www.w3.org/2001/xml-events"
  xmlns:h="http://www.w3.org/1999/xhtml"
  xmlns:jr="http://openrosa.org/javarosa"
  xmlns:odk="http://www.opendatakit.org/xforms"
  xmlns:orx="http://openrosa.org/xforms"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>(Dec 2021) IAS - 2. Participant Form</h:title>
    <model >

      <instance>
        <data id="lr_oncho_ias_2_participant_202112">
          <p_recorder_id/>
          <p_district/>
          <p_district_other/>
          <p_cluster_name/>
          <p_cluster_id/>
          <p_accent/>
          <p_consent/>
          <p_sex/>
          <p_age_yrs/>
          <p_how_long/>
          <p_ever_taken_ivm/>
          <p_times_took_ivm/>
          <p_ivm>
            <last_time_month/>
            <last_time_year/>
          </p_ivm>
          <p_ID_method/>
          <p_Barcode_ID/>
          <p_Generate_ID/>
          <p_end_survey_note/>
          <p_AdditionalNotes/>
          <p_start/>
          <p_end/>
          <meta>
            <instanceID/>
            <randomnum/>
          </meta>
        </data>
      </instance>

      <itext>
        <translation lang="English">
          <text id="/data/p_recorder_id:jr:constraintMsg">
            <value>Must be between 10 and 99 included</value>
          </text>
          <text id="/data/p_recorder_id:label">
            <value>Recorder ID</value>
          </text>
          <text id="/data/p_recorder_id:hint">
            <value>Code à 2 chiffres qui vous est attribué</value>
          </text>
          <text id="/data/p_district:label">
            <value>Select district</value>
          </text>
          <text id="/data/p_district_other:label">
            <value>Select the specific district</value>
          </text>
          <text id="/data/p_cluster_name:label">
            <value>Select the community</value>
          </text>
          <text id="/data/p_cluster_id:label">
            <value>Select the community ID</value>
          </text>
          <text id="/data/p_accent:label">
            <value>Accent</value>
          </text>
          <text id="/data/p_consent:label">
            <value>Consent</value>
          </text>
          <text id="/data/p_sex:label">
            <value>Gender</value>
          </text>
          <text id="/data/p_age_yrs:label">
            <value>Enter the age in years</value>
          </text>
          <text id="/data/p_how_long:jr:constraintMsg">
            <value>This value must be less than or equal to the participant age</value>
          </text>
          <text id="/data/p_how_long:label">
            <value>How long have you been living in this district or community?</value>
          </text>
          <text id="/data/p_ever_taken_ivm:label">
            <value>Have you ever taken Mectizan?</value>
          </text>
          <text id="/data/p_times_took_ivm:label">
            <value>How many times have you taken Mectizan?</value>
          </text>
          <text id="/data/p_ivm:label">
            <value>When was the last time you took Mectizan?</value>
          </text>
          <text id="/data/p_ivm/last_time_month:label">
            <value>specify the month</value>
          </text>
          <text id="/data/p_ivm/last_time_year:jr:constraintMsg">
            <value>Please enter a valid year</value>
          </text>
          <text id="/data/p_ivm/last_time_year:label">
            <value>specify the year</value>
          </text>
          <text id="/data/p_ID_method:label">
            <value>Participant ID format</value>
          </text>
          <text id="/data/p_Barcode_ID:label">
            <value>Please scan the QR code / barcode</value>
          </text>
          <text id="/data/p_Generate_ID:label">
            <value>Please record the following unique ID code for the respondent on a separate list and on each diagnostic test(s) adminstered</value>
          </text>
          <text id="/data/p_end_survey_note:label">
            <value>You have just entered a value that will end the survey</value>
          </text>
          <text id="/data/p_AdditionalNotes:label">
            <value>Notes</value>
          </text>
        </translation>
      </itext>
      <bind constraint=". &gt; 9 and .&lt; 100" jr:constraintMsg="jr:itext('/data/p_recorder_id:jr:constraintMsg')" nodeset="/data/p_recorder_id" required="true()" type="int"/>
      <bind nodeset="/data/p_district" required="true()" type="string" db_get="col_1"/>
      <bind nodeset="/data/p_district_other" relevant=" /data/p_district  = 'Other'" required="true()" type="string" db_get="col_2" db_filter_by_col_1="/data/p_district"/>
      <bind nodeset="/data/p_cluster_name" required="true()" type="string" db_get="col_3" db_filter_by_col_1="/data/p_district"/>
      <bind nodeset="/data/p_cluster_id" required="true()" type="string" db_get="col_4" db_filter_by_col_3="/data/p_cluster_name"/>
      <bind nodeset="/data/p_accent" required="true()" type="string"/>
      <bind nodeset="/data/p_consent" relevant=" /data/p_accent  = 'Given'" required="true()" type="string"/>
      <bind nodeset="/data/p_sex" relevant=" /data/p_accent  = 'Given' and  /data/p_consent  = 'Given'" required="true()" type="string"/>
      <bind nodeset="/data/p_age_yrs" relevant=" /data/p_accent  = 'Given' and  /data/p_consent  = 'Given'" required="true()" type="int"/>
      <bind constraint=". &lt;=  /data/p_age_yrs " jr:constraintMsg="jr:itext('/data/p_how_long:jr:constraintMsg')" nodeset="/data/p_how_long" relevant=" /data/p_accent  = 'Given' and  /data/p_consent  = 'Given'" required="true()" type="int"/>
      <bind nodeset="/data/p_ever_taken_ivm" relevant=" /data/p_accent  = 'Given' and  /data/p_consent  = 'Given'" required="true()" type="string"/>
      <bind nodeset="/data/p_times_took_ivm" relevant=" /data/p_accent  = 'Given' and  /data/p_consent  = 'Given' and  /data/p_ever_taken_ivm  = 'Yes'" required="true()" type="int"/>
      <bind nodeset="/data/p_ivm" relevant=" /data/p_accent  = 'Given' and  /data/p_consent  = 'Given' and  /data/p_ever_taken_ivm  = 'Yes'"/>
      <bind nodeset="/data/p_ivm/last_time_month" required="true()" type="string"/>
      <bind constraint=". &gt;= 1970 and . &lt; 2022" jr:constraintMsg="jr:itext('/data/p_ivm/last_time_year:jr:constraintMsg')" nodeset="/data/p_ivm/last_time_year" required="true()" type="int"/>
      <bind nodeset="/data/p_ID_method" relevant=" /data/p_accent  = 'Given' and  /data/p_consent  = 'Given'" required="true()" type="string"/>
      <bind nodeset="/data/p_Barcode_ID" relevant=" /data/p_ID_method  = 'Scanner'" required="true()" type="barcode"/>
      <bind nodeset="/data/p_Generate_ID" calculate="concat( /data/p_recorder_id ,'-', /data/p_cluster_id ,'-', /data/meta/randomnum)" readonly="true()" relevant=" /data/p_ID_method  = 'Auto generate'" required="true()" type="string"/>
      <bind nodeset="/data/p_end_survey_note" readonly="true()" relevant=" /data/p_accent  = 'Refused' or  /data/p_consent  = 'Refused'" type="string"/>
      <bind nodeset="/data/p_AdditionalNotes" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/p_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/p_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
      <bind nodeset="/data/meta/randomnum" type="string" calculate="substr(random(),3,9)"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/data/p_recorder_id">
      <label ref="jr:itext('/data/p_recorder_id:label')"/>
      <hint ref="jr:itext('/data/p_recorder_id:hint')"/>
    </input>
    <input ref="/data/p_district">
      <label ref="jr:itext('/data/p_district:label')"/>
    </input>
    <input ref="/data/p_district_other">
      <label ref="jr:itext('/data/p_district_other:label')"/>
    </input>
    <input ref="/data/p_cluster_name">
      <label ref="jr:itext('/data/p_cluster_name:label')"/>
    </input>
    <input ref="/data/p_cluster_id">
      <label ref="jr:itext('/data/p_cluster_id:label')"/>
    </input>
    <select1 ref="/data/p_accent">
      <label ref="jr:itext('/data/p_accent:label')"/>
      <item>
        <label>Given</label>
        <value>Given</value>
      </item>
      <item>
        <label>Refused</label>
        <value>Refused</value>
      </item>
    </select1>
    <select1 ref="/data/p_consent">
      <label ref="jr:itext('/data/p_consent:label')"/>
      <item>
        <label>Given</label>
        <value>Given</value>
      </item>
      <item>
        <label>Refused</label>
        <value>Refused</value>
      </item>
    </select1>
    <select1 ref="/data/p_sex">
      <label ref="jr:itext('/data/p_sex:label')"/>
      <item>
        <label>Male</label>
        <value>Male</value>
      </item>
      <item>
        <label>Female</label>
        <value>Female</value>
      </item>
    </select1>
    <input ref="/data/p_age_yrs">
      <label ref="jr:itext('/data/p_age_yrs:label')"/>
    </input>
    <input ref="/data/p_how_long">
      <label ref="jr:itext('/data/p_how_long:label')"/>
    </input>
    <select1 ref="/data/p_ever_taken_ivm">
      <label ref="jr:itext('/data/p_ever_taken_ivm:label')"/>
      <item>
        <label>Yes</label>
        <value>Yes</value>
      </item>
      <item>
        <label>No</label>
        <value>No</value>
      </item>
    </select1>
    <input ref="/data/p_times_took_ivm">
      <label ref="jr:itext('/data/p_times_took_ivm:label')"/>
    </input>
    <group appearance="field-list" ref="/data/p_ivm">
      <label ref="jr:itext('/data/p_ivm:label')"/>
      <select1 appearance="minimal" ref="/data/p_ivm/last_time_month">
        <label ref="jr:itext('/data/p_ivm/last_time_month:label')"/>
        <item>
          <label>January</label>
          <value>January</value>
        </item>
        <item>
          <label>February</label>
          <value>February</value>
        </item>
        <item>
          <label>March</label>
          <value>March</value>
        </item>
        <item>
          <label>April</label>
          <value>April</value>
        </item>
        <item>
          <label>May</label>
          <value>May</value>
        </item>
        <item>
          <label>June</label>
          <value>June</value>
        </item>
        <item>
          <label>July</label>
          <value>July</value>
        </item>
        <item>
          <label>August</label>
          <value>August</value>
        </item>
        <item>
          <label>September</label>
          <value>September</value>
        </item>
        <item>
          <label>October</label>
          <value>October</value>
        </item>
        <item>
          <label>November</label>
          <value>November</value>
        </item>
        <item>
          <label>December</label>
          <value>December</value>
        </item>
      </select1>
      <input ref="/data/p_ivm/last_time_year">
        <label ref="jr:itext('/data/p_ivm/last_time_year:label')"/>
      </input>
    </group>
    <select1 ref="/data/p_ID_method">
      <label ref="jr:itext('/data/p_ID_method:label')"/>
      <item>
        <label>Scanner</label>
        <value>Scanner</value>
      </item>
      <item>
        <label>Auto generate</label>
        <value>Auto generate</value>
      </item>
    </select1>
    <input ref="/data/p_Barcode_ID">
      <label ref="jr:itext('/data/p_Barcode_ID:label')"/>
    </input>
    <input ref="/data/p_Generate_ID">
      <label ref="jr:itext('/data/p_Generate_ID:label')"/>
    </input>
    <input ref="/data/p_end_survey_note">
      <label ref="jr:itext('/data/p_end_survey_note:label')"/>
    </input>
    <input ref="/data/p_AdditionalNotes">
      <label ref="jr:itext('/data/p_AdditionalNotes:label')"/>
    </input>
  </h:body>
</h:html>
