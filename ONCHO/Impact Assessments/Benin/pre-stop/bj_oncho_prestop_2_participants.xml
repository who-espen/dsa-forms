<?xml version="1.0" encoding="UTF-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms"
  xmlns:ev="http://www.w3.org/2001/xml-events"
  xmlns:h="http://www.w3.org/1999/xhtml"
  xmlns:jr="http://openrosa.org/javarosa"
  xmlns:odk="http://www.opendatakit.org/xforms"
  xmlns:orx="http://openrosa.org/xforms"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Benin ONCHO Pre stop - 2. Formulaire Participants</h:title>
    <model >

      <instance>
        <data id="bj_oncho_prestop_2_participants" version="20201130">
          <p_RecorderID/>
          <p_region/>
          <p_District/>
          <p_Cluster_name/>
          <p_Cluster_id/>
          <p_Consent/>
          <p_Sex/>
          <p_AgeYrs/>
          <p_born_to_village/>
          <p_HowLongLived/>
          <p_Travel/>
          <p_IVM/>
          <p_IVMWhere/>
          <p_BarcodeID/>
          <p_ending_survey_note/>
          <p_AdditionalNotes/>
          <p_StartTime/>
          <p_EndTime/>
          <meta>
            <instanceID/>
          </meta>
        </data>
      </instance>

      <itext>
        <translation default="true()" lang="French">
          <text id="/data/p_RecorderID:jr:constraintMsg">
            <value>Le code doit être un nombre à deux chiffres entre 9 et 99</value>
          </text>
          <text id="/data/p_RecorderID:label">
            <value>Entrer l'identifiant de l'enregistreur</value>
          </text>
          <text id="/data/p_RecorderID:hint">
            <value>Code à deux chiffres qui vous a été assigné</value>
          </text>
          <text id="/data/p_region:label">
            <value>Sélectionner un département</value>
          </text>
          <text id="/data/p_District:label">
            <value>Sélectionner une commune</value>
          </text>
          <text id="/data/p_Cluster_name:label">
            <value>Sectionner un grappe</value>
          </text>
          <text id="/data/p_Cluster_id:label">
            <value>Sectionner le code du grappe</value>
          </text>
          <text id="/data/p_Consent:label">
            <value>Le répondant a-t-il donnée son consentement?</value>
          </text>
          <text id="/data/p_Consent:hint">
            <value>Non entrainera la fin de l'enquête</value>
          </text>
          <text id="/data/p_Consent/Yes:label">
            <value>Oui</value>
          </text>
          <text id="/data/p_Consent/No:label">
            <value>Non</value>
          </text>
          <text id="/data/p_Sex:label">
            <value>Sexe du répondant</value>
          </text>
          <text id="/data/p_Sex/Male:label">
            <value>Masculin</value>
          </text>
          <text id="/data/p_Sex/Female:label">
            <value>Féminin</value>
          </text>
          <text id="/data/p_AgeYrs:jr:constraintMsg">
            <value>L'âge doit être entre 5 et 9 ans</value>
          </text>
          <text id="/data/p_AgeYrs:label">
            <value>Entrer l'âge en année</value>
          </text>
          <text id="/data/p_AgeYrs:hint">
            <value>L'âge doit être entre 5 et 9 ans</value>
          </text>
          <text id="/data/p_born_to_village:label">
            <value>Née au village ?</value>
          </text>
          <text id="/data/p_born_to_village/Yes:label">
            <value>Oui</value>
          </text>
          <text id="/data/p_born_to_village/No:label">
            <value>Non</value>
          </text>
          <text id="/data/p_HowLongLived:jr:constraintMsg">
            <value>La valeur ne doit pas être supérieure à l'âge</value>
          </text>
          <text id="/data/p_HowLongLived:label">
            <value>Combien d'année est-ce que le répondant a-t-il vécu en cette endroit</value>
          </text>
          <text id="/data/p_Travel:label">
            <value>Voyagez-vous fréquemment sur des longues distances (15 KM) depuis ce village?</value>
          </text>
          <text id="/data/p_Travel/Yes:label">
            <value>Oui</value>
          </text>
          <text id="/data/p_Travel/No:label">
            <value>Non</value>
          </text>
          <text id="/data/p_IVM:label">
            <value>Avez-vous pris l'Ivermectin au courant des 12 derniers mois</value>
          </text>
          <text id="/data/p_IVM/Yes:label">
            <value>Oui</value>
          </text>
          <text id="/data/p_IVM/No:label">
            <value>Non</value>
          </text>
          <text id="/data/p_IVM/Don't know:label">
            <value>Ne sais pas</value>
          </text>
          <text id="/data/p_IVMWhere:label">
            <value>Si oui, où est-ce que vous avez reçu l'Ivermectin</value>
          </text>
          <text id="/data/p_IVMWhere/In.this.village,.as.part.of.a.mass.treatment.campaign:label">
            <value>Dans ce village, dans le cadre d'une campagne de traitement de masse</value>
          </text>
          <text id="/data/p_IVMWhere/In.this.village,.local.clinic.or.pharmacy:label">
            <value>Dans ce village, dans une clinique locale ou une pharmacie</value>
          </text>
          <text id="/data/p_IVMWhere/In.another.village,.as.part.of.mass.treatment.campaign:label">
            <value>Dans un autre village, dans le cadre d'une campagne de traitement de masse</value>
          </text>
          <text id="/data/p_IVMWhere/In.another.village,.local.clinic.or.pharmacy:label">
            <value>Dans un autre village, une clinique locale ou une pharmacie</value>
          </text>
          <text id="/data/p_BarcodeID:label">
            <value>Scanner le barcode</value>
          </text>
          <text id="/data/p_ending_survey_note:label">
            <value>Vous venez d'entrer une valeur qui entrainera la fin de l'enquête</value>
          </text>
          <text id="/data/p_AdditionalNotes:label">
            <value>Notes Additionnelles</value>
          </text>
        </translation>
      </itext>
      <bind constraint="regex(.,'^[0-9]{2}$') and . &gt; 9 and . &lt; 1000" jr:constraintMsg="jr:itext('/data/p_RecorderID:jr:constraintMsg')" nodeset="/data/p_RecorderID" required="true()" type="int"/>
      <bind nodeset="/data/p_region" type="string" db_get="col_1"/>
      <bind nodeset="/data/p_District" required="true()" type="string" db_get="col_2" db_filter_by_col_1="/data/p_region"/>
      <bind nodeset="/data/p_Cluster_name" required="true()" type="string" db_get="col_3" db_filter_by_col_2="/data/p_District"/>
      <bind nodeset="/data/p_Cluster_id" required="true()" type="string" db_get="col_4" db_filter_by_col_3="/data/p_Cluster_name"/>
      <bind nodeset="/data/p_Consent" required="true()" type="string"/>
      <bind nodeset="/data/p_Sex" relevant=" /data/p_Consent  = 'Yes'" required="true()" type="string"/>
      <bind constraint=". &gt;=5 and .&lt;=9" jr:constraintMsg="jr:itext('/data/p_AgeYrs:jr:constraintMsg')" nodeset="/data/p_AgeYrs" relevant=" /data/p_Consent  = 'Yes'" required="true()" type="int"/>
      <bind nodeset="/data/p_born_to_village" relevant=" /data/p_Consent  = 'Yes'" type="string"/>
      <bind constraint=". &lt;=  /data/p_AgeYrs " jr:constraintMsg="jr:itext('/data/p_HowLongLived:jr:constraintMsg')" nodeset="/data/p_HowLongLived" relevant=" /data/p_Consent  = 'Yes'" required="true()" type="int"/>
      <bind nodeset="/data/p_Travel" relevant=" /data/p_Consent  = 'Yes'" required="true()" type="string"/>
      <bind nodeset="/data/p_IVM" relevant=" /data/p_Consent  = 'Yes'" required="true()" type="string"/>
      <bind nodeset="/data/p_IVMWhere" relevant=" /data/p_Consent  = 'Yes' and  /data/p_IVM  = 'Yes'" required="true()" type="string"/>
      <bind nodeset="/data/p_BarcodeID" relevant=" /data/p_Consent  = 'Yes'" required="true()" type="barcode"/>
      <bind nodeset="/data/p_ending_survey_note" readonly="true()" relevant=" /data/p_Consent  = 'No'" type="string"/>
      <bind nodeset="/data/p_AdditionalNotes" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/p_StartTime" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/p_EndTime" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/data/p_RecorderID">
      <label ref="jr:itext('/data/p_RecorderID:label')"/>
      <hint ref="jr:itext('/data/p_RecorderID:hint')"/>
    </input>
    <input ref="/data/p_region">
      <label ref="jr:itext('/data/p_region:label')"/>
    </input>
    <input ref="/data/p_District">
      <label ref="jr:itext('/data/p_District:label')"/>
    </input>
    <input ref="/data/p_Cluster_name">
      <label ref="jr:itext('/data/p_Cluster_name:label')"/>
    </input>
    <input ref="/data/p_Cluster_id">
      <label ref="jr:itext('/data/p_Cluster_id:label')"/>
    </input>
    <select1 ref="/data/p_Consent">
      <label ref="jr:itext('/data/p_Consent:label')"/>
      <hint ref="jr:itext('/data/p_Consent:hint')"/>
      <item>
        <label ref="jr:itext('/data/p_Consent/Yes:label')"/>
        <value>Yes</value>
      </item>
      <item>
        <label ref="jr:itext('/data/p_Consent/No:label')"/>
        <value>No</value>
      </item>
    </select1>
    <select1 ref="/data/p_Sex">
      <label ref="jr:itext('/data/p_Sex:label')"/>
      <item>
        <label ref="jr:itext('/data/p_Sex/Male:label')"/>
        <value>Male</value>
      </item>
      <item>
        <label ref="jr:itext('/data/p_Sex/Female:label')"/>
        <value>Female</value>
      </item>
    </select1>
    <input ref="/data/p_AgeYrs">
      <label ref="jr:itext('/data/p_AgeYrs:label')"/>
      <hint ref="jr:itext('/data/p_AgeYrs:hint')"/>
    </input>
    <select1 ref="/data/p_born_to_village">
      <label ref="jr:itext('/data/p_born_to_village:label')"/>
      <item>
        <label ref="jr:itext('/data/p_born_to_village/Yes:label')"/>
        <value>Yes</value>
      </item>
      <item>
        <label ref="jr:itext('/data/p_born_to_village/No:label')"/>
        <value>No</value>
      </item>
    </select1>
    <input ref="/data/p_HowLongLived">
      <label ref="jr:itext('/data/p_HowLongLived:label')"/>
    </input>
    <select1 ref="/data/p_Travel">
      <label ref="jr:itext('/data/p_Travel:label')"/>
      <item>
        <label ref="jr:itext('/data/p_Travel/Yes:label')"/>
        <value>Yes</value>
      </item>
      <item>
        <label ref="jr:itext('/data/p_Travel/No:label')"/>
        <value>No</value>
      </item>
    </select1>
    <select1 ref="/data/p_IVM">
      <label ref="jr:itext('/data/p_IVM:label')"/>
      <item>
        <label ref="jr:itext('/data/p_IVM/Yes:label')"/>
        <value>Yes</value>
      </item>
      <item>
        <label ref="jr:itext('/data/p_IVM/No:label')"/>
        <value>No</value>
      </item>
      <item>
        <label ref="jr:itext('/data/p_IVM/Don't know:label')"/>
        <value>Don't know</value>
      </item>
    </select1>
    <select ref="/data/p_IVMWhere">
      <label ref="jr:itext('/data/p_IVMWhere:label')"/>
      <item>
        <label ref="jr:itext('/data/p_IVMWhere/In.this.village,.as.part.of.a.mass.treatment.campaign:label')"/>
        <value>In.this.village,.as.part.of.a.mass.treatment.campaign</value>
      </item>
      <item>
        <label ref="jr:itext('/data/p_IVMWhere/In.this.village,.local.clinic.or.pharmacy:label')"/>
        <value>In.this.village,.local.clinic.or.pharmacy</value>
      </item>
      <item>
        <label ref="jr:itext('/data/p_IVMWhere/In.another.village,.as.part.of.mass.treatment.campaign:label')"/>
        <value>In.another.village,.as.part.of.mass.treatment.campaign</value>
      </item>
      <item>
        <label ref="jr:itext('/data/p_IVMWhere/In.another.village,.local.clinic.or.pharmacy:label')"/>
        <value>In.another.village,.local.clinic.or.pharmacy</value>
      </item>
    </select>
    <input ref="/data/p_BarcodeID">
      <label ref="jr:itext('/data/p_BarcodeID:label')"/>
    </input>
    <input ref="/data/p_ending_survey_note">
      <label ref="jr:itext('/data/p_ending_survey_note:label')"/>
    </input>
    <input ref="/data/p_AdditionalNotes">
      <label ref="jr:itext('/data/p_AdditionalNotes:label')"/>
    </input>
  </h:body>
</h:html>
