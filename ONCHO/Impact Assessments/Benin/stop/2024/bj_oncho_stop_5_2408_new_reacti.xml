      <bind nodeset="/data/bj_o_r_2408/n_gadx_invalid_reason_other" relevant=" ../n_gadx_invalid_reason  = 'Other'" required="true()" type="string"/>
      <bind nodeset="/data/bj_o_r_2408/n_result_dbs" relevant=" ../n_type_test  = 'DBS sur SD Bioline'" required="true()" type="select1"/>
