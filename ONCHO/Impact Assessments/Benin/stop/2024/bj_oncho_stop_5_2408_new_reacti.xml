      <bind nodeset="/data/bj_o_r_2408/n_result_not_clear_reason" relevant=" ../n_result_ddtd  = 'Invalide' or  ../n_result_ddtd  = 'Manque de clarté'" required="true()" type="select1"/>
      <bind nodeset="/data/bj_o_r_2408/n_result_not_clear_reason_other" relevant=" ../n_result_not_clear_reason  = 'Other'" required="true()" type="string"/>
      <bind nodeset="/data/bj_o_r_2408/n_result_gadx" relevant=" ../n_type_test  = 'GADx'" required="true()" type="select1"/>
      <bind constraint="regex(.,'^(0?[1-9]|00|10)$')" jr:constraintMsg="jr:itext('/data/bj_o_r_2408/n_line:jr:constraintMsg')" nodeset="/data/bj_o_r_2408/n_line" relevant=" ../n_type_test  = 'GADx' and  ../n_result_gadx != 'Invalide' and  ../n_result_gadx != 'Manque de clarté'" required="true()" type="string"/>
