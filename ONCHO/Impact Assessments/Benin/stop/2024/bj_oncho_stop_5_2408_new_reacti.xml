      <bind nodeset="/data/bj_o_r_2408/n_barecode_id" relevant=" ../n_id_type  = 'Scanner'" required="true()" type="barcode"/>
      <bind nodeset="/data/bj_o_r_2408/n_manual_id" relevant=" ../n_id_type  = 'Manuel'" required="true()" type="string"/>
      <bind constraint=". =  ../n_manual_id " jr:constraintMsg="jr:itext('/data/bj_o_r_2408/n_manual_id2:jr:constraintMsg')" nodeset="/data/bj_o_r_2408/n_manual_id2" relevant=" ../n_id_type  = 'Manuel'" required="true()" type="string"/>
      <bind calculate="if( ../n_id_type  = 'Scanner', concat( ../n_barecode_id ), concat( ../n_manual_id ))" constraint="not(selected( ../C3 ,  ../n_code_id ))" jr:constraintMsg="jr:itext('/data/bj_o_r_2408/n_code_id:jr:constraintMsg')" nodeset="/data/bj_o_r_2408/n_code_id" readonly="true()" type="string"/>
