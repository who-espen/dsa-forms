      <bind calculate="position(..)" nodeset="/data/bj_o_p_2408/C2" type="string"/>
      <bind calculate="if ( ../C2  = 1,'',substring-after( /data/C1 , ../p_code_id ))" nodeset="/data/bj_o_p_2408/C3" type="string"/>
      <bind nodeset="/data/bj_o_p_2408/p_num_maison" relevant=" /data/p_consent  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/bj_o_p_2408/p_id_type" relevant=" /data/p_consent  = 'Oui'" required="true()" type="select1"/>
