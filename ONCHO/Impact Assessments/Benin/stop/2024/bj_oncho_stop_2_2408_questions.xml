      <bind nodeset="/data/bj_o_p_2408/p_manual_id" relevant=" /data/p_consent  = 'Oui' and  ../p_id_type  = 'Manuel'" required="true()" type="string"/>
      <bind calculate="if( ../p_id_type  = 'Scanner', concat( ../p_barecode_id ), concat( ../p_manual_id ))" constraint="not(selected( ../C3 ,  ../p_code_id ))" jr:constraintMsg="jr:itext('/data/bj_o_p_2408/p_code_id:jr:constraintMsg')" nodeset="/data/bj_o_p_2408/p_code_id" readonly="true()" type="string"/>
      <bind nodeset="/data/bj_o_p_2408/p_sex" relevant=" /data/p_consent  = 'Oui'" required="true()" type="select1"/>
      <bind nodeset="/data/bj_o_p_2408/p_age_yrs_tranche" relevant=" /data/p_consent  = 'Oui'" required="true()" type="select1"/>
      <bind nodeset="/data/bj_o_p_2408/p_age_yrs_1_4_yrs" relevant=" /data/p_consent  = 'Oui' and ( ../p_age_yrs_tranche  = '1 an à 4 ans')" required="true()" type="select1"/>
      <bind nodeset="/data/bj_o_p_2408/p_age_yrs_5_9_yrs" relevant=" /data/p_consent  = 'Oui' and ( ../p_age_yrs_tranche  = '5 ans à 9 ans' )" required="true()" type="select1"/>
      <bind nodeset="/data/bj_o_p_2408/p_age_yrs_10_15_yrs" relevant=" /data/p_consent  = 'Oui' and ( ../p_age_yrs_tranche  = '10 ans à 15 ans' )" required="true()" type="select1"/>
      <bind constraint=". &gt; 15" nodeset="/data/bj_o_p_2408/p_age_yrs_16_yrs" relevant=" /data/p_consent  = 'Oui' and ( ../p_age_yrs_tranche  = 'supérieur à 20 ans')" required="true()" type="int"/>
      <bind nodeset="/data/bj_o_p_2408/p_age_yrs_mois" relevant=" /data/p_consent  = 'Oui' and  ../p_age_yrs_tranche  = '6 mois à 11 mois'" required="true()" type="select1"/>
      <bind nodeset="/data/bj_o_p_2408/p_nb_child_5_9_yrs" relevant=" /data/p_consent  = 'Oui' and ( ../p_age_yrs_tranche  ='5 ans à 9 ans')" required="true()" type="int"/>
      <bind nodeset="/data/bj_o_p_2408/p_time_in_village" relevant=" /data/p_consent  = 'Oui'" required="true()" type="int"/>
      <bind nodeset="/data/bj_o_p_2408/p_ivm_distributed" relevant=" /data/p_consent  = 'Oui'" required="true()" type="select1"/>
      <bind nodeset="/data/bj_o_p_2408/p_ivm_distributed_last_yrs" relevant=" /data/p_consent  = 'Oui' and  ../p_ivm_distributed != 'Non' and  ../p_ivm_distributed != 'Ne sait pas' and  ../p_ivm_distributed != 'Refuse de repondre'" required="true()" type="select1"/>
      <bind nodeset="/data/bj_o_p_2408/p_took_ivm" relevant=" /data/p_consent  = 'Oui'" required="true()" type="select1"/>
      <bind nodeset="/data/bj_o_p_2408/p_took_ivm_last_yrs" relevant=" /data/p_consent  = 'Oui' and  ../p_took_ivm != 'Non' and  ../p_took_ivm != 'Ne sait pas' and  ../p_took_ivm != 'Refuse de repondre'" required="true()" type="select1"/>
      <bind nodeset="/data/bj_o_p_2408/p_note" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/p_StartTime" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/p_EndTime" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
