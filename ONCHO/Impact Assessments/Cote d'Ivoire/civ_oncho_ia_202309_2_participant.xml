          <p_sexe/>
          <p_age/>
          <p_born_in_village/>
          <p_live_outside_village/>
          <p_bite_per_black_flies/>
          <p_took_ivm/>
          <p_nb_tablet/>
          <p_notes/>
          <p_start/>
          <p_end/>
          <meta>
            <instanceID/>
          </meta>
        </data>
      </instance>

      <itext>
        <translation default="true()" lang="French">
          <text id="/data/p_district_id:label">
            <value>4. Sélectionner le code du district</value>
          </text>
          <text id="/data/p_numero:label">
            <value>9.a. Entrer le numéro du participant</value>
          </text>
          <text id="/data/p_site:label">
            <value>6. Sélectionner le site d'enquête</value>
          </text>
          <text id="/data/p_recorder_id:jr:constraintMsg">
            <value>Le code doit être un nombre à deux chiffres entre 9 et 100</value>
          </text>
          <text id="/data/p_district:label">
            <value>3. Sélectionner un district</value>
          </text>
          <text id="/data/p_age/8:label">
            <value>8</value>
          </text>
          <text id="/data/p_site_id:label">
            <value>7. Sélectionner un code du site</value>
          </text>
          <text id="/data/p_live_outside_village:label">
            <value>15. Avez-vous déjà vécu en dehors de ce village durant ses 9 dernières années</value>
          </text>
          <text id="/data/p_age/9:label">
            <value>9</value>
          </text>
          <text id="/data/p_born_in_village:label">
            <value>14. Etes-vous né dans ce village</value>
          </text>
          <text id="/data/p_sexe:label">
            <value>12. Quel est le sexe de cette personne ?</value>
          </text>
          <text id="/data/p_notes:label">
            <value>19. Notes supplémentaires facultatives</value>
          </text>
          <text id="/data/p_consent:label">
            <value>8. Cette personne consent-elle à participer à l'étude ?</value>
          </text>
          <text id="/data/p_bite_per_black_flies:label">
            <value>16. Avez-vous déjà été piqué par les simulies</value>
          </text>
          <text id="/data/p_numero:jr:constraintMsg">
            <value>Le code doit être un nombre à trois chiffres entre 001 et 100</value>
          </text>
          <text id="/data/p_region:label">
            <value>2. Sélectionner une région</value>
          </text>
          <text id="/data/p_bite_per_black_flies/Non:label">
            <value>Non</value>
          </text>
          <text id="/data/p_recorder_id:hint">
            <value>Code à deux chiffres qui vous a été assigné</value>
          </text>
          <text id="/data/p_recorder_id:label">
            <value>1. Entrer l'identifiant de l'enregistreur</value>
          </text>
          <text id="/data/p_bite_per_black_flies/Oui:label">
            <value>Oui</value>
          </text>
          <text id="/data/p_took_ivm/Non:label">
            <value>Non</value>
          </text>
          <text id="/data/p_took_ivm:label">
            <value>17. As-tu déjà pris les comprimés d'Ivermectine</value>
          </text>
          <text id="/data/p_live_outside_village/Non:label">
            <value>Non</value>
          </text>
          <text id="/data/p_took_ivm/Oui:label">
            <value>Oui</value>
          </text>
          <text id="/data/p_sexe/Homme:label">
            <value>Homme</value>
          </text>
          <text id="/data/p_age/7:label">
            <value>7</value>
          </text>
          <text id="/data/p_nom_parrent:label">
            <value>10. Nom du parent</value>
          </text>
          <text id="/data/p_born_in_village/Non:label">
            <value>Non</value>
          </text>
          <text id="/data/p_profession_parent:label">
            <value>11. Profession du parrent</value>
          </text>
          <text id="/data/p_nb_tablet:label">
            <value>18. Combien de comprimes as-tu déjà pris</value>
          </text>
