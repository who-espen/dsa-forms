          </text>
          <text id="/data/p_live_outside_village/Oui:label">
            <value>Oui</value>
          </text>
          <text id="/data/p_age:label">
            <value>13. Quel est l'âge de cette personne ?</value>
          </text>
          <text id="/data/p_born_in_village/Oui:label">
            <value>Oui</value>
          </text>
          <text id="/data/p_age/5:label">
            <value>5</value>
          </text>
          <text id="/data/p_consent/Oui:label">
            <value>Oui</value>
          </text>
          <text id="/data/p_age/6:label">
            <value>6</value>
          </text>
        </translation>
      </itext>

      <bind constraint="regex(.,'^[0-9]{2}$')" jr:constraintMsg="jr:itext('/data/p_recorder_id:jr:constraintMsg')" nodeset="/data/p_recorder_id" required="true()" type="int"/>
      <bind nodeset="/data/p_region" required="true()" type="string" db_get="col_1"/>
      <bind nodeset="/data/p_district" required="true()" type="string" db_get="col_2" db_filter_by_col_1="/data/p_region"/>
      <bind nodeset="/data/p_district_id" required="true()" type="string" db_get="col_3" db_filter_by_col_2="/data/p_district"/>

      <bind nodeset="/data/p_site" required="true()" type="string" db_get="col_4" db_filter_by_col_3="/data/p_district_id"/>
      <bind nodeset="/data/p_site_id" required="true()" type="string" db_get="col_5" db_filter_by_col_4="/data/p_site"/>
      <bind nodeset="/data/p_consent" required="true()" type="select1"/>
      <bind constraint="regex(.,'^(0[1-9][0-9]|100|0[0-9][1-9])$')" jr:constraintMsg="jr:itext('/data/p_numero:jr:constraintMsg')" nodeset="/data/p_numero" relevant=" /data/p_consent  = 'Oui'" required="true()" type="string"/>
      <bind calculate="concat('0_',  /data/p_district_id , '_',  /data/p_site_id , '_',  /data/p_numero )" nodeset="/data/p_code_id" readonly="true()" type="string"/>
      <bind nodeset="/data/p_nom_parrent" relevant=" /data/p_consent  = 'Oui'" type="string"/>
      <bind nodeset="/data/p_profession_parent" relevant=" /data/p_consent  = 'Oui'" type="string"/>
      <bind nodeset="/data/p_sexe" relevant=" /data/p_consent  = 'Oui'" required="true()" type="select1"/>
      <bind nodeset="/data/p_age" relevant=" /data/p_consent  = 'Oui'" required="true()" type="select1"/>
      <bind nodeset="/data/p_born_in_village" relevant=" /data/p_consent  = 'Oui'" required="true()" type="select1"/>
      <bind nodeset="/data/p_live_outside_village" relevant=" /data/p_consent  = 'Oui'" required="true()" type="select1"/>
      <bind nodeset="/data/p_bite_per_black_flies" relevant=" /data/p_consent  = 'Oui'" required="true()" type="select1"/>
      <bind nodeset="/data/p_took_ivm" relevant=" /data/p_consent  = 'Oui'" required="true()" type="select1"/>
      <bind nodeset="/data/p_nb_tablet" relevant=" /data/p_consent  = 'Oui'" required="true()" type="int"/>
      <bind nodeset="/data/p_notes" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/p_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/p_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
