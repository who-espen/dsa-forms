      <bind nodeset="/data/d_recorder_id" required="true()" type="int"/>
      <bind nodeset="/data/d_region" required="true()" type="string" db_get="col_1"/>
      <bind nodeset="/data/d_district" required="true()" type="string" db_get="col_2" db_filter_by_col_1="/data/d_region"/>
      <bind nodeset="/data/d_village" required="true()" type="string" db_get="col_3" db_filter_by_col_2="/data/d_district"/>
      <bind nodeset="/data/d_village_id" required="true()" type="string" db_get="col_4" db_filter_by_col_3="/data/d_village"/>
      <bind nodeset="/data/d_ecole" required="true()" type="string" db_get="col_5" db_filter_by_col_4="/data/d_village_id"/>
      <bind nodeset="/data/d_ecole_id" required="true()" type="string" db_get="col_6" db_filter_by_col_5="/data/d_ecole"/>
      <bind nodeset="/data/d_num_ordre" required="true()" type="int"/>
      <bind calculate="concat( /data/d_ecole_id , '_',  /data/d_num_ordre )" nodeset="/data/d_num_final" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/data/generated_note_name_11" readonly="true()" type="string"/>
      <bind nodeset="/data/d_dbs" required="true()" type="select1"/>
      <bind nodeset="/data/generated_note_name_14" readonly="true()" type="string"/>
      <bind nodeset="/data/d_nodule" required="true()" type="select1"/>
      <bind nodeset="/data/d_nombre_nodules" required="true()" type="int"/>
      <bind nodeset="/data/d_localisation_nodules" required="true()" type="string"/>
      <bind nodeset="/data/d_gale" required="true()" type="select1"/>
      <bind nodeset="/data/d_prurit" required="true()" type="select1"/>
      <bind nodeset="/data/d_autre_morbidite" type="string"/>
      <bind nodeset="/data/d_notes" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/d_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/d_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
