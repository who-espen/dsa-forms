<?xml version="1.0" encoding="UTF-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms"
  xmlns:ev="http://www.w3.org/2001/xml-events"
  xmlns:h="http://www.w3.org/1999/xhtml"
  xmlns:jr="http://openrosa.org/javarosa"
  xmlns:odk="http://www.opendatakit.org/xforms"
  xmlns:orx="http://openrosa.org/xforms"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>(2023 Juillet) ONCHO pre Stop - 2. Questionnaire individuel</h:title>
    <model >

      <instance>
        <data id="bi_oncho_prestop_2_202307_questions">
          <p_RecorderID/>
          <p_region/>
          <p_District/>
          <p_site_name/>
          <p_consent/>
          <p_inf_gen/>
          <p_num_maison/>
          <p_id_type/>
          <p_barecode_id/>
          <p_manual_id/>
          <p_sex/>
          <p_age_yrs/>
          <p_time_in_village/>
          <p_note/>
          <p_StartTime/>
          <p_EndTime/>
          <meta>
            <instanceID/>
          </meta>
        </data>
      </instance>

      <itext>
        <translation default="true()" lang="French">
          <text id="/data/p_RecorderID:jr:constraintMsg">
            <value>Le code doit être un nombre à deux chiffres entre 9 et 99</value>
          </text>
          <text id="/data/p_RecorderID:label">
            <value>Entrer l'identifiant de l'enregistreur</value>
          </text>
          <text id="/data/p_RecorderID:hint">
            <value>Code à deux chiffres qui vous a été assigné</value>
          </text>
          <text id="/data/p_region:label">
            <value>Sélectionner une commune</value>
          </text>
          <text id="/data/p_District:label">
            <value>Sélectionner un arrondissement</value>
          </text>
          <text id="/data/p_site_name:label">
            <value>2) Sélectionner un village</value>
          </text>
          <text id="/data/p_consent:label">
            <value>Le participant a-t-il donné son consentement?</value>
          </text>
          <text id="/data/p_consent/Oui:label">
            <value>Oui</value>
          </text>
          <text id="/data/p_consent/Non:label">
            <value>Non</value>
          </text>
          <text id="/data/p_inf_gen:label">
            <value>INFORMATIONS GENERALES</value>
          </text>
          <text id="/data/p_num_maison:label">
            <value>4) Numéro de maison</value>
          </text>
          <text id="/data/p_id_type:label">
            <value>Type d'identification</value>
          </text>
          <text id="/data/p_id_type/Scanner:label">
            <value>Scanner</value>
          </text>
          <text id="/data/p_id_type/Manuel:label">
            <value>Manuel</value>
          </text>
          <text id="/data/p_barecode_id:label">
            <value>5) Numéro d'identification du participant</value>
          </text>
          <text id="/data/p_manual_id:label">
            <value>5) Numéro d'identification du participant</value>
          </text>
          <text id="/data/p_sex:label">
            <value>6) Quel est le sexe du participant?</value>
          </text>
          <text id="/data/p_sex/Homme:label">
            <value>Homme</value>
          </text>
          <text id="/data/p_sex/Femme:label">
            <value>Femme</value>
          </text>
          <text id="/data/p_age_yrs:label">
            <value>7) Sélectionner la tranche d'âge du participant</value>
          </text>
          <text id="/data/p_age_yrs/5:label">
            <value>5 ans</value>
          </text>
          <text id="/data/p_age_yrs/6:label">
            <value>6 ans</value>
          </text>
          <text id="/data/p_age_yrs/7:label">
            <value>7 ans</value>
          </text>
          <text id="/data/p_age_yrs/8:label">
            <value>8 ans</value>
          </text>
          <text id="/data/p_age_yrs/9:label">
            <value>9 ans</value>
          </text>
          <text id="/data/p_time_in_village:label">
            <value>9) Depuis combien d'années vivez-vous / votre enfant vit-il dans le village ?</value>
          </text>
          <text id="/data/p_note:label">
            <value>Note additionelle</value>
          </text>
        </translation>
      </itext>

      <bind constraint="regex(.,'^[0-9]{2}$') and . &gt; 9 and . &lt; 1000" jr:constraintMsg="jr:itext('/data/p_RecorderID:jr:constraintMsg')" nodeset="/data/p_RecorderID" required="true()" type="int"/>
      <bind nodeset="/data/p_region" required="true()" type="string" db_get="col_1"/>
      <bind nodeset="/data/p_District" required="true()" type="string" db_get="col_2" db_filter_by_col_1="/data/p_region"/>
      <bind nodeset="/data/p_site_name" required="true()" type="string" db_get="col_3" db_filter_by_col_2="/data/p_District"/>
      <bind nodeset="/data/p_consent" required="true()" type="string"/>
      <bind nodeset="/data/p_inf_gen" readonly="true()" relevant=" /data/p_consent  = 'Oui'" type="string"/>
      <bind nodeset="/data/p_num_maison" relevant=" /data/p_consent  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/p_id_type" relevant=" /data/p_consent  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/p_barecode_id" relevant=" /data/p_consent  = 'Oui' and  /data/p_id_type  = 'Scanner'" required="true()" type="barcode"/>
      <bind nodeset="/data/p_manual_id" relevant=" /data/p_consent  = 'Oui' and  /data/p_id_type  = 'Manuel'" required="true()" type="string"/>
      <bind nodeset="/data/p_sex" relevant=" /data/p_consent  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/p_age_yrs" relevant=" /data/p_consent  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/p_time_in_village" relevant=" /data/p_consent  = 'Oui'" required="true()" type="int"/>
      <bind nodeset="/data/p_note" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/p_StartTime" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/p_EndTime" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/data/p_RecorderID">
      <label ref="jr:itext('/data/p_RecorderID:label')"/>
      <hint ref="jr:itext('/data/p_RecorderID:hint')"/>
    </input>
    <input ref="/data/p_region">
      <label ref="jr:itext('/data/p_region:label')"/>
    </input>
    <input ref="/data/p_District">
      <label ref="jr:itext('/data/p_District:label')"/>
    </input>
    <input ref="/data/p_site_name">
      <label ref="jr:itext('/data/p_site_name:label')"/>
    </input>
    <select1 ref="/data/p_consent">
      <label ref="jr:itext('/data/p_consent:label')"/>
      <item>
        <label ref="jr:itext('/data/p_consent/Oui:label')"/>
        <value>Oui</value>
      </item>
      <item>
        <label ref="jr:itext('/data/p_consent/Non:label')"/>
        <value>Non</value>
      </item>
    </select1>
    <input ref="/data/p_inf_gen">
      <label ref="jr:itext('/data/p_inf_gen:label')"/>
    </input>
    <input ref="/data/p_num_maison">
      <label ref="jr:itext('/data/p_num_maison:label')"/>
    </input>
    <select1 ref="/data/p_id_type">
      <label ref="jr:itext('/data/p_id_type:label')"/>
      <item>
        <label ref="jr:itext('/data/p_id_type/Scanner:label')"/>
        <value>Scanner</value>
      </item>
      <item>
        <label ref="jr:itext('/data/p_id_type/Manuel:label')"/>
        <value>Manuel</value>
      </item>
    </select1>
    <input ref="/data/p_barecode_id">
      <label ref="jr:itext('/data/p_barecode_id:label')"/>
    </input>
    <input ref="/data/p_manual_id">
      <label ref="jr:itext('/data/p_manual_id:label')"/>
    </input>
    <select1 ref="/data/p_sex">
      <label ref="jr:itext('/data/p_sex:label')"/>
      <item>
        <label ref="jr:itext('/data/p_sex/Homme:label')"/>
        <value>Homme</value>
      </item>
      <item>
        <label ref="jr:itext('/data/p_sex/Femme:label')"/>
        <value>Femme</value>
      </item>
    </select1>
    <select1 ref="/data/p_age_yrs">
      <label ref="jr:itext('/data/p_age_yrs:label')"/>
      <item>
        <label ref="jr:itext('/data/p_age_yrs/5:label')"/>
        <value>5</value>
      </item>
      <item>
        <label ref="jr:itext('/data/p_age_yrs/6:label')"/>
        <value>6</value>
      </item>
      <item>
        <label ref="jr:itext('/data/p_age_yrs/7:label')"/>
        <value>7</value>
      </item>
      <item>
        <label ref="jr:itext('/data/p_age_yrs/8:label')"/>
        <value>8</value>
      </item>
      <item>
        <label ref="jr:itext('/data/p_age_yrs/9:label')"/>
        <value>9</value>
      </item>
    </select1>
    <input ref="/data/p_time_in_village">
      <label ref="jr:itext('/data/p_time_in_village:label')"/>
    </input>
    <input ref="/data/p_note">
      <label ref="jr:itext('/data/p_note:label')"/>
    </input>
  </h:body>
</h:html>
