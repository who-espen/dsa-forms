      <bind constraint=". &gt; 9 and .&lt; 100" jr:constraintMsg="jr:itext('/data/p_recorder_id:jr:constraintMsg')" nodeset="/data/p_recorder_id" required="true()" type="int"/>
      <bind nodeset="/data/p_region" required="true()" type="string" db_get="col_1"/>
      <bind nodeset="/data/p_district" required="true()" type="string" db_get="col_2" db_filter_by_col_1="/data/p_region"/>
      <bind nodeset="/data/p_cluster_name" required="true()" type="string" db_get="col_3" db_filter_by_col_2="/data/p_district"/>
      <bind nodeset="/data/p_cluster_id" required="true()" type="string" db_get="col_4" db_filter_by_col_3="/data/p_cluster_name"/>
      <bind nodeset="/data/p_date" type="date"/>
      <bind nodeset="/data/p_consent" required="true()" type="string"/>
      <bind nodeset="/data/p_id_method" relevant=" /data/p_consent  = 'Sim'" type="string"/>
      <bind constraint="regex(., '^[0-9]{3}$')" jr:constraintMsg="jr:itext('/data/p_serial_number1:jr:constraintMsg')" nodeset="/data/p_serial_number1" relevant=" /data/p_consent  = 'Sim' and  /data/p_id_method  = 'Não'" required="true()" type="string"/>
      <bind constraint=". =  /data/p_serial_number1 " jr:constraintMsg="jr:itext('/data/p_serial_number2:jr:constraintMsg')" nodeset="/data/p_serial_number2" relevant=" /data/p_consent  = 'Sim' and  /data/p_id_method  = 'Não'" required="true()" type="string"/>
      <bind calculate="concat( /data/p_recorder_id , '-',  /data/p_cluster_id , '-',  /data/p_serial_number1 )" nodeset="/data/p_code_id" readonly="true()" relevant=" /data/p_consent  = 'Sim' and  /data/p_id_method  = 'Não'" required="true()" type="string"/>
      <bind nodeset="/data/p_barcode_id" relevant=" /data/p_consent  = 'Sim' and  /data/p_id_method  = 'Sim'" required="true()" type="barcode"/>
      <bind nodeset="/data/p_sex" relevant=" /data/p_consent  = 'Sim'" required="true()" type="string"/>
      <bind constraint=". &gt;= 5 and . &lt;= 9" jr:constraintMsg="jr:itext('/data/p_age_yrs:jr:constraintMsg')" nodeset="/data/p_age_yrs" relevant=" /data/p_consent  = 'Sim'" required="true()" type="int"/>
      <bind constraint=". &lt;=  /data/p_age_yrs " jr:constraintMsg="jr:itext('/data/p_how_long_lived:jr:constraintMsg')" nodeset="/data/p_how_long_lived" relevant=" /data/p_consent  = 'Sim'" required="true()" type="int"/>
      <bind nodeset="/data/p_travel" relevant=" /data/p_consent  = 'Sim'" required="true()" type="string"/>
      <bind nodeset="/data/p_IVM_taken" relevant=" /data/p_consent  = 'Sim'" required="true()" type="string"/>
      <bind nodeset="/data/p_IVM_taken_last_12" relevant=" /data/p_consent  = 'Sim' and  /data/p_IVM_taken  = 'Sim'" required="true()" type="string"/>
      <bind nodeset="/data/p_IVM_month" relevant=" /data/p_consent  = 'Sim' and  /data/p_IVM_taken_last_12  = 'Sim'" required="true()" type="string"/>
      <bind nodeset="/data/p_ivm_place" relevant=" /data/p_consent  = 'Sim' and  /data/p_IVM_taken_last_12  = 'Sim'" required="true()" type="string"/>
      <bind nodeset="/data/p_sample_collected" relevant=" /data/p_consent  = 'Sim'" required="true()" type="string"/>
      <bind nodeset="/data/p_end_survey_note" readonly="true()" relevant=" /data/p_consent  = 'Não'" type="string"/>
      <bind nodeset="/data/p_AdditionalNotes" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/p_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/p_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
