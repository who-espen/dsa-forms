      <bind constraint=". &gt; 9 and .&lt; 100" jr:constraintMsg="jr:itext('/data/c_recorder_id:jr:constraintMsg')" nodeset="/data/c_recorder_id" required="true()" type="int"/>
      <bind nodeset="/data/c_region" required="true()" type="string"/>
      <bind nodeset="/data/c_district" required="true()" type="string"/>
      <bind nodeset="/data/c_cluster_name" required="true()" type="string"/>
      <bind constraint=". &gt; 99 and . &lt; 1000" jr:constraintMsg="jr:itext('/data/c_cluster_id:jr:constraintMsg')" nodeset="/data/c_cluster_id" required="true()" type="int"/>
      <bind nodeset="/data/c_gps_method" required="true()" type="string"/>
      <bind nodeset="/data/c_GPS" relevant=" /data/c_gps_method  = 'Automatique'" type="geopoint"/>
      <bind nodeset="/data/c_lat" relevant=" /data/c_gps_method != 'Automatique'" required="true()" type="decimal"/>
      <bind nodeset="/data/c_lng" relevant=" /data/c_gps_method != 'Automatique'" required="true()" type="decimal"/>
      <bind nodeset="/data/c_date" required="true()" type="date"/>
      <bind nodeset="/data/c_/add_participant" required="true()" type="string"/>
      <bind nodeset="/data/c_/consent" relevant=" ../add_participant  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/c_/ID_method" relevant=" ../consent  = 'Oui' and  ../add_participant  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/c_/BarcodeID" relevant=" ../consent  = 'Oui' and  ../ID_method  = 'Scanner' and  ../add_participant  = 'Oui'" required="true()" type="barcode"/>
      <bind nodeset="/data/c_/GenerateID" relevant=" ../consent  = 'Oui' and  ../ID_method  = 'Automatique' and  ../add_participant  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/c_/filiation" relevant=" ../consent  = 'Oui' and  ../add_participant  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/c_/sex" relevant=" ../consent  = 'Oui' and  ../add_participant  = 'Oui'" required="true()" type="string"/>
      <bind constraint=". &gt;= 5 and . &lt;= 9" jr:constraintMsg="jr:itext('/data/c_/age_yrs:jr:constraintMsg')" nodeset="/data/c_/age_yrs" relevant=" ../consent  = 'Oui' and  ../add_participant  = 'Oui'" required="true()" type="int"/>
      <bind nodeset="/data/c_/occupations" relevant=" ../consent  = 'Oui' and  ../add_participant  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/c_/dbs_collected" relevant=" ../consent  = 'Oui' and  ../age_yrs &gt;= 5 and  ../age_yrs &lt;= 9 and  ../add_participant  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/c_/is_natif" relevant=" ../consent  = 'Oui' and  ../add_participant  = 'Oui'" required="true()" type="string"/>
      <bind jr:constraintMsg="jr:itext('/data/c_/origine:jr:constraintMsg')" nodeset="/data/c_/origine" relevant=" ../consent  = 'Oui' and  ../is_natif  = 'Non' and  ../add_participant  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/c_/yrs_arriving" relevant=" ../consent  = 'Oui' and  ../is_natif  = 'Non' and  ../add_participant  = 'Oui'" required="true()" type="int"/>
      <bind nodeset="/data/c_/ending_survey_note" readonly="true()" relevant=" ../consent  = 'Non'" required="true()" type="string"/>
      <bind nodeset="/data/c_/AdditionalNotes" relevant=" ../add_participant  = 'Oui'" type="string"/>
      <bind nodeset="/data/c_cluster_notes" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/c_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/c_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
