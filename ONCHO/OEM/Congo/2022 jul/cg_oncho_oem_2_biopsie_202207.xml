      <bind constraint=". &gt; 99 and . &lt; 1000" jr:constraintMsg="jr:itext('/data/cluster_id:jr:constraintMsg')" nodeset="/data/cluster_id" required="true()" type="int"/>
      <bind nodeset="/data/gps_method" required="true()" type="string"/>
      <bind nodeset="/data/GPS" relevant=" /data/gps_method  = 'Automatique'" required="true()" type="geopoint"/>
      <bind nodeset="/data/lat" relevant=" /data/gps_method != 'Automatique'" required="true()" type="string"/>
      <bind nodeset="/data/lng" relevant=" /data/gps_method != 'Automatique'" required="true()" type="string"/>
      <bind nodeset="/data/p_/add_participant" required="true()" type="string"/>
      <bind nodeset="/data/p_/Imethod" relevant=" ../add_participant  = 'Oui'" required="true()" type="string"/>
      <bind constraint="regex(., '^OEM-[0-9]{8}$')" jr:constraintMsg="jr:itext('/data/p_/BarcodeID:jr:constraintMsg')" nodeset="/data/p_/BarcodeID" relevant=" ../Imethod  = 'Scanner' and  ../add_participant  = 'Oui'" required="true()" type="barcode"/>
      <bind constraint="regex(., '^OEM-[0-9]{8}$')" jr:constraintMsg="jr:itext('/data/p_/GenerateID:jr:constraintMsg')" nodeset="/data/p_/GenerateID" relevant=" ../Imethod  = 'Manuel' and  ../add_participant  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/p_/filiation" relevant=" ../add_participant  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/p_/occupation" relevant=" ../add_participant  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/p_/sex" relevant=" ../add_participant  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/p_/age_yrs" relevant=" ../add_participant  = 'Oui'" required="true()" type="int"/>
      <bind nodeset="/data/p_/is_natif" relevant=" ../add_participant  = 'Oui'" required="true()" type="string"/>
      <bind jr:constraintMsg="jr:itext('/data/p_/origine:jr:constraintMsg')" nodeset="/data/p_/origine" relevant=" ../is_natif  = 'Non' and  ../add_participant  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/p_/yrs_arriving" relevant=" ../is_natif  = 'Non' and  ../add_participant  = 'Oui'" required="true()" type="int"/>
      <bind nodeset="/data/p_/num_biopsie" relevant=" ../add_participant  = 'Oui'" required="true()" type="int"/>
      <bind nodeset="/data/p_/has_nodule" relevant=" ../add_participant  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/p_/nodule_nbr" relevant=" ../has_nodule  = 'Oui' and  ../add_participant  = 'Oui'" required="true()" type="int"/>
      <bind nodeset="/data/p_/nodule_location" relevant=" ../has_nodule  = 'Oui' and  ../add_participant  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/p_/mfs_gauche" relevant=" ../add_participant  = 'Oui'" required="true()" type="decimal"/>
      <bind nodeset="/data/p_/mfs_droite" relevant=" ../add_participant  = 'Oui'" required="true()" type="decimal"/>
      <bind nodeset="/data/p_/AdditionalNotes" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/data/recorder_id">
      <label ref="jr:itext('/data/recorder_id:label')"/>
      <hint ref="jr:itext('/data/recorder_id:hint')"/>
    </input>
    <select1 ref="/data/region">
      <label ref="jr:itext('/data/region:label')"/>
      <item>
        <label ref="jr:itext('/data/region/Bouenza:label')"/>
        <value>Bouenza</value>
      </item>
      <item>
        <label ref="jr:itext('/data/region/Lékoumou:label')"/>
        <value>Lékoumou</value>
      </item>
      <item>
        <label ref="jr:itext('/data/region/Niari:label')"/>
        <value>Niari</value>
      </item>
      <item>
        <label ref="jr:itext('/data/region/Pool:label')"/>
        <value>Pool</value>
      </item>
    </select1>
    <select1 ref="/data/district">
      <label ref="jr:itext('/data/district:label')"/>
      <itemset nodeset="instance('district_list')/root/item[region_list =  /data/region ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <input ref="/data/cluster_name">
      <label ref="jr:itext('/data/cluster_name:label')"/>
    </input>
    <input ref="/data/cluster_id">
      <label ref="jr:itext('/data/cluster_id:label')"/>
    </input>
    <select1 ref="/data/gps_method">
      <label ref="jr:itext('/data/gps_method:label')"/>
      <item>
        <label ref="jr:itext('/data/gps_method/Manuel:label')"/>
        <value>Manuel</value>
      </item>
      <item>
        <label ref="jr:itext('/data/gps_method/Automatique:label')"/>
        <value>Automatique</value>
      </item>
    </select1>
    <input ref="/data/GPS">
      <label ref="jr:itext('/data/GPS:label')"/>
      <hint ref="jr:itext('/data/GPS:hint')"/>
    </input>
    <input ref="/data/lat">
      <label ref="jr:itext('/data/lat:label')"/>
    </input>
    <input ref="/data/lng">
      <label ref="jr:itext('/data/lng:label')"/>
    </input>
    <group ref="/data/p_">
      <label ref="jr:itext('/data/p_:label')"/>
      <repeat nodeset="/data/p_">
        <select1 ref="/data/p_/add_participant">
          <label ref="jr:itext('/data/p_/add_participant:label')"/>
          <item>
            <label ref="jr:itext('/data/p_/add_participant/Oui:label')"/>
            <value>Oui</value>
          </item>
          <item>
            <label ref="jr:itext('/data/p_/add_participant/Non:label')"/>
            <value>Non</value>
          </item>
        </select1>
        <select1 ref="/data/p_/Imethod">
          <label ref="jr:itext('/data/p_/Imethod:label')"/>
          <item>
            <label ref="jr:itext('/data/p_/Imethod/Scanner:label')"/>
            <value>Scanner</value>
          </item>
          <item>
            <label ref="jr:itext('/data/p_/Imethod/Manuel:label')"/>
            <value>Manuel</value>
          </item>
        </select1>
        <input ref="/data/p_/BarcodeID">
          <label ref="jr:itext('/data/p_/BarcodeID:label')"/>
        </input>
        <input ref="/data/p_/GenerateID">
          <label ref="jr:itext('/data/p_/GenerateID:label')"/>
        </input>
        <select1 ref="/data/p_/filiation">
          <label ref="jr:itext('/data/p_/filiation:label')"/>
          <item>
            <label ref="jr:itext('/data/p_/filiation/Chef de famille / ménage:label')"/>
            <value>Chef de famille / ménage</value>
          </item>
          <item>
            <label ref="jr:itext('/data/p_/filiation/Epouse:label')"/>
            <value>Epouse</value>
          </item>
          <item>
            <label ref="jr:itext('/data/p_/filiation/Fils /  fille:label')"/>
            <value>Fils /  fille</value>
          </item>
          <item>
            <label ref="jr:itext('/data/p_/filiation/Frère / Soeur / cousin:label')"/>
            <value>Frère / Soeur / cousin</value>
          </item>
          <item>
            <label ref="jr:itext('/data/p_/filiation/Neuveu / Nièce / Petit-flis / Petite-fille:label')"/>
            <value>Neuveu / Nièce / Petit-flis / Petite-fille</value>
          </item>
          <item>
            <label ref="jr:itext('/data/p_/filiation/Veuf / veuve:label')"/>
            <value>Veuf / veuve</value>
          </item>
          <item>
            <label ref="jr:itext('/data/p_/filiation/Père ou mère:label')"/>
            <value>Père ou mère</value>
          </item>
          <item>
            <label ref="jr:itext('/data/p_/filiation/Pas de parenté:label')"/>
            <value>Pas de parenté</value>
          </item>
        </select1>
        <select1 ref="/data/p_/occupation">
          <label ref="jr:itext('/data/p_/occupation:label')"/>
          <item>
            <label ref="jr:itext('/data/p_/occupation/Agriculteur:label')"/>
            <value>Agriculteur</value>
          </item>
          <item>
            <label ref="jr:itext('/data/p_/occupation/Pêcheur:label')"/>
            <value>Pêcheur</value>
          </item>
          <item>
            <label ref="jr:itext('/data/p_/occupation/Fonctionnaire:label')"/>
            <value>Fonctionnaire</value>
          </item>
          <item>
            <label ref="jr:itext('/data/p_/occupation/Élève/étudiant:label')"/>
            <value>Élève/étudiant</value>
          </item>
          <item>
            <label ref="jr:itext('/data/p_/occupation/Ouvrier:label')"/>
            <value>Ouvrier</value>
          </item>
          <item>
            <label ref="jr:itext('/data/p_/occupation/Mécanicien:label')"/>
            <value>Mécanicien</value>
          </item>
          <item>
            <label ref="jr:itext('/data/p_/occupation/Ménagère:label')"/>
            <value>Ménagère</value>
          </item>
          <item>
            <label ref="jr:itext('/data/p_/occupation/Commerçant:label')"/>
            <value>Commerçant</value>
          </item>
          <item>
            <label ref="jr:itext('/data/p_/occupation/Autre:label')"/>
            <value>Autre</value>
          </item>
        </select1>
        <select1 ref="/data/p_/sex">
          <label ref="jr:itext('/data/p_/sex:label')"/>
          <item>
            <label ref="jr:itext('/data/p_/sex/Masculin:label')"/>
            <value>Masculin</value>
          </item>
          <item>
            <label ref="jr:itext('/data/p_/sex/Féminin:label')"/>
            <value>Féminin</value>
          </item>
        </select1>
        <input ref="/data/p_/age_yrs">
          <label ref="jr:itext('/data/p_/age_yrs:label')"/>
        </input>
        <select1 ref="/data/p_/is_natif">
          <label ref="jr:itext('/data/p_/is_natif:label')"/>
          <item>
            <label ref="jr:itext('/data/p_/is_natif/Oui:label')"/>
            <value>Oui</value>
          </item>
          <item>
            <label ref="jr:itext('/data/p_/is_natif/Non:label')"/>
            <value>Non</value>
          </item>
        </select1>
        <input ref="/data/p_/origine">
          <label ref="jr:itext('/data/p_/origine:label')"/>
        </input>
        <input ref="/data/p_/yrs_arriving">
          <label ref="jr:itext('/data/p_/yrs_arriving:label')"/>
        </input>
        <input ref="/data/p_/num_biopsie">
          <label ref="jr:itext('/data/p_/num_biopsie:label')"/>
        </input>
        <select1 ref="/data/p_/has_nodule">
          <label ref="jr:itext('/data/p_/has_nodule:label')"/>
          <item>
            <label ref="jr:itext('/data/p_/has_nodule/Oui:label')"/>
            <value>Oui</value>
          </item>
          <item>
            <label ref="jr:itext('/data/p_/has_nodule/Non:label')"/>
            <value>Non</value>
          </item>
        </select1>
        <input ref="/data/p_/nodule_nbr">
          <label ref="jr:itext('/data/p_/nodule_nbr:label')"/>
        </input>
        <select ref="/data/p_/nodule_location">
          <label ref="jr:itext('/data/p_/nodule_location:label')"/>
          <item>
            <label ref="jr:itext('/data/p_/nodule_location/Membres.Supérieures:label')"/>
            <value>Membres.Supérieures</value>
          </item>
          <item>
            <label ref="jr:itext('/data/p_/nodule_location/Membres.Inférieures:label')"/>
            <value>Membres.Inférieures</value>
          </item>
          <item>
            <label ref="jr:itext('/data/p_/nodule_location/Torse:label')"/>
            <value>Torse</value>
          </item>
        </select>
        <input ref="/data/p_/mfs_gauche">
          <label ref="jr:itext('/data/p_/mfs_gauche:label')"/>
        </input>
        <input ref="/data/p_/mfs_droite">
          <label ref="jr:itext('/data/p_/mfs_droite:label')"/>
        </input>
        <input ref="/data/p_/AdditionalNotes">
          <label ref="jr:itext('/data/p_/AdditionalNotes:label')"/>
        </input>
      </repeat>
    </group>
  </h:body>
</h:html>
