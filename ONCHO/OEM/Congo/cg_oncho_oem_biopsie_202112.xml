      <bind nodeset="/data/cluster_name" required="true()" type="string"/>
      <bind nodeset="/data/zone" required="true()" type="string"/>
      <bind nodeset="/data/Imethod" required="true()" type="string"/>
      <bind constraint="regex(., '^OEM-[0-9]{8}$')" jr:constraintMsg="jr:itext('/data/BarcodeID:jr:constraintMsg')" nodeset="/data/BarcodeID" relevant=" /data/Imethod  = 'Scanner'" required="true()" type="barcode"/>
      <bind constraint="regex(., '^OEM-[0-9]{8}$')" jr:constraintMsg="jr:itext('/data/GenerateID:jr:constraintMsg')" nodeset="/data/GenerateID" relevant=" /data/Imethod  = 'Manuel'" required="true()" type="string"/>
