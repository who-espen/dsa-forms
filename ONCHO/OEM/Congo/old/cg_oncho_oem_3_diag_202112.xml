      <bind constraint=". &gt; 9 and .&lt; 100" jr:constraintMsg="jr:itext('/data/d_recorder_id:jr:constraintMsg')" nodeset="/data/d_recorder_id" required="true()" type="int"/>
      <bind nodeset="/data/d_region" type="string"/>
      <bind nodeset="/data/d_district" required="true()" type="string"/>
      <bind nodeset="/data/d_cluster_name" required="true()" type="string"/>
      <bind constraint=". &gt; 99 and . &lt; 1000" jr:constraintMsg="jr:itext('/data/d_cluster_id1:jr:constraintMsg')" nodeset="/data/d_cluster_id1" required="true()" type="int"/>
      <bind constraint=". =  /data/d_cluster_id1 " jr:constraintMsg="jr:itext('/data/d_cluster_id2:jr:constraintMsg')" nodeset="/data/d_cluster_id2" required="true()" type="int"/>
      <bind nodeset="/data/d_ID_method" required="true()" type="string"/>
      <bind nodeset="/data/d_BarcodeID" relevant=" /data/d_ID_method  = 'Scanner'" required="true()" type="barcode"/>
      <bind constraint="regex(., '^[0-9]{3}-[0-9]{6}$')" jr:constraintMsg="jr:itext('/data/d_GenerateID:jr:constraintMsg')" nodeset="/data/d_GenerateID" relevant=" /data/d_ID_method  = 'Automatique'" required="true()" type="string"/>
