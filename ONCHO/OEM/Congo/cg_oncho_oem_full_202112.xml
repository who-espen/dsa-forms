      <bind nodeset="/data/cluster_name" required="true()" type="string"/>
      <bind nodeset="/data/zone" required="true()" type="string"/>
      <bind nodeset="/data/GPS" required="true()" type="geopoint"/>
      <bind nodeset="/data/participant/add_more" type="string"/>
      <bind nodeset="/data/participant/consent" relevant=" ../add_more  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/participant/sex" relevant=" ../consent  = 'Oui' and  ../add_more  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/participant/Imethod" relevant=" ../consent  = 'Oui' and  ../add_more  = 'Oui'" required="true()" type="string"/>
      <bind constraint="regex(., '^OEM-[0-9]{8}$')" jr:constraintMsg="jr:itext('/data/participant/BarcodeID:jr:constraintMsg')" nodeset="/data/participant/BarcodeID" relevant=" ../consent  = 'Oui' and  ../Imethod  = 'Scanner' and  ../add_more  = 'Oui'" required="true()" type="barcode"/>
      <bind constraint="regex(., '^OEM-[0-9]{8}$')" jr:constraintMsg="jr:itext('/data/participant/GenerateID:jr:constraintMsg')" nodeset="/data/participant/GenerateID" relevant=" ../consent  = 'Oui' and  ../Imethod  = 'Manuel' and  ../add_more  = 'Oui'" required="true()" type="string"/>
      <bind constraint=". &gt; 17 and . &lt;= 120" jr:constraintMsg="jr:itext('/data/participant/age_yrs:jr:constraintMsg')" nodeset="/data/participant/age_yrs" relevant=" ../consent  = 'Oui' and  ../add_more  = 'Oui'" required="true()" type="int"/>
      <bind nodeset="/data/participant/is_natif" relevant=" ../consent  = 'Oui' and  ../add_more  = 'Oui'" required="true()" type="string"/>
      <bind constraint=". &lt;= (2021 -  ../age_yrs )" jr:constraintMsg="jr:itext('/data/participant/yrs_arriving:jr:constraintMsg')" nodeset="/data/participant/yrs_arriving" relevant=" ../consent  = 'Oui' and  ../is_natif  = 'Non' and  ../add_more  = 'Oui'" required="true()" type="int"/>
      <bind nodeset="/data/participant/num_biopsie" relevant=" ../consent  = 'Oui' and  ../add_more  = 'Oui'" required="true()" type="int"/>
      <bind nodeset="/data/participant/mfs_1" relevant=" ../consent  = 'Oui' and  ../add_more  = 'Oui'" required="true()" type="decimal"/>
      <bind nodeset="/data/participant/mfs_2" relevant=" ../consent  = 'Oui' and  ../add_more  = 'Oui'" required="true()" type="decimal"/>
      <bind nodeset="/data/participant/nodule_localisation" relevant=" ../consent  = 'Oui' and  ../add_more  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/participant/nodule_nbe" relevant=" ../consent  = 'Oui' and  ../add_more  = 'Oui'" required="true()" type="int"/>
      <bind nodeset="/data/participant/collectedbs" relevant=" ../consent  = 'Oui' and  ../add_more  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/participant/ending_survey_note" readonly="true()" relevant=" ../consent  = 'Non' and  ../add_more  = 'Oui'" type="string"/>
      <bind nodeset="/data/participant/AdditionalNotes" relevant=" ../add_more  = 'Oui'" type="string"/>
