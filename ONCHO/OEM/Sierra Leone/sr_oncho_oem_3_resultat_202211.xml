    </select1>
    <select1 ref="/data/r_cluster_name">
      <label ref="jr:itext('/data/r_cluster_name:label')"/>
      <itemset nodeset="instance('community')/root/item[district_list =  /data/r_district ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <select1 ref="/data/r_cluster_id">
