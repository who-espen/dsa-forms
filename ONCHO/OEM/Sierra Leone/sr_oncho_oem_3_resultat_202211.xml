      <bind constraint="regex(., '^[0-9]{3}$')" jr:constraintMsg="jr:itext('/data/r_serial_number1:jr:constraintMsg')" nodeset="/data/r_serial_number1" required="true()" type="string"/>
      <bind constraint=". =  /data/r_serial_number1 " jr:constraintMsg="jr:itext('/data/r_serial_number2:jr:constraintMsg')" nodeset="/data/r_serial_number2" required="true()" type="string"/>
