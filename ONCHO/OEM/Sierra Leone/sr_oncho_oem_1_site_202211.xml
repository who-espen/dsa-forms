      <bind constraint=". &gt; 9 and .&lt; 100" jr:constraintMsg="jr:itext('/data/c_recorder_id:jr:constraintMsg')" nodeset="/data/c_recorder_id" required="true()" type="int"/>
      <bind nodeset="/data/c_district" required="true()" type="string" db_add_col_1="."/>
      <bind nodeset="/data/c_cluster_name" required="true()" type="string" db_add_col_1="/data/c_district" db_add_col_2="."/>
      <bind nodeset="/data/c_cluster_name_other" relevant=" /data/c_cluster_name  = 'Other'" required="true()" type="string" db_add_col_1="/data/c_district" db_add_col_2="."/>
      <bind constraint=". &gt; 99 and . &lt; 1000" jr:constraintMsg="jr:itext('/data/c_cluster_id:jr:constraintMsg')" nodeset="/data/c_cluster_id" relevant=" /data/c_cluster_name != 'Other'" required="true()" type="string"  db_add_col_2="/data/c_cluster_name" db_add_col_3="."/>
      <bind constraint=". &gt; 99 and . &lt; 1000" jr:constraintMsg="jr:itext('/data/c_cluster_id_other:jr:constraintMsg')" nodeset="/data/c_cluster_id_other" relevant=" /data/c_cluster_name  = 'Other'" required="true()" type="string" db_add_col_2="/data/c_cluster_name_other" db_add_col_3="."/>
      <bind nodeset="/data/c_date" required="true()" type="date"/>
      <bind nodeset="/data/c_gps" required="true()" type="geopoint"/>
      <bind nodeset="/data/c_population" required="true()" type="int"/>
      <bind calculate="concat(B19,' ',B20)" nodeset="/data/c_BlackFly" required="true()" type="string"/>
      <bind nodeset="/data/c_notes" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/c_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/c_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
