  <h:body>
    <input ref="/data/c_recorder_id">
      <label ref="jr:itext('/data/c_recorder_id:label')"/>
      <hint ref="jr:itext('/data/c_recorder_id:hint')"/>
    </input>
    <select1 ref="/data/c_district">
      <label ref="jr:itext('/data/c_district:label')"/>
      <item>
        <label>BONTHE</label>
        <value>BONTHE</value>
      </item>
      <item>
        <label>WESTERN RURAL</label>
        <value>WESTERN RURAL</value>
      </item>
    </select1>
    <select1 ref="/data/c_cluster_name">
      <label ref="jr:itext('/data/c_cluster_name:label')"/>
      <itemset nodeset="instance('community')/root/item[district_list =  /data/c_district ]">
        <value ref="name"/>
        <label ref="label"/>
      </itemset>
    </select1>
    <input ref="/data/c_cluster_name_other">
      <label ref="jr:itext('/data/c_cluster_name_other:label')"/>
    </input>
    <select1 ref="/data/c_cluster_id">
      <label ref="jr:itext('/data/c_cluster_id:label')"/>
      <itemset nodeset="instance('community_id')/root/item[community =  /data/c_cluster_name ]">
        <value ref="name"/>
        <label ref="label"/>
      </itemset>
    </select1>
    <select1 ref="/data/c_cluster_id_other">
      <label ref="jr:itext('/data/c_cluster_id_other:label')"/>
      <itemset nodeset="instance('community_replacement_id')/root/item[district_list =  /data/c_district ]">
        <value ref="name"/>
        <label ref="label"/>
      </itemset>
    </select1>
    <input ref="/data/c_date">
      <label ref="jr:itext('/data/c_date:label')"/>
    </input>
    <input ref="/data/c_gps">
      <label ref="jr:itext('/data/c_gps:label')"/>
      <hint ref="jr:itext('/data/c_gps:hint')"/>
    </input>
    <input ref="/data/c_population">
      <label ref="jr:itext('/data/c_population:label')"/>
    </input>
    <select1 ref="/data/c_BlackFly">
      <label ref="jr:itext('/data/c_BlackFly:label')"/>
      <item>
        <label>Yes</label>
        <value>Yes</value>
      </item>
      <item>
        <label>No</label>
        <value>No</value>
      </item>
    </select1>
    <input ref="/data/c_notes">
      <label ref="jr:itext('/data/c_notes:label')"/>
    </input>
  </h:body>
</h:html>
