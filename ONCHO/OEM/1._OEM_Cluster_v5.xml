<h:html xmlns="http://www.w3.org/2002/xforms" 
  xmlns:h="http://www.w3.org/1999/xhtml" 
  xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
  xmlns:jr="http://openrosa.org/javarosa">
  <h:head>
    <h:title>1. (English) OEM Cluster v5 Mozambique</h:title>
    <model>
      <instance>
        <data id="OEMClusterv5_Mozambique">
          <meta>
            <instanceID/>
          </meta>
          <c_StartTime/>
          <c_EndTime/>
          <c_RecorderID/>
          <c_DistrictID/>
          <c_EU/>
          <c_ClusterName/>
          <c_ClusterID1/>
          <c_ClusterID2/>
          <c_GPS/>
          <c_VillagePop/>
          <c_BlackFly/>
          <c_ClusterNotes/>
        </data>
      </instance>
      <itext>
        <translation lang="English">
          <text id="/data/c_RecorderID:label">
            <value>Enter Recorder ID</value>
          </text>
          <text id="/data/c_RecorderID:hint">
            <value>2 digit code assigned to you</value>
          </text>
          <text id="/data/c_RecorderID:requiredMsg">
            <value></value>
          </text>
          <text id="/data/c_DistrictID:label">
            <value>Select District</value>
          </text>

          <!-- District label begin -->

          
          <text id="/data/c_DistrictID:option0">
            <value>Buzi</value>
          </text>
          <text id="/data/c_DistrictID:option1">
            <value>G么ndola</value>
          </text>
          <text id="/data/c_DistrictID:option2">
            <value>Lago</value>
          </text>
          <text id="/data/c_DistrictID:option3">
            <value>Machaze</value>
          </text>
          <text id="/data/c_DistrictID:option4">
            <value>Manica</value>
          </text>
          <text id="/data/c_DistrictID:option5">
            <value>Maravia</value>
          </text>
          <text id="/data/c_DistrictID:option6">
            <value>Sanga</value>
          </text>
          <text id="/data/c_DistrictID:option7">
            <value>Spared District (SP)</value>
          </text>


          <!-- District label end -->

          <!-- EU label Begin -->

          <text id="/data/c_EU:label">
            <value>Select EU</value>
          </text>
          <text id="/data/c_EU:option0">
            <value>BZI</value>
          </text>
          <text id="/data/c_EU:option1">
            <value>GDL</value>
          </text>
          <text id="/data/c_EU:option2">
            <value>LGO</value>
          </text>
          <text id="/data/c_EU:option3">
            <value>MCZ</value>
          </text>
          <text id="/data/c_EU:option4">
            <value>MNC</value>
          </text>
          <text id="/data/c_EU:option5">
            <value>MRV</value>
          </text>
          <text id="/data/c_EU:option6">
            <value>SNG</value>
          </text>
          <text id="/data/c_EU:option7">
            <value>SPD</value>
          </text>

          <!-- EU label End -->

          <text id="/data/c_ClusterName:label">
            <value>Enter Cluster Name</value>
          </text>

          <text id="/data/c_ClusterID1:label">
            <value>Enter Cluster ID</value>
          </text>
          <text id="/data/c_ClusterID2:label">
            <value>Enter Cluster ID again</value>
          </text>          
          <text id="/data/c_ClusterID1:hint">
            <value>example: A100, D5, SP300</value>
          </text>
          <text id="/data/c_GPS:label">
            <value>Collect GPS data</value>
          </text>
          <text id="/data/c_GPS:hint">
            <value>Works best outside of buildings</value>
          </text>
          <text id="/data/c_VillagePop:label">
            <value>Enter the total population of the village (cluster)</value>
          </text>
          <text id="/data/c_VillagePop:hint">
            <value>Please obtain this information from the community MDA register</value>
          </text>
          <text id="/data/c_BlackFly:label">
            <value>Can local leaders confirm the presence of biting black flies in or near the community?</value>
          </text>
          <text id="/data/c_BlackFly:option0">
            <value>Yes</value>
          </text>
          <text id="/data/c_BlackFly:option1">
            <value>No</value>
          </text>
          <text id="/data/c_BlackFly:option2">
            <value>Don't know</value>
          </text>
          <text id="/data/c_ClusterNotes:label">
            <value>Additional notes to add?</value>
          </text>
        </translation>
      </itext>

      <instance id="districts">
        <root>          
          <item>
            <itextId>static_instance-districts-0</itextId>
            <name>Buzi; 33210</name>
          </item>
          <item>
            <itextId>static_instance-districts-1</itextId>
            <name>G么ndola; 33152</name>
          </item>
          <item>
            <itextId>static_instance-districts-2</itextId>
            <name>Lago; 33196</name>
          </item>
          <item>
            <itextId>static_instance-districts-3</itextId>
            <name>Machaze; 33155</name>
          </item>
          <item>
            <itextId>static_instance-districts-4</itextId>
            <name>Manica; 33157</name>
          </item>
          <item>
            <itextId>static_instance-districts-5</itextId>
            <name>Maravia; 33233</name>
          </item>
          <item>
            <itextId>static_instance-districts-6</itextId>
            <name>Sanga; 33209</name>
          </item>
          <item>
            <itextId>static_instance-districts-7</itextId>
           <name>Spared District</name>
          </item>  
   
        </root>
      </instance>

      <instance id="eus">
        <root>          
          <item>
            <itextId>/data/c_EU:option0</itextId>
            <name>BZI</name>
            <c_DistrictID>Buzi; 33210</c_DistrictID>
          </item>
          <item>
            <itextId>/data/c_EU:option1</itextId>
            <name>GDL</name>
            <c_DistrictID>G么ndola; 33152</c_DistrictID>
          </item>
          <item>
            <itextId>/data/c_EU:option2</itextId>
            <name>LGO</name>
            <c_DistrictID>Lago; 33196</c_DistrictID>
          </item>
          <item>
            <itextId>/data/c_EU:option3</itextId>
            <name>MCZ</name>
            <c_DistrictID>Machaze; 33155</c_DistrictID>
          </item>
          <item>
            <itextId>/data/c_EU:option4</itextId>
            <name>MNC</name>
            <c_DistrictID>Manica; 33157</c_DistrictID>
          </item>
          <item>
            <itextId>/data/c_EU:option5</itextId>
            <name>MRV</name>
            <c_DistrictID>Maravia; 33233</c_DistrictID>
          </item>
          <item>
            <itextId>/data/c_EU:option6</itextId>
            <name>SNG</name>
            <c_DistrictID>Sanga; 33209</c_DistrictID>
          </item>
          <item>
            <itextId>/data/c_EU:option7</itextId>
            <name>SPD</name>
           <c_DistrictID>Spared District</c_DistrictID>
          </item>  
   
        </root>
      </instance>


      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
      <bind nodeset="/data/c_StartTime" type="dateTime" jr:preload="timestamp" jr:preloadParams="start"/>
      <bind nodeset="/data/c_EndTime" type="dateTime" jr:preload="timestamp" jr:preloadParams="end"/>
      <bind nodeset="/data/c_RecorderID" type="int" required="true()" maxLength="2" constraint="regex(., '^[0-9]{2}$')" jr:constraintMsg="Invalid. Recorder ID code is 2 numbers long."/>
      <bind nodeset="/data/c_DistrictID" type="select1" required="true()" db_add_col_1="."/>
      <bind nodeset="/data/c_EU" type="select1" required="true()" db_add_col_1="/data/c_DistrictID" db_add_col_2="."/>
      <bind nodeset="/data/c_ClusterName" type="string"  required="true()" />
      <bind nodeset="/data/c_ClusterID1" type="string" required="true()" maxLength="5" constraint="regex(., '^([a-gA-G]{1}|sp|SP|Sp|sP)[0-9]{1,3}$')"  jr:constraintMsg="Invalid. The format of the cluster ID is incorrect." />
      <bind nodeset="/data/c_ClusterID2" type="string" required="true()" maxLength="5" constraint=". =/data/c_ClusterID1" db_add_col_1="/data/c_DistrictID" db_add_col_2="/data/c_EU" db_add_col_3="."/>
      <bind nodeset="/data/c_GPS" type="geopoint" required="true()"/>
      <bind nodeset="/data/c_VillagePop" type="int" required="true()"/>
      <bind nodeset="/data/c_BlackFly" type="select1" required="true()"/>
      <bind nodeset="/data/c_ClusterNotes" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/data/c_RecorderID">
      <label ref="jr:itext('/data/c_RecorderID:label')"/>
      <hint ref="jr:itext('/data/c_RecorderID:hint')"/>
    </input>    

    <!-- District Begin -->
    <select1 ref="/data/c_DistrictID">
      <label ref="jr:itext('/data/c_DistrictID:label')"/>
      <item>
        <label ref="jr:itext('/data/c_DistrictID:option0')"/>
        <value>Buzi; 33210</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_DistrictID:option1')"/>
        <value>G么ndola; 33152</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_DistrictID:option2')"/>
        <value>Lago; 33196</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_DistrictID:option3')"/>
        <value>Machaze; 33155</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_DistrictID:option4')"/>
        <value>Manica; 33157</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_DistrictID:option5')"/>
        <value>Maravia; 33233</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_DistrictID:option6')"/>
        <value>Sanga; 33209</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_DistrictID:option7')"/>
        <value>Spared District</value>
      </item>
    </select1>   
    <!-- District label End -->

    <select1 ref="/data/c_EU">
      <label ref="jr:itext('/data/c_EU:label')"/>
      <itemset nodeset="instance('eus')/root/item[c_DistrictID= /data/c_DistrictID ]">
      <value ref="name"/>
      <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <!-- EU label End -->

    <input ref="/data/c_ClusterName">
      <label ref="jr:itext('/data/c_ClusterName:label')"/>
    </input>
    <input ref="/data/c_ClusterID1">
      <label ref="jr:itext('/data/c_ClusterID1:label')"/>      
      <hint ref="jr:itext('/data/c_ClusterID1:hint')"/>
    </input>
    <input ref="/data/c_ClusterID2">
      <label ref="jr:itext('/data/c_ClusterID2:label')"/>
    </input>
    <input ref="/data/c_GPS">
      <label ref="jr:itext('/data/c_GPS:label')"/>
      <hint ref="jr:itext('/data/c_GPS:hint')"/>
    </input>
    <input ref="/data/c_VillagePop">
      <label ref="jr:itext('/data/c_VillagePop:label')"/>
      <hint ref="jr:itext('/data/c_VillagePop:hint')"/>
    </input>
    <select1 ref="/data/c_BlackFly">
      <label ref="jr:itext('/data/c_BlackFly:label')"/>
      <item>
        <label ref="jr:itext('/data/c_BlackFly:option0')"/>
        <value>Yes</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_BlackFly:option1')"/>
        <value>No</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_BlackFly:option2')"/>
        <value>DontKnow</value>
      </item>
    </select1>
    <input ref="/data/c_ClusterNotes">
      <label ref="jr:itext('/data/c_ClusterNotes:label')"/>
    </input>
  </h:body>
</h:html>
