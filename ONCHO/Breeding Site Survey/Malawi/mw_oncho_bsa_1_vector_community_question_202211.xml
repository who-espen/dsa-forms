      <bind nodeset="/data/c_oncho_explanation" relevant=" /data/c_consent  = 'Yes' and selected( /data/c_know_oncho , 'Yes')" required="true()" type="string"/>
      <bind nodeset="/data/c_gps" relevant=" /data/c_consent  = 'Yes'" required="true()" type="geopoint"/>
      <bind nodeset="/data/c_note" type="string"/>
      <bind nodeset="/data/c_end_survey_note" readonly="true()" relevant=" /data/c_consent  = 'No'" type="string"/>
