          </item>
          <item>
            <itextId>river-7</itextId>
            <district>Phalombe</district>
            <name>Ruo River</name>
          </item>
          <item>
            <itextId>river-8</itextId>
            <district>Thyolo</district>
            <name>Maperera River</name>
          </item>
          <item>
            <itextId>river-9</itextId>
            <district>Thyolo</district>
            <name>Nachipere River</name>
          </item>
          <item>
            <itextId>river-10</itextId>
            <district>Thyolo</district>
            <name>Nswazi River</name>
          </item>
          <item>
            <itextId>river-11</itextId>
            <district>Thyolo</district>
            <name>Ruo River</name>
          </item>
          <item>
            <itextId>river-12</itextId>
