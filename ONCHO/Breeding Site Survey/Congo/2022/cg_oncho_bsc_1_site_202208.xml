  <h:body>
    <input ref="/data/s_recorder_id">
      <label ref="jr:itext('/data/s_recorder_id:label')"/>
      <hint ref="jr:itext('/data/s_recorder_id:hint')"/>
    </input>
    <select1 ref="/data/s_province">
      <label ref="jr:itext('/data/s_province:label')"/>
      <item>
        <label ref="jr:itext('/data/s_province/Bouenza:label')"/>
        <value>Bouenza</value>
      </item>
      <item>
        <label ref="jr:itext('/data/s_province/Cuvette-Ouest:label')"/>
        <value>Cuvette-Ouest</value>
      </item>
      <item>
        <label ref="jr:itext('/data/s_province/Lékoumou:label')"/>
        <value>Lékoumou</value>
      </item>
      <item>
        <label ref="jr:itext('/data/s_province/Pool:label')"/>
        <value>Pool</value>
      </item>
      <item>
        <label ref="jr:itext('/data/s_province/Sangha:label')"/>
        <value>Sangha</value>
      </item>
    </select1>
    <select1 ref="/data/s_district">
      <label ref="jr:itext('/data/s_district:label')"/>
      <itemset nodeset="instance('district_list')/root/item[region_list= /data/s_province ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <input ref="/data/s_team">
      <label ref="jr:itext('/data/s_team:label')"/>
    </input>
    <input ref="/data/s_code_team">
      <label ref="jr:itext('/data/s_code_team:label')"/>
    </input>
    <input ref="/data/s_river_basin_name">
      <label ref="jr:itext('/data/s_river_basin_name:label')"/>
    </input>
    <input ref="/data/s_river_name">
      <label ref="jr:itext('/data/s_river_name:label')"/>
    </input>
    <input ref="/data/s_site_id">
      <label ref="jr:itext('/data/s_site_id:label')"/>
    </input>
    <input ref="/data/s_GPS">
      <label ref="jr:itext('/data/s_GPS:label')"/>
      <hint ref="jr:itext('/data/s_GPS:hint')"/>
    </input>
    <select1 ref="/data/s_density">
      <label ref="jr:itext('/data/s_density:label')"/>
      <item>
        <label ref="jr:itext('/data/s_density/-:label')"/>
        <value>-</value>
      </item>
      <item>
        <label ref="jr:itext('/data/s_density/+:label')"/>
        <value>+</value>
      </item>
      <item>
        <label ref="jr:itext('/data/s_density/++:label')"/>
        <value>++</value>
      </item>
      <item>
        <label ref="jr:itext('/data/s_density/+++:label')"/>
        <value>+++</value>
      </item>
    </select1>
    <input ref="/data/s_temperature">
      <label ref="jr:itext('/data/s_temperature:label')"/>
    </input>
    <input ref="/data/s_notes">
      <label ref="jr:itext('/data/s_notes:label')"/>
    </input>
  </h:body>
</h:html>
