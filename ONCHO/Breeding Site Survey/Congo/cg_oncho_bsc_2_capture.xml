<?xml version="1.0" encoding="UTF-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms"
  xmlns:ev="http://www.w3.org/2001/xml-events"
  xmlns:h="http://www.w3.org/1999/xhtml"
  xmlns:jr="http://openrosa.org/javarosa"
  xmlns:odk="http://www.opendatakit.org/xforms"
  xmlns:orx="http://openrosa.org/xforms"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>2. Formulaire de capture des larves</h:title>
    <model >

      <instance>
        <data id="cg_oncho_bsc_2_capture" version="20200813">
          <b_recorder_id/>
          <b_district/>
          <b_site_id/>
          <b_hour/>
          <b_larvae_capture/>
          <b_captor_initial/>
          <b_weather/>
          <b_larvae_observation/>
          <b_notes/>
          <b_start/>
          <b_end/>
          <meta>
            <instanceID/>
          </meta>
        </data>
      </instance>

      <itext>
        <translation default="true()" lang="French">
          <text id="/data/b_recorder_id:jr:constraintMsg">
            <value>Le code doit être un nombre à deux chiffres entre 9 et 1000</value>
          </text>
          <text id="/data/b_recorder_id:label">
            <value>Entrer l'identifiant de l'enregistreur</value>
          </text>
          <text id="/data/b_recorder_id:hint">
            <value>Code à deux chiffres qui vous a été assigné</value>
          </text>
          <text id="/data/b_district:label">
            <value>Sélectionner un district (UI)</value>
          </text>
          <text id="/data/b_site_id:label">
            <value>Sélectionner le site ID</value>
          </text>
          <text id="/data/b_hour:label">
            <value>Sélectionner l'heure</value>
          </text>
          <text id="/data/b_hour/7:00 - 8:00:label">
            <value>7:00 - 8:00</value>
          </text>
          <text id="/data/b_hour/8:00 - 9:00:label">
            <value>8:00 - 9:00</value>
          </text>
          <text id="/data/b_hour/9:00 - 10:00:label">
            <value>9:00 - 10:00</value>
          </text>
          <text id="/data/b_hour/10:00 - 11:00:label">
            <value>10:00 - 11:00</value>
          </text>
          <text id="/data/b_hour/11:00 - 12:00:label">
            <value>11:00 - 12:00</value>
          </text>
          <text id="/data/b_hour/12:00 - 13:00:label">
            <value>12:00 - 13:00</value>
          </text>
          <text id="/data/b_hour/13:00 - 14:00:label">
            <value>13:00 - 14:00</value>
          </text>
          <text id="/data/b_hour/14:00 - 15:00:label">
            <value>14:00 - 15:00</value>
          </text>
          <text id="/data/b_hour/15:00 - 16:00:label">
            <value>15:00 - 16:00</value>
          </text>
          <text id="/data/b_hour/16:00 - 17:00:label">
            <value>16:00 - 17:00</value>
          </text>
          <text id="/data/b_larvae_capture:label">
            <value>Nombre de larves collectées</value>
          </text>
          <text id="/data/b_captor_initial:jr:constraintMsg">
            <value>La longueur des initiales doit être comprise entre 2 et 5</value>
          </text>
          <text id="/data/b_captor_initial:label">
            <value>Initiales du captureur</value>
          </text>
          <text id="/data/b_weather:label">
            <value>Observation climatique</value>
          </text>
          <text id="/data/b_weather/Cloudy:label">
            <value>Nuageux</value>
          </text>
          <text id="/data/b_weather/Rainy:label">
            <value>Pluvieux</value>
          </text>
          <text id="/data/b_weather/Sunny:label">
            <value>Ensoleillé</value>
          </text>
          <text id="/data/b_larvae_observation:label">
            <value>Observation macroscopique des larves</value>
          </text>
          <text id="/data/b_notes:label">
            <value>Informations additionnelles</value>
          </text>
        </translation>
        <translation lang="English">
          <text id="/data/b_recorder_id:jr:constraintMsg">
            <value>The code must be a two-digit number between 9 and 1000</value>
          </text>
          <text id="/data/b_recorder_id:label">
            <value>Enter Recorder ID</value>
          </text>
          <text id="/data/b_recorder_id:hint">
            <value>2 digit code assigned to you</value>
          </text>
          <text id="/data/b_district:label">
            <value>Select district</value>
          </text>
          <text id="/data/b_site_id:label">
            <value>Select the site ID</value>
          </text>
          <text id="/data/b_hour:label">
            <value>Select the hour</value>
          </text>
          <text id="/data/b_hour/7:00 - 8:00:label">
            <value>7:00 - 8:00</value>
          </text>
          <text id="/data/b_hour/8:00 - 9:00:label">
            <value>8:00 - 9:00</value>
          </text>
          <text id="/data/b_hour/9:00 - 10:00:label">
            <value>9:00 - 10:00</value>
          </text>
          <text id="/data/b_hour/10:00 - 11:00:label">
            <value>10:00 - 11:00</value>
          </text>
          <text id="/data/b_hour/11:00 - 12:00:label">
            <value>11:00 - 12:00</value>
          </text>
          <text id="/data/b_hour/12:00 - 13:00:label">
            <value>12:00 - 13:00</value>
          </text>
          <text id="/data/b_hour/13:00 - 14:00:label">
            <value>13:00 - 14:00</value>
          </text>
          <text id="/data/b_hour/14:00 - 15:00:label">
            <value>14:00 - 15:00</value>
          </text>
          <text id="/data/b_hour/15:00 - 16:00:label">
            <value>15:00 - 16:00</value>
          </text>
          <text id="/data/b_hour/16:00 - 17:00:label">
            <value>16:00 - 17:00</value>
          </text>
          <text id="/data/b_larvae_capture:label">
            <value>Number of larvae collected</value>
          </text>
          <text id="/data/b_captor_initial:jr:constraintMsg">
            <value>The length of the initials must be between 2 and 5</value>
          </text>
          <text id="/data/b_captor_initial:label">
            <value>Captor's initials</value>
          </text>
          <text id="/data/b_weather:label">
            <value>Weather observation</value>
          </text>
          <text id="/data/b_weather/Cloudy:label">
            <value>Cloudy</value>
          </text>
          <text id="/data/b_weather/Rainy:label">
            <value>Rainy</value>
          </text>
          <text id="/data/b_weather/Sunny:label">
            <value>Sunny</value>
          </text>
          <text id="/data/b_larvae_observation:label">
            <value>Macroscopic aspect of larvae</value>
          </text>
          <text id="/data/b_notes:label">
            <value>Additional notes</value>
          </text>
        </translation>
      </itext>
      <bind constraint="regex(.,'^[0-9]{2}$')" jr:constraintMsg="jr:itext('/data/b_recorder_id:jr:constraintMsg')" nodeset="/data/b_recorder_id" required="true()" type="int"/>
      <bind nodeset="/data/b_district" required="true()" type="string" db_get="col_1"/>
      <bind nodeset="/data/b_site_id" type="string" db_get="col_2" db_filter_by_col_1="/data/b_district"/>
      <bind nodeset="/data/b_hour" required="true()" type="string"/>
      <bind nodeset="/data/b_larvae_capture" required="true()" type="int"/>
      <bind constraint="regex(.,'^[a-zA-Z0-9]{2,5}$')" jr:constraintMsg="jr:itext('/data/b_captor_initial:jr:constraintMsg')" nodeset="/data/b_captor_initial" required="true()" type="string"/>
      <bind nodeset="/data/b_weather" required="true()" type="string"/>
      <bind nodeset="/data/b_larvae_observation" type="string"/>
      <bind nodeset="/data/b_notes" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/b_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/b_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/data/b_recorder_id">
      <label ref="jr:itext('/data/b_recorder_id:label')"/>
      <hint ref="jr:itext('/data/b_recorder_id:hint')"/>
    </input>
    <input ref="/data/b_district">
      <label ref="jr:itext('/data/b_district:label')"/>
    </input>
    <input ref="/data/b_site_id">
      <label ref="jr:itext('/data/b_site_id:label')"/>
    </input>
    <select1 ref="/data/b_hour">
      <label ref="jr:itext('/data/b_hour:label')"/>
      <item>
        <label ref="jr:itext('/data/b_hour/7:00 - 8:00:label')"/>
        <value>7:00 - 8:00</value>
      </item>
      <item>
        <label ref="jr:itext('/data/b_hour/8:00 - 9:00:label')"/>
        <value>8:00 - 9:00</value>
      </item>
      <item>
        <label ref="jr:itext('/data/b_hour/9:00 - 10:00:label')"/>
        <value>9:00 - 10:00</value>
      </item>
      <item>
        <label ref="jr:itext('/data/b_hour/10:00 - 11:00:label')"/>
        <value>10:00 - 11:00</value>
      </item>
      <item>
        <label ref="jr:itext('/data/b_hour/11:00 - 12:00:label')"/>
        <value>11:00 - 12:00</value>
      </item>
      <item>
        <label ref="jr:itext('/data/b_hour/12:00 - 13:00:label')"/>
        <value>12:00 - 13:00</value>
      </item>
      <item>
        <label ref="jr:itext('/data/b_hour/13:00 - 14:00:label')"/>
        <value>13:00 - 14:00</value>
      </item>
      <item>
        <label ref="jr:itext('/data/b_hour/14:00 - 15:00:label')"/>
        <value>14:00 - 15:00</value>
      </item>
      <item>
        <label ref="jr:itext('/data/b_hour/15:00 - 16:00:label')"/>
        <value>15:00 - 16:00</value>
      </item>
      <item>
        <label ref="jr:itext('/data/b_hour/16:00 - 17:00:label')"/>
        <value>16:00 - 17:00</value>
      </item>
    </select1>
    <input ref="/data/b_larvae_capture">
      <label ref="jr:itext('/data/b_larvae_capture:label')"/>
    </input>
    <input ref="/data/b_captor_initial">
      <label ref="jr:itext('/data/b_captor_initial:label')"/>
    </input>
    <select1 ref="/data/b_weather">
      <label ref="jr:itext('/data/b_weather:label')"/>
      <item>
        <label ref="jr:itext('/data/b_weather/Cloudy:label')"/>
        <value>Cloudy</value>
      </item>
      <item>
        <label ref="jr:itext('/data/b_weather/Rainy:label')"/>
        <value>Rainy</value>
      </item>
      <item>
        <label ref="jr:itext('/data/b_weather/Sunny:label')"/>
        <value>Sunny</value>
      </item>
    </select1>
    <input ref="/data/b_larvae_observation">
      <label ref="jr:itext('/data/b_larvae_observation:label')"/>
    </input>
    <input ref="/data/b_notes">
      <label ref="jr:itext('/data/b_notes:label')"/>
    </input>
  </h:body>
</h:html>
