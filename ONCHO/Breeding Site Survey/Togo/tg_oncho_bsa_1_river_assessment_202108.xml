          </text>
          <text id="/data/r_SiteSuitable/Oui:label">
            <value>Oui</value>
          </text>
          <text id="/data/r_SiteSuitable/Non:label">
            <value>Non</value>
          </text>
          <text id="/data/r_EvidenceOfLarvae:label">
