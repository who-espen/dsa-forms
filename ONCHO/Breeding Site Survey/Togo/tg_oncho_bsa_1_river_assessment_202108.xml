  <h:head>
    <h:title>(Août 2021) 1. Enquête Entomologique d'ONCHO - Inspection de la rivière v2</h:title>
    <model >
      <itext>
        <translation default="true()" lang="French">
          <text id="/data/s_recorder_id:jr:constraintMsg">
            <value>Invalide. Le code d'identification doit être compris entre 9 et 100.</value>
          </text>
          <text id="/data/s_recorder_id:label">
            <value>Code enregistreur</value>
          </text>
          <text id="/data/s_recorder_id:hint">
            <value>Le code à deux chiffres qui vous a été assigné</value>
          </text>
          <text id="/data/s_province:label">
            <value>Sélectionner une province</value>
          </text>
          <text id="/data/s_province/Centrale:label">
            <value>Centrale</value>
          </text>
          <text id="/data/s_province/Kara/Centrale:label">
            <value>Kara/Centrale</value>
          </text>
          <text id="/data/s_district:label">
            <value>Sélectionner un district</value>
          </text>
          <text id="/data/s_health_facility:label">
            <value>Sélectionner une formation sanitaire</value>
          </text>
          <text id="/data/s_site:label">
