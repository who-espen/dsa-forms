          <text id="fs_list-3">
            <value>USP SAGBADAI</value>
          </text>
          <text id="fs_list-4">
            <value>USP TINDJASSE</value>
          </text>
          <text id="site_list-0">
            <value>Baghan ou Mo village (Sur Mo)</value>
          </text>
          <text id="site_list-1">
            <value>Bato (Sur Mo)</value>
          </text>
          <text id="site_list-2">
            <value>Kouida (Sur Koui)</value>
          </text>
          <text id="site_list-3">
            <value>Mo à Mo ou Bouzalo (Sur Mo)</value>
          </text>
          <text id="site_list-4">
            <value>Tintsro ( Sur Assoukawkaw)</value>
          </text>
          <text id="code_site-0">
            <value>101</value>
          </text>
          <text id="code_site-1">
            <value>102</value>
          </text>
          <text id="code_site-2">
            <value>103</value>
          </text>
          <text id="code_site-3">
            <value>104</value>
          </text>
          <text id="code_site-4">
            <value>105</value>
          </text>
        </translation>
      </itext>
      <instance>
        <data id="tg_oncho_bsa_1_river_assessment_202108_v2">
          <s_recorder_id/>
          <s_province/>
          <s_district/>
          <s_health_facility/>
          <s_site/>
          <s_site_id/>
          <s_river_basin_name/>
          <s_river_name/>
          <s_date/>
          <s_gps/>
          <r_SiteSuitable/>
          <r_EvidenceOfLarvae/>
          <r_LarvaeRate/>
          <s_notes/>
          <s_start/>
          <s_end/>
          <meta>
            <instanceID/>
          </meta>
        </data>
      </instance>
      <instance id="yes_no">
        <root>
          <item>
            <itextId>yes_no-0</itextId>
            <name>Oui</name>
          </item>
          <item>
            <itextId>yes_no-1</itextId>
            <name>Non</name>
          </item>
        </root>
      </instance>
      <instance id="larvae_rate">
        <root>
          <item>
            <itextId>larvae_rate-0</itextId>
            <name>-</name>
          </item>
          <item>
            <itextId>larvae_rate-1</itextId>
            <name>+</name>
          </item>
          <item>
            <itextId>larvae_rate-2</itextId>
            <name>++</name>
          </item>
          <item>
            <itextId>larvae_rate-3</itextId>
            <name>+++</name>
          </item>
        </root>
