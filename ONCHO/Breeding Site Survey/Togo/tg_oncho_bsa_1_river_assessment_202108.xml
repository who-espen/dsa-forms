          <item>
            <itextId>region_list-1</itextId>
            <name>Kara/Centrale</name>
          </item>
        </root>
      </instance>
      <instance id="district_list">
        <root>
          <item>
            <itextId>district_list-0</itextId>
            <name>Bassar</name>
            <region_list>Kara/Centrale</region_list>
          </item>
          <item>
            <itextId>district_list-1</itextId>
            <name>Blitta</name>
