      <bind nodeset="/data/c_site" required="true()" type="string"/>
      <bind nodeset="/data/c_consent" required="true()" type="string"/>
      <bind nodeset="/data/c_gender" required="true()" type="string"/>
      <bind constraint=". &gt; 0 and . &lt;=120" jr:constraintMsg="jr:itext('/data/c_age:jr:constraintMsg')" nodeset="/data/c_age" relevant=" /data/c_consent  = 'Sim'" required="true()" type="int"/>
