      <bind constraint=". &lt;=  /data/c_age " jr:constraintMsg="jr:itext('/data/c_how_lng_lived:jr:constraintMsg')" nodeset="/data/c_how_lng_lived" relevant=" /data/c_consent  = 'Sim'" required="true()" type="int"/>
