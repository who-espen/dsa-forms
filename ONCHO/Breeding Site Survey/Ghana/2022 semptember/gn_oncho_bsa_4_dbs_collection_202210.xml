      <bind nodeset="/data/d_ivm" relevant="selected( /data/d_consent ,'Yes')" required="true()" type="string"/>
      <bind nodeset="/data/d_ivm_when" relevant="selected( /data/d_consent ,'Yes') and  /data/d_ivm  = 'Yes'"/>
