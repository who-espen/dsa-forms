      <bind nodeset="/data/d_ivm_where" relevant="selected( /data/d_consent ,'Yes') and  /data/d_ivm  = 'Yes'" required="true()" type="string"/>
      <bind nodeset="/data/d_IDMethod" relevant="selected( /data/d_consent ,'Yes')" required="true()" type="string"/>
      <bind nodeset="/data/d_BarcodeID" relevant="selected( /data/d_consent ,'Yes') and  /data/d_IDMethod  = 'Scanner'" required="true()" type="barcode"/>
      <bind nodeset="/data/d_GenerateID" relevant="selected( /data/d_consent ,'Yes') and  /data/d_IDMethod != 'Scanner'" required="true()" type="string"/>
      <bind nodeset="/data/d_end_survey_note" readonly="true()" relevant=" /data/d_consent  = 'No'" type="string"/>
      <bind nodeset="/data/d_AdditionalNotes" type="string"/>
