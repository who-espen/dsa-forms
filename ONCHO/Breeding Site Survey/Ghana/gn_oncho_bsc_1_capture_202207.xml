      <bind nodeset="/data/s_region" required="true()" type="string"/>
      <bind nodeset="/data/s_district" required="true()" type="string"/>
      <bind nodeset="/data/s_transmission_zonz" required="true()" type="string"/>
      <bind nodeset="/data/s_gps" required="true()" type="geopoint"/>
      <bind nodeset="/data/s_village" required="true()" type="string"/>
      <bind nodeset="/data/s_site" required="true()" type="string"/>
      <bind nodeset="/data/s_site_id" required="true()" type="string"/>
      <bind nodeset="/data/s_river" required="true()" type="string"/>
      <bind nodeset="/data/s_collector_a" required="true()" type="string"/>
      <bind nodeset="/data/s_collector_b" required="true()" type="string"/>
      <bind nodeset="/data/s_date" required="true()" type="date"/>
      <bind nodeset="/data/s_/add_fly" required="true()" type="select1"/>
      <bind nodeset="/data/s_/time_interval" relevant=" ../add_fly  = &quot;Yes&quot;" required="true()" type="select1"/>
      <bind nodeset="/data/s_/nb_collected" relevant=" ../add_fly  = &quot;Yes&quot;" required="true()" type="int"/>
      <bind nodeset="/data/s_/weather" relevant=" ../add_fly  = &quot;Yes&quot;" required="true()" type="select1"/>
      <bind nodeset="/data/s_/remark" relevant=" ../add_fly  = &quot;Yes&quot;" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/s_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/s_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
