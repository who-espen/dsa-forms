      <bind nodeset="/data/c_consent" required="true()" type="string"/>
      <bind nodeset="/data/c_health_area" relevant=" /data/c_consent  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/c_age" relevant=" /data/c_consent  = 'Oui'" required="true()" type="int"/>
      <bind nodeset="/data/c_profession" relevant=" /data/c_consent  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/c_time_community" relevant=" /data/c_consent  = 'Oui'" required="true()" type="int"/>
      <bind nodeset="/data/c_black_flie_issue" relevant=" /data/c_consent  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/c_bite_nbr" relevant=" /data/c_consent  = 'Oui' and  /data/c_black_flie_issue  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/c_month" relevant=" /data/c_consent  = 'Oui' and  /data/c_black_flie_issue  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/c_end_note" readonly="true()" relevant=" /data/c_consent  = 'Non'" type="string"/>
      <bind nodeset="/data/c_notes" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/c_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/c_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
