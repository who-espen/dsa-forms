			<bind nodeset="/data/p_sac/p_sex" relevant=" ../p_consent  = 'Yes' and  ../p_add_more_child  = 'Yes'" required="true()" type="string"/>
			<bind nodeset="/data/p_sac/p_urinate_place" relevant=" ../p_consent  = 'Yes' and  ../p_add_more_child  = 'Yes'" required="true()" type="string"/>
			<bind nodeset="/data/p_sac/p_urinate_place_other" relevant=" ../p_urinate_place  = 'Other' and  ../p_consent  = 'Yes' and  ../p_add_more_child  = 'Yes'" required="true()" type="string"/>
			<bind nodeset="/data/p_sac/p_urinate_wash" relevant=" ../p_consent  = 'Yes' and  ../p_add_more_child  = 'Yes'" required="true()" type="string"/>
			<bind nodeset="/data/p_sac/p_urinate_wash_noreason" relevant=" ../p_urinate_wash  = 'No' and  ../p_consent  = 'Yes' and  ../p_add_more_child  = 'Yes'" required="true()" type="string"/>
			<bind nodeset="/data/p_sac/p_defecate_place" relevant=" ../p_consent  = 'Yes' and  ../p_add_more_child  = 'Yes'" required="true()" type="string"/>
			<bind nodeset="/data/p_sac/p_defecate_place_other" relevant=" ../p_defecate_place  = 'Other' and  ../p_consent  = 'Yes' and  ../p_add_more_child  = 'Yes'" required="true()" type="string"/>
