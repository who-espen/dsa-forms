			<bind nodeset="/data/p_consent" required="true()" type="string"/>
      <bind nodeset="/data/p_id_method" relevant=" /data/p_consent  = 'Yes'" required="true()" type="string"/>
      <bind nodeset="/data/p_barcodeid" relevant=" /data/p_id_method  = 'Scanner' and  /data/p_consent  = 'Yes'" required="true()" type="barcode"/>
      <bind nodeset="/data/p_generateid" relevant=" /data/p_id_method != 'Scanner' and  /data/p_consent  = 'Yes'" required="true()" type="string" calculate="concat(/data/p_school_id, '-', /data/meta/randomnum)" readonly="true()"/>
