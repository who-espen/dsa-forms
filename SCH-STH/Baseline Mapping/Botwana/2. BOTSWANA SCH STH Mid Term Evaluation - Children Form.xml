			<bind nodeset="/data/p_district" required="true()" type="string" db_get="col_2" db_filter_by_col_1="/data/p_region"/>
			<bind nodeset="/data/p_dist_abbr" required="true()" type="string" db_get="col_3" db_filter_by_col_2="/data/p_district"/>
			<bind nodeset="/data/p_school_name" required="true()" type="string" db_get="col_4" db_filter_by_col_2="/data/p_district"/>
			<bind constraint="regex(.,'^[0-9]{2}$')" jr:constraintMsg="jr:itext('/data/p_school_id:jr:constraintMsg')" nodeset="/data/p_school_id" required="true()" type="string" db_get="col_5" db_filter_by_col_4="/data/p_school_name"/>
			<bind nodeset="/data/p_sac/p_add_more_child" type="string"/>
			<bind nodeset="/data/p_sac/p_consent" relevant=" ../p_add_more_child  = 'Yes'" required="true()" type="string"/>
			<bind calculate="concat( /data/p_dist_abbr ,'-', /data/p_school_id ,'-',if(position(..) &lt; 10, concat('0', position(..)), position(..)))" constraint="regex(.,'^[A-Z]{2}-[0-9]{2}-[0-9]{2}$')" jr:constraintMsg="jr:itext('/data/p_sac/p_generateid:jr:constraintMsg')" nodeset="/data/p_sac/p_generateid" relevant=" ../p_consent  = 'Yes' and  ../p_add_more_child  = 'Yes'" required="true()" type="string"/>
