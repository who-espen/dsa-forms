      <bind nodeset="/data/p_urinate_place" relevant=" /data/p_consent  = 'Yes'" required="true()" type="string"/>
      <bind nodeset="/data/p_urinate_place_other" relevant=" /data/p_urinate_place  = 'Other' and  /data/p_consent  = 'Yes'" required="true()" type="string"/>
