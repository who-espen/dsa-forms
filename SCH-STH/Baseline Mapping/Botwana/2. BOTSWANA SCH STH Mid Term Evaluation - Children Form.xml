			<bind nodeset="/data/p_sac/p_defecate_wash_noreason" relevant=" ../p_defecate_wash  = 'No' and  ../p_consent  = 'Yes' and  ../p_add_more_child  = 'Yes'" required="true()" type="string"/>
			<bind nodeset="/data/p_sac/p_water_activities" relevant=" ../p_consent  = 'Yes' and  ../p_add_more_child  = 'Yes'" required="true()" type="string"/>
			<bind nodeset="/data/p_sac/p_ending_survey_note" readonly="true()" relevant=" ../p_consent  = 'No'" type="string"/>
			<bind nodeset="/data/p_sac/p_remarks" type="string"/>
			<bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/p_start" type="dateTime"/>
			<bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/p_end" type="dateTime"/>
			<bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
