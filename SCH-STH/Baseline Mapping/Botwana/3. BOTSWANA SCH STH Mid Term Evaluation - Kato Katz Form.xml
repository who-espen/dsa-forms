			</item>
		</select1>
		<select1 ref="/data/k_district">
			<label ref="jr:itext('/data/k_district:label')"/>
			<itemset nodeset="instance('district_list')/root/item[provinces =  /data/k_region ]">
				<value ref="name"/>
				<label ref="jr:itext(itextId)"/>
			</itemset>
		</select1>
		<select1 ref="/data/k_school_name">
			<label ref="jr:itext('/data/k_school_name:label')"/>
			<itemset nodeset="instance('school_list')/root/item[district_list =  /data/k_district ]">
