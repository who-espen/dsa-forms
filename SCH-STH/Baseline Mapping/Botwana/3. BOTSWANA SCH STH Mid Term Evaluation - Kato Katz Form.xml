			<bind nodeset="/data/k_region" required="true()" type="string"/>
			<bind nodeset="/data/k_district" required="true()" type="string"/>
			<bind nodeset="/data/k_school_name" required="true()" type="string"/>
			<bind constraint="regex(.,'^[0-9]{3}$')" jr:constraintMsg="jr:itext('/data/k_school_id:jr:constraintMsg')" nodeset="/data/k_school_id" required="true()" type="string"/>
			<bind nodeset="/data/k_id_type" required="true()" type="string"/>
			<bind nodeset="/data/k_barcode_id" relevant=" /data/k_id_type  = 'Scanner'" required="true()" type="barcode"/>
			<bind constraint="regex(.,'^[0-9]{3}-[0-9]{6}$')" jr:constraintMsg="jr:itext('/data/k_generate_id:jr:constraintMsg')" nodeset="/data/k_generate_id" relevant=" /data/k_id_type  != 'Scanner'" required="true()" type="string"/>
			<bind constraint=". =  /data/k_generate_id " jr:constraintMsg="jr:itext('/data/k_generate_id2:jr:constraintMsg')" nodeset="/data/k_generate_id2" relevant=" /data/k_id_type  != 'Scanner'" required="true()" type="string"/>
			<bind nodeset="/data/k_sch_man_sa" required="true()" type="int"/>
			<bind nodeset="/data/k_sch_man_sb" required="true()" type="int"/>
			<bind nodeset="/data/k_ascaris_lumb_sa" required="true()" type="int"/>
			<bind nodeset="/data/k_ascaris_lumb_sb" required="true()" type="int"/>
			<bind nodeset="/data/k_hookworm_sa" required="true()" type="int"/>
			<bind nodeset="/data/k_hookworm_sb" required="true()" type="int"/>
			<bind nodeset="/data/k_trichuris_sa" required="true()" type="int"/>
			<bind nodeset="/data/k_trichuris_sb" required="true()" type="int"/>
			<bind nodeset="/data/k_other_name_1" required="true()" type="string"/>
			<bind nodeset="/data/k_other_quantity_1" required="true()" type="int"/>
			<bind nodeset="/data/k_other_name_2" required="true()" type="string"/>
			<bind nodeset="/data/k_other_quantity_2" required="true()" type="int"/>
			<bind nodeset="/data/k_remarks" type="string"/>
			<bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/k_start" type="dateTime"/>
			<bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/k_end" type="dateTime"/>
			<bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
		</model>
	</h:head>
	<h:body>
		<input ref="/data/k_RecorderID">
			<label ref="jr:itext('/data/k_RecorderID:label')"/>
			<hint ref="jr:itext('/data/k_RecorderID:hint')"/>
		</input>
		<select1 ref="/data/k_region">
			<label ref="jr:itext('/data/k_region:label')"/>
			<item>
				<label ref="jr:itext('/data/k_region/Northern:label')"/>
				<value>Northern</value>
			</item>
			<item>
				<label ref="jr:itext('/data/k_region/Southern:label')"/>
