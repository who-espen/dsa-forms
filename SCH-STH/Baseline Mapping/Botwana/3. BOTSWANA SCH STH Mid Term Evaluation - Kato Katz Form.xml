			<hint ref="jr:itext('/data/k_school_id:hint')"/>
			<itemset nodeset="instance('school_id_list')/root/item[school_list =  /data/k_school_name ]">
				<value ref="name"/>
				<label ref="jr:itext(itextId)"/>
			</itemset>
		</select1>
		<select1 ref="/data/k_id_type">
			<label ref="jr:itext('/data/k_id_type:label')"/>
			<item>
				<label ref="jr:itext('/data/k_id_type/Scanner:label')"/>
				<value>Scanner</value>
			</item>
			<item>
				<label ref="jr:itext('/data/k_id_type/ID_generation:label')"/>
				<value>ID_generation</value>
			</item>
		</select1>
		<input ref="/data/k_barcode_id">
			<label ref="jr:itext('/data/k_barcode_id:label')"/>
		</input>
		<input ref="/data/k_generate_id">
			<label ref="jr:itext('/data/k_generate_id:label')"/>
		</input>
		<input ref="/data/k_generate_id2">
			<label ref="jr:itext('/data/k_generate_id2:label')"/>
		</input>
		<input ref="/data/k_sch_man_sa">
			<label ref="jr:itext('/data/k_sch_man_sa:label')"/>
		</input>
		<input ref="/data/k_sch_man_sb">
			<label ref="jr:itext('/data/k_sch_man_sb:label')"/>
		</input>
		<input ref="/data/k_ascaris_lumb_sa">
			<label ref="jr:itext('/data/k_ascaris_lumb_sa:label')"/>
		</input>
		<input ref="/data/k_ascaris_lumb_sb">
			<label ref="jr:itext('/data/k_ascaris_lumb_sb:label')"/>
		</input>
		<input ref="/data/k_hookworm_sa">
			<label ref="jr:itext('/data/k_hookworm_sa:label')"/>
		</input>
		<input ref="/data/k_hookworm_sb">
			<label ref="jr:itext('/data/k_hookworm_sb:label')"/>
		</input>
		<input ref="/data/k_trichuris_sa">
			<label ref="jr:itext('/data/k_trichuris_sa:label')"/>
		</input>
		<input ref="/data/k_trichuris_sb">
			<label ref="jr:itext('/data/k_trichuris_sb:label')"/>
		</input>
		<input ref="/data/k_other_name_1">
			<label ref="jr:itext('/data/k_other_name_1:label')"/>
		</input>
		<input ref="/data/k_other_quantity_1">
			<label ref="jr:itext('/data/k_other_quantity_1:label')"/>
		</input>
		<input ref="/data/k_other_name_2">
			<label ref="jr:itext('/data/k_other_name_2:label')"/>
		</input>
		<input ref="/data/k_other_quantity_2">
			<label ref="jr:itext('/data/k_other_quantity_2:label')"/>
		</input>
		<input ref="/data/k_remarks">
			<label ref="jr:itext('/data/k_remarks:label')"/>
		</input>
	</h:body>
</h:html>