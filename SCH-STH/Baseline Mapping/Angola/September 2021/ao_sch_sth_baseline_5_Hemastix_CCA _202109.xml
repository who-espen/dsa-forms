      <bind nodeset="/data/h_province" required="true()" type="text" db_get="col_1"/>
      <bind nodeset="/data/h_municipality" required="true()" type="text" db_get="col_2" db_filter_by_col_1="/data/h_province"/>
      <bind nodeset="/data/h_commune" required="true()" type="text" db_get="col_3" db_filter_by_col_2="/data/h_municipality"/>
      <bind nodeset="/data/h_school" required="true()" type="text" db_get="col_4" db_filter_by_col_3="/data/h_commune"/>
      <bind nodeset="/data/h_school_id" required="true()" type="text" db_get="col_5" db_filter_by_col_4="/data/h_school"/>
      <bind constraint="regex(., '^[A-Za-z]{1,5}$')" jr:constraintMsg="jr:itext('/data/h_RecorderID:jr:constraintMsg')" nodeset="/data/h_RecorderID" required="true()" type="text"/>
      <bind nodeset="/data/h_date" required="true()" type="date"/>
      <bind jr:constraintMsg="jr:itext('/data/hemastix:jr:constraintMsg')" nodeset="/data/hemastix"/>
      <bind nodeset="/data/hemastix/h_add_more_child" required="true()" type="text"/>
      <bind nodeset="/data/hemastix/h" relevant=" ../h_add_more_child  = 'Sim'"/>
      <bind constraint="regex(., '^[0-9]{2}$')" jr:constraintMsg="jr:itext('/data/hemastix/h/num:jr:constraintMsg')" nodeset="/data/hemastix/h/num" required="true()" type="text"/>
      <bind constraint=". =  ../num " jr:constraintMsg="jr:itext('/data/hemastix/h/num2:jr:constraintMsg')" nodeset="/data/hemastix/h/num2" required="true()" type="text"/>
      <bind calculate="concat( /data/h_school_id , '-',  ../num )" nodeset="/data/hemastix/h/code_id" readonly="true()" required="true()" type="text"/>
      <bind nodeset="/data/hemastix/h/cca" required="true()" type="text"/>
      <bind nodeset="/data/hemastix/h/grading" relevant=" ../cca  = '1.Positivo'" required="true()" type="text"/>
      <bind nodeset="/data/hemastix/h/haematuria" required="true()" type="text"/>
      <bind nodeset="/data/hemastix/h/haematuria_other" relevant=" ../haematuria  = 'Outra'" required="true()" type="text"/>
      <bind nodeset="/data/hemastix/h/hematix" required="true()" type="text"/>
      <bind nodeset="/data/hemastix/h/remarks" type="text"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/h_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/h_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="text"/>
    </model>
