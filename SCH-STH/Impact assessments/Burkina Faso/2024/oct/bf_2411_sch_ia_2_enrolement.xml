      <bind nodeset="/data/bf_ia_e_2411_2/p_Echantillonselle" relevant=" ../p_consent  = '1_oui'" required="true()" type="select1"/>
      <bind nodeset="/data/bf_ia_e_2411_2/p_ending_survey_note" readonly="true()" relevant=" ../p_consent  = '2_non'" type="string"/>
      <bind nodeset="/data/bf_ia_e_2411_2/commentaires" type="text"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/p_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/p_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
