      <bind db_filter_by_col_5="/data/p_Ecole" db_get="col_6" nodeset="/data/p_Code_site" required="true()" type="string"/>
      <bind calculate="join(' ',  /data/bf_ia_e_2411_2/p_Numero_Enquete_complet )" nodeset="/data/C1" type="text"/>
      <bind calculate="position(..)" nodeset="/data/bf_ia_e_2411_2/C2" type="text"/>
      <bind calculate="if ( ../C2  = 1,'',substring-after( /data/C1 , ../p_Numero_Enquete_complet ))" nodeset="/data/bf_ia_e_2411_2/C3" type="text"/>
