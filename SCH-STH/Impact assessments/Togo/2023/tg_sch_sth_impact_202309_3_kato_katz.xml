<?xml version="1.0" encoding="UTF-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms"
  xmlns:ev="http://www.w3.org/2001/xml-events"
  xmlns:h="http://www.w3.org/1999/xhtml"
  xmlns:jr="http://openrosa.org/javarosa"
  xmlns:odk="http://www.opendatakit.org/xforms"
  xmlns:orx="http://openrosa.org/xforms"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>(2023 Avril) - 3. SCH/STH - Kato Katz</h:title>
    <model >

      <instance>
        <data id="tg_sch_sth_impact_3_kato_katz_202110">
          <k_Recorder/>
          <k_district/>
          <k_usp/>
          <k_site_name/>
          <k_barcodeid/>
          <k_barcode_ok/>
          <k_manual_code_id/>
          <k_sch_man_sa/>
          <k_sch_man_sb/>
          <k_ascaris_lumb_sa/>
          <k_ascaris_lumb_sb/>
          <k_hookworm_sa/>
          <k_hookworm_sb/>
          <k_trichuris_sa/>
          <k_trichuris_sb/>
          <k_sch_hae_sa/>
          <k_sch_hae_sb/>
          <k_remarks/>
          <k_start/>
          <k_end/>
          <meta>
            <instanceID/>
          </meta>
        </data>
      </instance>

      <itext>
        <translation default="true()" lang="French">
          <text id="/data/k_Recorder:jr:constraintMsg">
            <value>Doit contenir 2 à 4 caractère alphanumérique</value>
          </text>
          <text id="/data/k_Recorder:label">
            <value>Sélectionner l'enquêteur</value>
          </text>
          <text id="/data/k_district:label">
            <value>Sélectionner le district</value>
          </text>
          <text id="/data/k_usp:label">
            <value>Sélectionner le sous-district (USP)</value>
          </text>
          <text id="/data/k_site_name:label">
            <value>Sélectionner le nom du village</value>
          </text>
          <text id="/data/k_barcodeid:label">
            <value>Veuillez scanner le QR Code</value>
          </text>
          <text id="/data/k_barcode_ok:label">
            <value>Le code QR a-t-il été correctement scanné</value>
          </text>
          <text id="/data/k_barcode_ok/Oui:label">
            <value>Oui</value>
          </text>
          <text id="/data/k_barcode_ok/Non:label">
            <value>Non</value>
          </text>
          <text id="/data/k_manual_code_id:label">
            <value>Saisez le code QR manuellement</value>
          </text>
          <text id="/data/k_sch_man_sa:label">
            <value>Schistosoma mansoni (oeufs) - Slide A</value>
          </text>
          <text id="/data/k_sch_man_sb:label">
            <value>Schistosoma mansoni (oeufs) - Slide B</value>
          </text>
          <text id="/data/k_ascaris_lumb_sa:label">
            <value>Ascaris lumbricoides (œufs) - Slide A</value>
          </text>
          <text id="/data/k_ascaris_lumb_sb:label">
            <value>Ascaris lumbricoides (œufs) - Slide B</value>
          </text>
          <text id="/data/k_hookworm_sa:label">
            <value>Ankylostome - Slide A</value>
          </text>
          <text id="/data/k_hookworm_sb:label">
            <value>Ankylostome - Slide B</value>
          </text>
          <text id="/data/k_trichuris_sa:label">
            <value>Trichocéphale trichura - Slide A</value>
          </text>
          <text id="/data/k_trichuris_sb:label">
            <value>Trichocéphale trichura - Slide B</value>
          </text>
          <text id="/data/k_sch_hae_sa:label">
            <value>Schistosoma haematobium – Slide A</value>
          </text>
          <text id="/data/k_sch_hae_sb:label">
            <value>Schistosoma haematobium – Slide B</value>
          </text>
          <text id="/data/k_remarks:label">
            <value>Notes additionnelles</value>
          </text>
        </translation>
      </itext>

      <bind constraint="regex(.,'^[a-zA-Z]{2,4}$')" jr:constraintMsg="jr:itext('/data/k_Recorder:jr:constraintMsg')" nodeset="/data/k_Recorder" required="true()" type="string"/>
      <bind nodeset="/data/k_district" required="true()" type="string" db_get="col_1"/>
      <bind nodeset="/data/k_usp" required="true()" type="string" db_get="col_2" db_filter_by_col_1="/data/k_district"/>
      <bind nodeset="/data/k_site_name" required="true()" type="string" db_get="col_3" db_filter_by_col_2="/data/k_usp"/>
      <bind nodeset="/data/k_barcodeid" type="barcode"/>
      <bind nodeset="/data/k_barcode_ok" required="true()" type="string"/>
      <bind nodeset="/data/k_manual_code_id" relevant=" /data/k_barcode_ok  = 'Non'" required="true()" type="string"/>
      <bind nodeset="/data/k_sch_man_sa" required="true()" type="int"/>
      <bind nodeset="/data/k_sch_man_sb" required="true()" type="int"/>
      <bind nodeset="/data/k_ascaris_lumb_sa" required="true()" type="int"/>
      <bind nodeset="/data/k_ascaris_lumb_sb" required="true()" type="int"/>
      <bind nodeset="/data/k_hookworm_sa" required="true()" type="int"/>
      <bind nodeset="/data/k_hookworm_sb" required="true()" type="int"/>
      <bind nodeset="/data/k_trichuris_sa" required="true()" type="int"/>
      <bind nodeset="/data/k_trichuris_sb" required="true()" type="int"/>
      <bind nodeset="/data/k_sch_hae_sa" type="int"/>
      <bind nodeset="/data/k_sch_hae_sb" type="int"/>
      <bind nodeset="/data/k_remarks" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/k_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/k_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/data/k_Recorder">
      <label ref="jr:itext('/data/k_Recorder:label')"/>
    </input>
    <input ref="/data/k_district">
      <label ref="jr:itext('/data/k_district:label')"/>
    </input>
    <input ref="/data/k_usp">
      <label ref="jr:itext('/data/k_usp:label')"/>
    </input>
    <input ref="/data/k_site_name">
      <label ref="jr:itext('/data/k_site_name:label')"/>
    </input>
    <input ref="/data/k_barcodeid">
      <label ref="jr:itext('/data/k_barcodeid:label')"/>
    </input>
    <select1 ref="/data/k_barcode_ok">
      <label ref="jr:itext('/data/k_barcode_ok:label')"/>
      <item>
        <label ref="jr:itext('/data/k_barcode_ok/Oui:label')"/>
        <value>Oui</value>
      </item>
      <item>
        <label ref="jr:itext('/data/k_barcode_ok/Non:label')"/>
        <value>Non</value>
      </item>
    </select1>
    <input ref="/data/k_manual_code_id">
      <label ref="jr:itext('/data/k_manual_code_id:label')"/>
    </input>
    <input ref="/data/k_sch_man_sa">
      <label ref="jr:itext('/data/k_sch_man_sa:label')"/>
    </input>
    <input ref="/data/k_sch_man_sb">
      <label ref="jr:itext('/data/k_sch_man_sb:label')"/>
    </input>
    <input ref="/data/k_ascaris_lumb_sa">
      <label ref="jr:itext('/data/k_ascaris_lumb_sa:label')"/>
    </input>
    <input ref="/data/k_ascaris_lumb_sb">
      <label ref="jr:itext('/data/k_ascaris_lumb_sb:label')"/>
    </input>
    <input ref="/data/k_hookworm_sa">
      <label ref="jr:itext('/data/k_hookworm_sa:label')"/>
    </input>
    <input ref="/data/k_hookworm_sb">
      <label ref="jr:itext('/data/k_hookworm_sb:label')"/>
    </input>
    <input ref="/data/k_trichuris_sa">
      <label ref="jr:itext('/data/k_trichuris_sa:label')"/>
    </input>
    <input ref="/data/k_trichuris_sb">
      <label ref="jr:itext('/data/k_trichuris_sb:label')"/>
    </input>
    <input ref="/data/k_sch_hae_sa">
      <label ref="jr:itext('/data/k_sch_hae_sa:label')"/>
    </input>
    <input ref="/data/k_sch_hae_sb">
      <label ref="jr:itext('/data/k_sch_hae_sb:label')"/>
    </input>
    <input ref="/data/k_remarks">
      <label ref="jr:itext('/data/k_remarks:label')"/>
    </input>
  </h:body>
</h:html>
