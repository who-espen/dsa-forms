<?xml version="1.0" encoding="UTF-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms"
  xmlns:ev="http://www.w3.org/2001/xml-events"
  xmlns:h="http://www.w3.org/1999/xhtml"
  xmlns:jr="http://openrosa.org/javarosa"
  xmlns:odk="http://www.opendatakit.org/xforms"
  xmlns:orx="http://openrosa.org/xforms"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>(2023 Avril) - 4. SCH/STH - Urine Filtration</h:title>
    <model >

      <instance>
        <data id="tg_sch_sth_impact_202304_4_urine_filtration">
          <u_recorder/>
          <u_district/>
          <u_usp/>
          <u_site_name/>
          <u_barcodeid/>
          <u_barcode_ok/>
          <u_manual_code_id/>
          <u_urine_vol/>
          <u_sh_egp_10ml/>
          <k_sch_man/>
          <u_remarks/>
          <u_start/>
          <u_end/>
          <meta>
            <instanceID/>
          </meta>
        </data>
      </instance>

      <itext>
        <translation default="true()" lang="French">
          <text id="/data/u_recorder:jr:constraintMsg">
            <value>Doit contenir 2 à 4 caractère alphanumérique</value>
          </text>
          <text id="/data/u_recorder:label">
            <value>Sélectionner l'enquêteur</value>
          </text>
          <text id="/data/u_district:label">
            <value>Sélectionner le district</value>
          </text>
          <text id="/data/u_usp:label">
            <value>Sélectionner le sous-district (USP)</value>
          </text>
          <text id="/data/u_site_name:label">
            <value>Sélectionner le nom du village</value>
          </text>
          <text id="/data/u_barcodeid:label">
            <value>Veuillez scanner le QR Code</value>
          </text>
          <text id="/data/u_barcode_ok:label">
            <value>Le code QR a-t-il été correctement scanné</value>
          </text>
          <text id="/data/u_barcode_ok/Oui:label">
            <value>Oui</value>
          </text>
          <text id="/data/u_barcode_ok/Non:label">
            <value>Non</value>
          </text>
          <text id="/data/u_manual_code_id:label">
            <value>Saisez le code QR manuellement</value>
          </text>
          <text id="/data/u_urine_vol:jr:constraintMsg">
            <value>Le volume doit être limité à 10ml</value>
          </text>
          <text id="/data/u_urine_vol:label">
            <value>Volume d'urine : Consigner les millilitres (ml) d'urine prélevés sur la personne</value>
          </text>
          <text id="/data/u_sh_egp_10ml:label">
            <value>Nombre d'œufs par 10 ml</value>
          </text>
          <text id="/data/k_sch_man:label">
            <value>Schistosoma mansoni (oeufs)</value>
          </text>
          <text id="/data/u_remarks:label">
            <value>Notes additionnelles</value>
          </text>
        </translation>
      </itext>

      <bind constraint="regex(.,'^[a-zA-Z]{2,4}$')" jr:constraintMsg="jr:itext('/data/u_recorder:jr:constraintMsg')" nodeset="/data/u_recorder" required="true()" type="string"/>
      <bind nodeset="/data/u_district" required="true()" type="string" db_get="col_1"/>
      <bind nodeset="/data/u_usp" required="true()" type="string" db_get="col_2" db_filter_by_col_1="/data/u_district"/>
      <bind nodeset="/data/u_site_name" required="true()" type="string" db_get="col_3" db_filter_by_col_2="/data/u_usp"/>
      <bind nodeset="/data/u_barcodeid" type="barcode"/>
      <bind nodeset="/data/u_barcode_ok" required="true()" type="string"/>
      <bind nodeset="/data/u_manual_code_id" relevant=" /data/u_barcode_ok  = 'Non'" required="true()" type="string"/>
      <bind constraint=". &gt;=0 and .&lt; 11" jr:constraintMsg="jr:itext('/data/u_urine_vol:jr:constraintMsg')" nodeset="/data/u_urine_vol" type="int"/>
      <bind nodeset="/data/u_sh_egp_10ml" required="true()" type="int"/>
      <bind nodeset="/data/k_sch_man" type="int"/>
      <bind nodeset="/data/u_remarks" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/u_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/u_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/data/u_recorder">
      <label ref="jr:itext('/data/u_recorder:label')"/>
    </input>
    <input ref="/data/u_district">
      <label ref="jr:itext('/data/u_district:label')"/>
    </input>
    <input ref="/data/u_usp">
      <label ref="jr:itext('/data/u_usp:label')"/>
    </input>
    <input ref="/data/u_site_name">
      <label ref="jr:itext('/data/u_site_name:label')"/>
    </input>
    <input ref="/data/u_barcodeid">
      <label ref="jr:itext('/data/u_barcodeid:label')"/>
    </input>
    <select1 ref="/data/u_barcode_ok">
      <label ref="jr:itext('/data/u_barcode_ok:label')"/>
      <item>
        <label ref="jr:itext('/data/u_barcode_ok/Oui:label')"/>
        <value>Oui</value>
      </item>
      <item>
        <label ref="jr:itext('/data/u_barcode_ok/Non:label')"/>
        <value>Non</value>
      </item>
    </select1>
    <input ref="/data/u_manual_code_id">
      <label ref="jr:itext('/data/u_manual_code_id:label')"/>
    </input>
    <input ref="/data/u_urine_vol">
      <label ref="jr:itext('/data/u_urine_vol:label')"/>
    </input>
    <input ref="/data/u_sh_egp_10ml">
      <label ref="jr:itext('/data/u_sh_egp_10ml:label')"/>
    </input>
    <input ref="/data/k_sch_man">
      <label ref="jr:itext('/data/k_sch_man:label')"/>
    </input>
    <input ref="/data/u_remarks">
      <label ref="jr:itext('/data/u_remarks:label')"/>
    </input>
  </h:body>
</h:html>
