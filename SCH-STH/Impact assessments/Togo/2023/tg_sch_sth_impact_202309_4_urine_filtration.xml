      <bind calculate="round( 10 *  /data/u_sh_egg  div  /data/u_urine_vol  )" nodeset="/data/u_sh_egg_per_10ml" readonly="true()" relevant=" /data/u_urine_conserve  = 'Oui'" type="string" db_get="col_6" db_filter_by_col_5="/data/u_barcodeid"/>
      <bind nodeset="/data/u_sch_man" relevant=" /data/u_urine_conserve  = 'Oui'" required="true()" type="int"/>
      <bind nodeset="/data/u_end_note" readonly="true()" relevant=" /data/u_urine_conserve  = 'Non'" type="string"/>
      <bind nodeset="/data/u_remarks" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/u_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/u_end" type="dateTime"/>
