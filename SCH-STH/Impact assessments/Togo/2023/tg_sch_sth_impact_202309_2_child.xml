      <bind nodeset="/data/p_consent_note" readonly="true()" type="string"/>
      <bind nodeset="/data/p_consent" required="true()" type="select1"/>
      <bind constraint="regex(.,'^(MO.?|SOT|TCH|TCH|BAS|AGO|AKE|AMO|DAN|HAH|KLO|KPE|MOY|WAW)_\d{4}$') and (substr( /data/p_district , 0, 3) = substr(., 0, 3) or substr( /data/p_district , 0, 3) = 'MO')" jr:constraintMsg="jr:itext('/data/p_barcodeid:jr:constraintMsg')" nodeset="/data/p_barcodeid" relevant=" /data/p_consent  = 'A.donne'" type="barcode" db_add_col_3="/data/p_site" db_add_col_4="."/>
      <bind constraint=". =  /data/p_barcodeid " jr:constraintMsg="jr:itext('/data/p_barcodeid2:jr:constraintMsg')" nodeset="/data/p_barcodeid2" relevant=" /data/p_consent  = 'A.donne'" required="true()" type="barcode"/>
