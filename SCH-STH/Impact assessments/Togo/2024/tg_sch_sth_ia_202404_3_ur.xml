<?xml version="1.0" encoding="UTF-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms"
  xmlns:ev="http://www.w3.org/2001/xml-events"
  xmlns:h="http://www.w3.org/1999/xhtml"
  xmlns:jr="http://openrosa.org/javarosa"
  xmlns:odk="http://www.opendatakit.org/xforms"
  xmlns:orx="http://openrosa.org/xforms"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>(2024 Avr) - 3. SCH/STH - Urine Filtration</h:title>
    <model>

      <instance>
        <data id="tg_sch_sth_ia_202404_3_ur">
          <u_recorder/>
          <u_district/>
          <u_usp/>
          <u_site_name/>
          <C1/>
          <tg_sch_sth_u_2404 jr:template="">
            <C2/>
            <C3/>
            <u_barcodeid/>
            <u_barcodeid2/>
            <u_urine_conserve/>
            <u_urine_vol/>
            <u_sh_egg/>
            <u_sh_egg_per_10ml/>
            <u_sch_man/>
            <u_sch_man_per_10ml/>
            <u_control/>
            <u_barcodeid3/>
            <u_urine_vol_control/>
            <u_sh_egg_control/>
            <u_sh_egg_per_10ml_control/>
            <u_sch_man_control/>
            <u_sch_man_per_10ml_control/>
            <u_end_note/>
            <u_remarks/>
          </tg_sch_sth_u_2404>
          <u_start/>
          <u_end/>
          <meta>
            <instanceID/>
          </meta>
        </data>
      </instance>

      <itext>
        <translation default="true()" lang="French">
          <text id="/data/tg_sch_sth_u_2404/u_control/Oui:label">
            <value>Oui</value>
          </text>
          <text id="/data/tg_sch_sth_u_2404:label">
            <value>Résultats Urine</value>
          </text>
          <text id="/data/tg_sch_sth_u_2404/u_control:label">
            <value>La lame est retenue pour le contrôle qualité</value>
          </text>
          <text id="/data/tg_sch_sth_u_2404/u_urine_conserve:label">
            <value>L'urine de l'enfant avait-elle était concervée</value>
          </text>
          <text id="/data/tg_sch_sth_u_2404/u_barcodeid2:jr:constraintMsg">
            <value>Le QR code doit être le même que le QR code sélectioné.</value>
          </text>
          <text id="/data/tg_sch_sth_u_2404/C3:label">
            <value>Child code</value>
          </text>
          <text id="/data/tg_sch_sth_u_2404/u_urine_vol:jr:constraintMsg">
            <value>Le volume doit être limité à 10ml</value>
          </text>
          <text id="/data/u_recorder:label">
            <value>Code Enregistreur</value>
          </text>
          <text id="/data/tg_sch_sth_u_2404/u_urine_vol:label">
            <value>Volume d'urine : Consigner les millilitres (ml) d'urine prélevés sur la personne</value>
          </text>
          <text id="/data/tg_sch_sth_u_2404/u_sch_man_per_10ml_control:label">
            <value>Nombre d'œufs (S. Mansoni) par 10 ml pour le contrôle</value>
          </text>
          <text id="/data/tg_sch_sth_u_2404/u_barcodeid2:label">
            <value>Veuillez scanner le QR Code à nouveau</value>
          </text>
          <text id="/data/tg_sch_sth_u_2404/u_sh_egg_per_10ml:label">
            <value>Nombre d'œufs (S. Haematobium) par 10 ml</value>
          </text>
          <text id="/data/tg_sch_sth_u_2404/u_barcodeid:jr:constraintMsg">
            <value>Ce QR code a déjà été utiliser dans cette école</value>
          </text>
          <text id="/data/tg_sch_sth_u_2404/u_urine_vol_control:jr:constraintMsg">
            <value>Le volume doit être limité à 10ml</value>
          </text>
          <text id="/data/tg_sch_sth_u_2404/u_sh_egg_per_10ml_control:label">
            <value>Nombre d'œufs (S. Haematobium) par 10 ml pour le contrôle</value>
          </text>
          <text id="/data/tg_sch_sth_u_2404/u_sch_man_control:label">
            <value>Nombre d'œufs (S. Mansoni) pour le contrôle</value>
          </text>
          <text id="/data/tg_sch_sth_u_2404/u_remarks:label">
            <value>Notes additionnelles</value>
          </text>
          <text id="/data/tg_sch_sth_u_2404/u_barcodeid3:jr:constraintMsg">
            <value>Le QR code doit être le même que le QR code sélectioné.</value>
          </text>
          <text id="/data/C1:label">
            <value>How many samples were collected in this school?</value>
          </text>
          <text id="/data/tg_sch_sth_u_2404/u_urine_vol_control:label">
            <value>Volume d'urine pour le contrôle</value>
          </text>
          <text id="/data/tg_sch_sth_u_2404/u_sh_egg:label">
            <value>Nombre d'œufs (S. Haematobium)</value>
          </text>
          <text id="/data/u_recorder:hint">
            <value>Code à 2 chiffrre qui vous a été affecté</value>
          </text>
          <text id="/data/tg_sch_sth_u_2404/u_sh_egg_control:label">
            <value>Nombre d'œufs (S. Haematobium) pour le contrôle</value>
          </text>
          <text id="/data/tg_sch_sth_u_2404/u_sch_man:label">
            <value>Nombre d'œufs (S. Mansoni)</value>
          </text>
          <text id="/data/tg_sch_sth_u_2404/u_barcodeid:label">
            <value>Veuillez sélectionner le QR code le l'enfant enregistré</value>
          </text>
          <text id="/data/tg_sch_sth_u_2404/u_barcodeid3:label">
            <value>Veuillez scanner le QR Code à nouveau</value>
          </text>
          <text id="/data/tg_sch_sth_u_2404/u_control/Non:label">
            <value>Non</value>
          </text>
          <text id="/data/u_usp:label">
            <value>Sélectionner le sous-district (USP)</value>
          </text>
          <text id="/data/u_recorder:jr:constraintMsg">
            <value>Doit être entre 9 et 100 exclut</value>
          </text>
          <text id="/data/u_district:label">
            <value>Sélectionner le district</value>
          </text>
          <text id="/data/u_site_name:label">
            <value>Sélectionner le nom du village</value>
          </text>
          <text id="/data/tg_sch_sth_u_2404/u_sch_man_per_10ml:label">
            <value>Nombre d'œufs (S. Mansoni) par 10 ml</value>
          </text>
          <text id="/data/tg_sch_sth_u_2404/u_end_note:label">
            <value>L'urine de l'enfant n'a pas été consevé. Vous ne pouvez pas enregistré ses résultats.</value>
          </text>
          <text id="/data/tg_sch_sth_u_2404/C2:label">
            <value>Please inter the child sequence number</value>
          </text>
        </translation>
      </itext>
      <bind constraint=". &gt; 9 and . &lt; 100" jr:constraintMsg="jr:itext('/data/u_recorder:jr:constraintMsg')" nodeset="/data/u_recorder" required="true()" type="int"/>
      <bind nodeset="/data/u_district" required="true()" type="string" db_get="col_1"/>
      <bind nodeset="/data/u_usp" required="true()" type="string" db_get="col_2" db_filter_by_col_1="/data/u_district"/>
      <bind nodeset="/data/u_site_name" required="true()" type="string" db_get="col_3" db_filter_by_col_2="/data/u_usp"/>
      <bind calculate="join(' ',  /data/tg_sch_sth_u_2404/u_barcodeid )" nodeset="/data/C1" type="text"/>
      <bind calculate="position(..)" nodeset="/data/tg_sch_sth_u_2404/C2" type="text"/>
      <bind calculate="if ( ../C2  = 1,'',substring-after( /data/C1 , ../u_barcodeid ))" nodeset="/data/tg_sch_sth_u_2404/C3" type="text"/>
      <bind constraint="not(selected( ../C3 ,  ../u_barcodeid ))" jr:constraintMsg="jr:itext('/data/tg_sch_sth_u_2404/u_barcodeid:jr:constraintMsg')" nodeset="/data/tg_sch_sth_u_2404/u_barcodeid" required="true()" type="string" db_get="col_4" db_filter_by_col_3="/data/u_site_name"/>
      <bind constraint=". =  ../u_barcodeid " jr:constraintMsg="jr:itext('/data/tg_sch_sth_u_2404/u_barcodeid2:jr:constraintMsg')" nodeset="/data/tg_sch_sth_u_2404/u_barcodeid2" required="true()" type="barcode"/>
      <bind nodeset="/data/tg_sch_sth_u_2404/u_urine_conserve" required="true()" type="string"/>
      <bind constraint=". &gt;=0 and .&lt; 11" jr:constraintMsg="jr:itext('/data/tg_sch_sth_u_2404/u_urine_vol:jr:constraintMsg')" nodeset="/data/tg_sch_sth_u_2404/u_urine_vol" relevant=" ../u_urine_conserve  = 'Oui'" required="true()" type="int"/>
      <bind nodeset="/data/tg_sch_sth_u_2404/u_sh_egg" relevant=" ../u_urine_conserve  = 'Oui'" required="true()" type="int"/>
      <bind calculate="round( 10 *  ../u_sh_egg  div  ../u_urine_vol  )" nodeset="/data/tg_sch_sth_u_2404/u_sh_egg_per_10ml" readonly="true()" relevant=" ../u_urine_conserve  = 'Oui'" type="string"/>
      <bind nodeset="/data/tg_sch_sth_u_2404/u_sch_man" relevant=" ../u_urine_conserve  = 'Oui'" required="true()" type="int"/>
      <bind calculate="round( 10 *  ../u_sch_man  div  ../u_urine_vol  )" nodeset="/data/tg_sch_sth_u_2404/u_sch_man_per_10ml" readonly="true()" relevant=" ../u_urine_conserve  = 'Oui'" required="true()" type="int"/>
      <bind nodeset="/data/tg_sch_sth_u_2404/u_control" relevant=" ../u_urine_conserve  = 'Oui'" type="select1"/>
      <bind constraint=". =  ../u_barcodeid " jr:constraintMsg="jr:itext('/data/tg_sch_sth_u_2404/u_barcodeid3:jr:constraintMsg')" nodeset="/data/tg_sch_sth_u_2404/u_barcodeid3" relevant=" ../u_control  = 'Oui' and  ../u_urine_conserve  = 'Oui'" required="true()" type="barcode"/>
      <bind constraint=". &gt;=0 and .&lt; 11" jr:constraintMsg="jr:itext('/data/tg_sch_sth_u_2404/u_urine_vol_control:jr:constraintMsg')" nodeset="/data/tg_sch_sth_u_2404/u_urine_vol_control" relevant=" ../u_control  = 'Oui' and  ../u_urine_conserve  = 'Oui'" required="true()" type="int"/>
      <bind nodeset="/data/tg_sch_sth_u_2404/u_sh_egg_control" relevant=" ../u_control  = 'Oui' and  ../u_urine_conserve  = 'Oui'" required="true()" type="int"/>
      <bind calculate="round( 10 *  ../u_sh_egg_control  div  ../u_urine_vol_control  )" nodeset="/data/tg_sch_sth_u_2404/u_sh_egg_per_10ml_control" readonly="true()" relevant=" ../u_control  = 'Oui' and  ../u_urine_conserve  = 'Oui'" type="string"/>
      <bind nodeset="/data/tg_sch_sth_u_2404/u_sch_man_control" relevant=" ../u_control  = 'Oui' and  ../u_urine_conserve  = 'Oui'" required="true()" type="int"/>
      <bind calculate="round( 10 *  ../u_sch_man_control  div  ../u_urine_vol  )" nodeset="/data/tg_sch_sth_u_2404/u_sch_man_per_10ml_control" readonly="true()" relevant=" ../u_control  = 'Oui' and  ../u_urine_conserve  = 'Oui'" required="true()" type="int"/>
      <bind nodeset="/data/tg_sch_sth_u_2404/u_end_note" readonly="true()" relevant=" ../u_urine_conserve  = 'Non'" type="string"/>
      <bind nodeset="/data/tg_sch_sth_u_2404/u_remarks" type="text"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/u_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/u_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/data/u_recorder">
      <label ref="jr:itext('/data/u_recorder:label')"/>
      <hint ref="jr:itext('/data/u_recorder:hint')"/>
    </input>
    <input ref="/data/u_district">
      <label ref="jr:itext('/data/u_district:label')"/>
    </input>
    <input ref="/data/u_usp">
      <label ref="jr:itext('/data/u_usp:label')"/>
    </input>
    <input ref="/data/u_site_name">
      <label ref="jr:itext('/data/u_site_name:label')"/>
    </input>
    <group ref="/data/tg_sch_sth_u_2404">
      <label ref="jr:itext('/data/tg_sch_sth_u_2404:label')"/>
      <repeat nodeset="/data/tg_sch_sth_u_2404">
        <input ref="/data/tg_sch_sth_u_2404/u_barcodeid">
          <label ref="jr:itext('/data/tg_sch_sth_u_2404/u_barcodeid:label')"/>
        </input>
        <input ref="/data/tg_sch_sth_u_2404/u_barcodeid2">
          <label ref="jr:itext('/data/tg_sch_sth_u_2404/u_barcodeid2:label')"/>
        </input>
        <input ref="/data/tg_sch_sth_u_2404/u_urine_conserve">
          <label ref="jr:itext('/data/tg_sch_sth_u_2404/u_urine_conserve:label')"/>
        </input>
        <input ref="/data/tg_sch_sth_u_2404/u_urine_vol">
          <label ref="jr:itext('/data/tg_sch_sth_u_2404/u_urine_vol:label')"/>
        </input>
        <input ref="/data/tg_sch_sth_u_2404/u_sh_egg">
          <label ref="jr:itext('/data/tg_sch_sth_u_2404/u_sh_egg:label')"/>
        </input>
        <input ref="/data/tg_sch_sth_u_2404/u_sh_egg_per_10ml">
          <label ref="jr:itext('/data/tg_sch_sth_u_2404/u_sh_egg_per_10ml:label')"/>
        </input>
        <input ref="/data/tg_sch_sth_u_2404/u_sch_man">
          <label ref="jr:itext('/data/tg_sch_sth_u_2404/u_sch_man:label')"/>
        </input>
        <input ref="/data/tg_sch_sth_u_2404/u_sch_man_per_10ml">
          <label ref="jr:itext('/data/tg_sch_sth_u_2404/u_sch_man_per_10ml:label')"/>
        </input>
        <select1 ref="/data/tg_sch_sth_u_2404/u_control">
          <label ref="jr:itext('/data/tg_sch_sth_u_2404/u_control:label')"/>
          <item>
            <label ref="jr:itext('/data/tg_sch_sth_u_2404/u_control/Oui:label')"/>
            <value>Oui</value>
          </item>
          <item>
            <label ref="jr:itext('/data/tg_sch_sth_u_2404/u_control/Non:label')"/>
            <value>Non</value>
          </item>
        </select1>
        <input ref="/data/tg_sch_sth_u_2404/u_barcodeid3">
          <label ref="jr:itext('/data/tg_sch_sth_u_2404/u_barcodeid3:label')"/>
        </input>
        <input ref="/data/tg_sch_sth_u_2404/u_urine_vol_control">
          <label ref="jr:itext('/data/tg_sch_sth_u_2404/u_urine_vol_control:label')"/>
        </input>
        <input ref="/data/tg_sch_sth_u_2404/u_sh_egg_control">
          <label ref="jr:itext('/data/tg_sch_sth_u_2404/u_sh_egg_control:label')"/>
        </input>
        <input ref="/data/tg_sch_sth_u_2404/u_sh_egg_per_10ml_control">
          <label ref="jr:itext('/data/tg_sch_sth_u_2404/u_sh_egg_per_10ml_control:label')"/>
        </input>
        <input ref="/data/tg_sch_sth_u_2404/u_sch_man_control">
          <label ref="jr:itext('/data/tg_sch_sth_u_2404/u_sch_man_control:label')"/>
        </input>
        <input ref="/data/tg_sch_sth_u_2404/u_sch_man_per_10ml_control">
          <label ref="jr:itext('/data/tg_sch_sth_u_2404/u_sch_man_per_10ml_control:label')"/>
        </input>
        <input ref="/data/tg_sch_sth_u_2404/u_end_note">
          <label ref="jr:itext('/data/tg_sch_sth_u_2404/u_end_note:label')"/>
        </input>
        <input ref="/data/tg_sch_sth_u_2404/u_remarks">
          <label ref="jr:itext('/data/tg_sch_sth_u_2404/u_remarks:label')"/>
        </input>
      </repeat>
    </group>
  </h:body>
</h:html>
