					<text id="/data/bj_k2_2/k_hookworm_sb:label">
						<value>Ankylostome - Lame B</value>
					</text>
					<text id="/data/bj_k2_2/k_hookworm_sup:label">
						<value>Ankylostome - Superviseur</value>
					</text>
					<text id="/data/bj_k2_2/k_trichuris_sa:label">
						<value>Trichocéphale trichura - Lame A</value>
					</text>
					<text id="/data/bj_k2_2/k_trichuris_sb:label">
						<value>Trichocéphale trichura - Lame B</value>
					</text>
					<text id="/data/bj_k2_2/k_trichuris_sup:label">
						<value>Trichocéphale trichura - Superviseur</value>
					</text>
					<text id="/data/bj_k2_2/k_remarks:label">
						<value>Notes additionnelles</value>
					</text>
				</translation>
			</itext>
			<instance>
				<data id="bj_sch_sth_impact_202401_3_kato_katz_v4">
					<k_recorder/>
					<k_district/>
					<k_subdistrict/>
					<k_site_name/>
					<k_site_code/>
					<C1/>
					<bj_k2_2 jr:template="">
						<C2/>
						<C3/>
						<k_add_record/>
						<k_espen_code_id/>
						<k_sch_man_sa/>
						<k_sch_man_sb/>
						<k_sch_man_sup/>
						<k_ascaris_lumb_sa/>
						<k_ascaris_lumb_sb/>
						<k_ascaris_lumb_sup/>
						<k_hookworm_sa/>
						<k_hookworm_sb/>
						<k_hookworm_sup/>
						<k_trichuris_sa/>
						<k_trichuris_sb/>
						<k_trichuris_sup/>
						<k_remarks/>
					</bj_k2_2>
					<bj_k2_2>
						<C2/>
						<C3/>
						<k_add_record/>
						<k_espen_code_id/>
						<k_sch_man_sa/>
						<k_sch_man_sb/>
						<k_sch_man_sup/>
						<k_ascaris_lumb_sa/>
						<k_ascaris_lumb_sb/>
						<k_ascaris_lumb_sup/>
						<k_hookworm_sa/>
						<k_hookworm_sb/>
						<k_hookworm_sup/>
						<k_trichuris_sa/>
						<k_trichuris_sb/>
						<k_trichuris_sup/>
						<k_remarks/>
					</bj_k2_2>
					<k_start/>
					<k_end/>
					<meta>
						<instanceID/>
					</meta>
				</data>
			</instance>
			<instance id="yesNo">
				<root>
					<item>
						<itextId>yesNo-0</itextId>
						<name>Oui</name>
					</item>
					<item>
						<itextId>yesNo-1</itextId>
						<name>Non</name>
					</item>
				</root>
			</instance>
			<bind nodeset="/data/k_recorder" type="string" constraint="regex(.,'^[0-9]{2}$')" jr:constraintMsg="jr:itext('/data/k_recorder:jr:constraintMsg')" required="true()"/>
			<bind nodeset="/data/k_district" type="string" required="true()" db_get="col_1"/>
			<bind nodeset="/data/k_subdistrict" type="string" required="true()" db_get="col_2" db_filter_by_col_1="/data/k_district"/>
			<bind nodeset="/data/k_site_name" type="string" required="true()" db_get="col_3" db_filter_by_col_2="/data/k_subdistrict"/>
			<bind nodeset="/data/k_site_code" type="string" required="true()" db_get="col_4" db_filter_by_col_3="/data/k_site_name"/>
			<bind nodeset="/data/C1" type="string" calculate="join(' ',  /data/bj_k2_2/k_espen_code_id )"/>
			<bind nodeset="/data/bj_k2_2/C2" type="string" calculate="position(..)"/>
			<bind nodeset="/data/bj_k2_2/C3" type="string" calculate="if ( ../C2  = 1,'',substring-after( /data/C1 , ../k_espen_code_id ))"/>
			<bind nodeset="/data/bj_k2_2/k_add_record" type="string"/>
			<bind nodeset="/data/bj_k2_2/k_espen_code_id" type="string" constraint="not(selected( ../C3 ,  ../k_espen_code_id ))" jr:constraintMsg="jr:itext('/data/bj_k2_2/k_espen_code_id:jr:constraintMsg')" relevant=" ../k_add_record  = 'Oui'" required="true()" db_get="col_5" 
	  db_filter_by_col_4="/data/k_site_code"/>
			<bind nodeset="/data/bj_k2_2/k_sch_man_sa" type="int" relevant=" ../k_add_record  = 'Oui'" required="true()"/>
			<bind nodeset="/data/bj_k2_2/k_sch_man_sb" type="int" relevant=" ../k_add_record  = 'Oui'" required="true()"/>
			<bind nodeset="/data/bj_k2_2/k_sch_man_sup" type="int" relevant=" ../k_add_record  = 'Oui'"/>
			<bind nodeset="/data/bj_k2_2/k_ascaris_lumb_sa" type="int" relevant=" ../k_add_record  = 'Oui'" required="true()"/>
			<bind nodeset="/data/bj_k2_2/k_ascaris_lumb_sb" type="int" relevant=" ../k_add_record  = 'Oui'" required="true()"/>
			<bind nodeset="/data/bj_k2_2/k_ascaris_lumb_sup" type="int" relevant=" ../k_add_record  = 'Oui'"/>
			<bind nodeset="/data/bj_k2_2/k_hookworm_sa" type="int" relevant=" ../k_add_record  = 'Oui'" required="true()"/>
			<bind nodeset="/data/bj_k2_2/k_hookworm_sb" type="int" relevant=" ../k_add_record  = 'Oui'" required="true()"/>
			<bind nodeset="/data/bj_k2_2/k_hookworm_sup" type="int" relevant=" ../k_add_record  = 'Oui'"/>
			<bind nodeset="/data/bj_k2_2/k_trichuris_sa" type="int" relevant=" ../k_add_record  = 'Oui'" required="true()"/>
			<bind nodeset="/data/bj_k2_2/k_trichuris_sb" type="int" relevant=" ../k_add_record  = 'Oui'" required="true()"/>
			<bind nodeset="/data/bj_k2_2/k_trichuris_sup" type="int" relevant=" ../k_add_record  = 'Oui'"/>
			<bind nodeset="/data/bj_k2_2/k_remarks" type="string" relevant=" ../k_add_record  = 'Oui'"/>
			<bind nodeset="/data/k_start" jr:preload="timestamp" type="dateTime" jr:preloadParams="start"/>
			<bind nodeset="/data/k_end" jr:preload="timestamp" type="dateTime" jr:preloadParams="end"/>
			<bind nodeset="/data/meta/instanceID" type="string" readonly="true()" jr:preload="uid"/>
		</model>
	</h:head>
	<h:body>
		<input ref="/data/k_recorder">
			<label ref="jr:itext('/data/k_recorder:label')"/>
			<hint ref="jr:itext('/data/k_recorder:hint')"/>
		</input>
		<input ref="/data/k_district">
			<label ref="jr:itext('/data/k_district:label')"/>
		</input>
		<input ref="/data/k_subdistrict">
			<label ref="jr:itext('/data/k_subdistrict:label')"/>
		</input>
		<input ref="/data/k_site_name">
			<label ref="jr:itext('/data/k_site_name:label')"/>
		</input>
		<input ref="/data/k_site_code">
			<label ref="jr:itext('/data/k_site_code:label')"/>
		</input>
		<group ref="/data/bj_k2_2">
			<label ref="jr:itext('/data/bj_k2_2:label')"/>
			<repeat nodeset="/data/bj_k2_2">
				<select1 ref="/data/bj_k2_2/k_add_record">
					<label ref="jr:itext('/data/bj_k2_2/k_add_record:label')"/>
					<itemset nodeset="instance('yesNo')/root/item">
						<value ref="name"/>
						<label ref="jr:itext(itextId)"/>
					</itemset>
				</select1>
				<input ref="/data/bj_k2_2/k_espen_code_id">
					<label ref="jr:itext('/data/bj_k2_2/k_espen_code_id:label')"/>
					<hint ref="jr:itext('/data/bj_k2_2/k_espen_code_id:hint')"/>
				</input>
				<input ref="/data/bj_k2_2/k_sch_man_sa">
					<label ref="jr:itext('/data/bj_k2_2/k_sch_man_sa:label')"/>
				</input>
				<input ref="/data/bj_k2_2/k_sch_man_sb">
					<label ref="jr:itext('/data/bj_k2_2/k_sch_man_sb:label')"/>
				</input>
				<input ref="/data/bj_k2_2/k_sch_man_sup">
					<label ref="jr:itext('/data/bj_k2_2/k_sch_man_sup:label')"/>
				</input>
				<input ref="/data/bj_k2_2/k_ascaris_lumb_sa">
					<label ref="jr:itext('/data/bj_k2_2/k_ascaris_lumb_sa:label')"/>
				</input>
				<input ref="/data/bj_k2_2/k_ascaris_lumb_sb">
					<label ref="jr:itext('/data/bj_k2_2/k_ascaris_lumb_sb:label')"/>
				</input>
				<input ref="/data/bj_k2_2/k_ascaris_lumb_sup">
					<label ref="jr:itext('/data/bj_k2_2/k_ascaris_lumb_sup:label')"/>
				</input>
				<input ref="/data/bj_k2_2/k_hookworm_sa">
					<label ref="jr:itext('/data/bj_k2_2/k_hookworm_sa:label')"/>
				</input>
				<input ref="/data/bj_k2_2/k_hookworm_sb">
					<label ref="jr:itext('/data/bj_k2_2/k_hookworm_sb:label')"/>
				</input>
				<input ref="/data/bj_k2_2/k_hookworm_sup">
					<label ref="jr:itext('/data/bj_k2_2/k_hookworm_sup:label')"/>
				</input>
				<input ref="/data/bj_k2_2/k_trichuris_sa">
					<label ref="jr:itext('/data/bj_k2_2/k_trichuris_sa:label')"/>
				</input>
				<input ref="/data/bj_k2_2/k_trichuris_sb">
					<label ref="jr:itext('/data/bj_k2_2/k_trichuris_sb:label')"/>
				</input>
				<input ref="/data/bj_k2_2/k_trichuris_sup">
					<label ref="jr:itext('/data/bj_k2_2/k_trichuris_sup:label')"/>
				</input>
				<input ref="/data/bj_k2_2/k_remarks">
					<label ref="jr:itext('/data/bj_k2_2/k_remarks:label')"/>
				</input>
			</repeat>
		</group>
	</h:body>
</h:html>