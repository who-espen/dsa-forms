			<bind nodeset="/data/p_recorder" type="string" constraint="regex(.,'^[0-9]{2}$')" jr:constraintMsg="jr:itext('/data/p_recorder:jr:constraintMsg')" required="true()"/>
			<bind nodeset="/data/p_district" type="string" required="true()" db_get="col_1"/>
			<bind nodeset="/data/p_subdistrict" type="string" required="true()" db_get="col_2" db_filter_by_col_1="/data/p_district"/>
			<bind nodeset="/data/p_site_name" type="string" required="true()" db_get="col_3" db_filter_by_col_2="/data/p_subdistrict"/>
			<bind nodeset="/data/p_site_code" type="string" constraint="regex(.,'^[0-9]{3}$')" jr:constraintMsg="jr:itext('/data/p_site_code:jr:constraintMsg')" required="true()" db_get="col_4" db_filter_by_col_3="/data/p_site_name"/>
			<bind nodeset="/data/p_num" type="int" constraint=".&gt;=1 and .&lt;=99" jr:constraintMsg="jr:itext('/data/p_num:jr:constraintMsg')" required="true()"/>
			<bind nodeset="/data/p_classe" type="string" required="true()"/>
			<bind nodeset="/data/p_sex" type="string" required="true()"/>
			<bind nodeset="/data/p_consent" type="string" required="true()"/>
			<bind nodeset="/data/p_age_yrs" type="int" constraint=". &gt;= 5 and . &lt;= 14" jr:constraintMsg="jr:itext('/data/p_age_yrs:jr:constraintMsg')" relevant=" /data/p_consent  = 'A.donne'" required="true()"/>
			<bind nodeset="/data/p_chaussures" type="string" relevant=" /data/p_consent  = 'A.donne'" required="true()"/>
			<bind nodeset="/data/p_latrines" type="string" relevant=" /data/p_consent  = 'A.donne'"/>
			<bind nodeset="/data/p_espen_code_id" type="string" relevant=" /data/p_consent  = 'A.donne'" calculate="concat( /data/p_site_code ,'-',if( /data/p_num &lt;10, concat('0',  /data/p_num ),  /data/p_num ))" required="true()" readonly="true()"/>
			<bind nodeset="/data/p_collected_stool" type="string" relevant=" /data/p_consent  = 'A.donne'" required="true()"/>
			<bind nodeset="/data/p_collected_urine" type="string" relevant=" /data/p_consent  = 'A.donne'" required="true()"/>
			<bind nodeset="/data/p_grading" type="string" relevant=" /data/p_consent  = 'A.donne' and  /data/p_collected_urine  = 'Oui'"/>
			<bind nodeset="/data/p_urine_conserve" type="string" constraint="if(( /data/p_grading ='Trace.hemolysee' or  /data/p_grading ='+' or  /data/p_grading ='++' or  /data/p_grading ='+++' ) and . = 'Non', false(), true()) and if( /data/p_grading ='Trace.non.hemolysee' and . = 'Oui', false(), true())" jr:constraintMsg="jr:itext('/data/p_urine_conserve:jr:constraintMsg')" relevant=" /data/p_consent  = 'A.donne' and  /data/p_collected_urine  = 'Oui' and  /data/p_grading  != 'Negatif'"/>
			<bind nodeset="/data/p_ending_survey_note" readonly="true()" type="string" relevant=" /data/p_consent  = 'A.refuse'"/>
			<bind nodeset="/data/p_sample_note" readonly="true()" type="string" relevant=" /data/p_collected_stool  = 'Non' and  /data/p_collected_urine  = 'Non'"/>
			<bind nodeset="/data/p_remarks" type="string"/>
			<bind nodeset="/data/p_start" jr:preload="timestamp" type="dateTime" jr:preloadParams="start"/>
			<bind nodeset="/data/p_end" jr:preload="timestamp" type="dateTime" jr:preloadParams="end"/>
			<bind nodeset="/data/meta/instanceID" type="string" readonly="true()" jr:preload="uid"/>
		</model>
