			<bind nodeset="/data/p_boucle/p_/grading" relevant=" ../consent  = 'A.donne' and  ../collected_urine  = 'Oui'" type="string"/>
			<bind constraint="if(( ../grading ='Trace.hemolysee' or  ../grading ='+' or  ../grading ='++' or  ../grading ='+++' ) and . = 'Non', false(), true()) and if( ../grading ='Trace.non.hemolysee' and . = 'Oui', false(), true())" jr:constraintMsg="jr:itext('/data/p_boucle/p_/urine_conserve:jr:constraintMsg')" nodeset="/data/p_boucle/p_/urine_conserve" relevant=" ../consent  = 'A.donne' and  ../collected_urine  = 'Oui' and  ../grading  != 'Negatif'" type="string"/>
			<bind nodeset="/data/p_boucle/p_/ending_survey_note" readonly="true()" relevant=" ../consent  = 'A.refuse'" type="string"/>
			<bind nodeset="/data/p_boucle/p_/sample_note" readonly="true()" relevant=" ../collected_stool  = 'Non' and  ../collected_urine  = 'Non'" type="string"/>
			<bind nodeset="/data/p_boucle/p_/remarks" type="string"/>
			<bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/p_start" type="dateTime"/>
			<bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/p_end" type="dateTime"/>
			<bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
		</model>
