<?xml version="1.0" encoding="UTF-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms"
  xmlns:ev="http://www.w3.org/2001/xml-events"
  xmlns:h="http://www.w3.org/1999/xhtml"
  xmlns:jr="http://openrosa.org/javarosa"
  xmlns:odk="http://www.opendatakit.org/xforms"
  xmlns:orx="http://openrosa.org/xforms"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>(2024 Jan) - 4. SCH/STH – Urine Form V1.2</h:title>
    <model>

      <instance>
        <data id="lr_sch_sth_impact_202401_4_urine_v1_2">
          <u_recorder/>
          <u_district/>
          <u_subdistrict/>
          <u_school_name/>
          <u_school_id/>
          <u_samples_batch/>
          <C1/>
          <lr_u_202401_v12 jr:template="">
            <C2/>
            <C3/>
            <u_add_result/>
            <u_code_id/>
            <u_dipstick/>
            <u_urine_vol/>
            <u_sh_egg/>
            <u_sh_egg_per_10ml/>
            <u_sh_egg_per_10ml_heavy_intensity/>
            <u_sh_egg_per_10ml_low_intensity/>
            <u_remarks/>
          </lr_u_202401_v12>
          <u_start/>
          <u_end/>
          <meta>
            <instanceID/>
          </meta>
        </data>
      </instance>

      <itext>
        <translation default="true()" lang="English">
          <text id="/data/lr_u_202401_v12/u_dipstick/Trace haemolysed:label">
            <value>2 = Trace haemolysed</value>
          </text>
          <text id="/data/lr_u_202401_v12/C2:label">
            <value>Please inter the child sequence number</value>
          </text>
          <text id="/data/u_samples_batch:label">
            <value>How many samples were collected in this school?</value>
          </text>
          <text id="/data/u_recorder:hint">
            <value>The two digit code assigned to you</value>
          </text>
          <text id="/data/lr_u_202401_v12/C3:label">
            <value>Child code</value>
          </text>
          <text id="/data/lr_u_202401_v12/u_add_result:label">
            <value>Do you want to add an urine result</value>
          </text>
          <text id="/data/u_recorder:label">
            <value>Record code</value>
          </text>
          <text id="/data/lr_u_202401_v12/u_add_result/No:label">
            <value>No</value>
          </text>
          <text id="/data/lr_u_202401_v12/u_dipstick/++:label">
            <value>4 = ++</value>
          </text>
          <text id="/data/lr_u_202401_v12/u_code_id:label">
            <value>Please select the code of the registered child</value>
          </text>
          <text id="/data/lr_u_202401_v12/u_dipstick/Trace non-haemolysed:label">
            <value>1 = Trace non-haemolysed</value>
          </text>
          <text id="/data/lr_u_202401_v12/u_sh_egg:label">
            <value>Number of eggs (S. Haematobium)</value>
          </text>
          <text id="/data/lr_u_202401_v12/u_sh_egg_per_10ml:label">
            <value>Number of eggs (S. Haematobium) per 10 ml</value>
          </text>
          <text id="/data/lr_u_202401_v12/u_urine_vol:label">
            <value>Urine volume: Record the milliliters (ml) of urine collected from the person</value>
          </text>
          <text id="/data/lr_u_202401_v12:label">
            <value>Urine results</value>
          </text>
          <text id="/data/lr_u_202401_v12/u_dipstick:label">
            <value>Dipstick result (micro-haematuria)</value>
          </text>
          <text id="/data/lr_u_202401_v12/u_code_id:jr:constraintMsg">
            <value>This child code already exists. Please edit the sequence number from the previous question</value>
          </text>
          <text id="/data/lr_u_202401_v12/u_remarks:label">
            <value>Additional Notes</value>
          </text>
          <text id="/data/u_subdistrict:label">
            <value>Select ward</value>
          </text>
          <text id="/data/u_district:label">
            <value>Select the council</value>
          </text>
          <text id="/data/C1:label">
            <value>How many samples were collected in this school?</value>
          </text>
          <text id="/data/lr_u_202401_v12/u_urine_vol:jr:constraintMsg">
            <value>Le volume doit être limité à 10ml</value>
          </text>
          <text id="/data/u_recorder:jr:constraintMsg">
            <value>Must be two digit between 9 and 100</value>
          </text>
          <text id="/data/lr_u_202401_v12/u_dipstick/+:label">
            <value>3 = +</value>
          </text>
          <text id="/data/u_school_name:label">
            <value>Select school name</value>
          </text>
          <text id="/data/lr_u_202401_v12/u_dipstick/None:label">
            <value>0 = None</value>
          </text>
          <text id="/data/lr_u_202401_v12/u_add_result/Yes:label">
            <value>Yes</value>
          </text>
          <text id="/data/u_school_id:label">
            <value>Select school ID</value>
          </text>
          <text id="/data/lr_u_202401_v12/u_dipstick/+++:label">
            <value>5 = +++</value>
          </text>
        </translation>
      </itext>

      <bind constraint=". &gt; 9 and . &lt; 100" jr:constraintMsg="jr:itext('/data/u_recorder:jr:constraintMsg')" nodeset="/data/u_recorder" required="true()" type="int"/>
      <bind nodeset="/data/u_district" required="true()" type="string" db_get="col_1"/>
      <bind nodeset="/data/u_subdistrict" required="true()" type="string" db_get="col_2" db_filter_by_col_1="/data/u_district"/>
      <bind nodeset="/data/u_school_name" required="true()" type="string" db_get="col_3" db_filter_by_col_2="/data/u_subdistrict"/>
      <bind nodeset="/data/u_school_id" required="true()" type="string" db_get="col_4" db_filter_by_col_3="/data/u_school_name"/>
      <bind nodeset="/data/u_samples_batch" required="true()" type="string"/>
      <bind calculate="join(' ',  /data/lr_u_202401_v12/u_code_id )" nodeset="/data/C1" type="string"/>
      <bind calculate="position(..)" nodeset="/data/lr_u_202401_v12/C2" type="string"/>
      <bind calculate="if ( ../C2  = 1,'',substring-after( /data/C1 , ../u_code_id ))" nodeset="/data/lr_u_202401_v12/C3" type="string"/>
      <bind nodeset="/data/lr_u_202401_v12/u_add_result" required="true()" type="select1"/>
      <bind constraint="not(selected( ../C3 ,  ../u_code_id ))" jr:constraintMsg="jr:itext('/data/lr_u_202401_v12/u_code_id:jr:constraintMsg')" nodeset="/data/lr_u_202401_v12/u_code_id" relevant=" ../u_add_result  = 'Yes'" required="true()" type="string" db_get="col_5" db_filter_by_col_4="/data/u_school_id"/>
      <bind nodeset="/data/lr_u_202401_v12/u_dipstick" relevant=" ../u_add_result  = 'Yes'" required="true()" type="select1"/>
      <bind constraint=". &gt;=0 and .&lt; 11" jr:constraintMsg="jr:itext('/data/lr_u_202401_v12/u_urine_vol:jr:constraintMsg')" nodeset="/data/lr_u_202401_v12/u_urine_vol" relevant=" ../u_add_result  = 'Yes'" required="true()" type="int"/>
      <bind nodeset="/data/lr_u_202401_v12/u_sh_egg" relevant=" ../u_add_result  = 'Yes'" required="true()" type="int"/>
      <bind calculate="round( 10 *  ../u_sh_egg  div  ../u_urine_vol  )" nodeset="/data/lr_u_202401_v12/u_sh_egg_per_10ml" readonly="true()" relevant=" ../u_add_result  = 'Yes'" type="string"/>
      <bind calculate="if( ../u_sh_egg_per_10ml &gt;= 50, 1, 0)" nodeset="/data/lr_u_202401_v12/u_sh_egg_per_10ml_heavy_intensity" relevant=" ../u_add_result  = 'Yes'" type="string"/>
      <bind calculate="if( ../u_sh_egg_per_10ml  &gt; 0 and  ../u_sh_egg_per_10ml  &lt; 50, 1, 0)" nodeset="/data/lr_u_202401_v12/u_sh_egg_per_10ml_low_intensity" relevant=" ../u_add_result  = 'Yes'" type="string"/>
      <bind nodeset="/data/lr_u_202401_v12/u_remarks" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/u_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/u_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/data/u_recorder">
      <label ref="jr:itext('/data/u_recorder:label')"/>
      <hint ref="jr:itext('/data/u_recorder:hint')"/>
    </input>
    <input ref="/data/u_district">
      <label ref="jr:itext('/data/u_district:label')"/>
    </input>
    <input ref="/data/u_subdistrict">
      <label ref="jr:itext('/data/u_subdistrict:label')"/>
    </input>
    <input ref="/data/u_school_name">
      <label ref="jr:itext('/data/u_school_name:label')"/>
    </input>
    <input ref="/data/u_school_id">
      <label ref="jr:itext('/data/u_school_id:label')"/>
    </input>
    <input ref="/data/u_samples_batch">
      <label ref="jr:itext('/data/u_samples_batch:label')"/>
    </input>
    <group ref="/data/lr_u_202401_v12">
      <label ref="jr:itext('/data/lr_u_202401_v12:label')"/>
      <repeat nodeset="/data/lr_u_202401_v12">
        <select1 ref="/data/lr_u_202401_v12/u_add_result">
          <label ref="jr:itext('/data/lr_u_202401_v12/u_add_result:label')"/>
          <item>
            <label ref="jr:itext('/data/lr_u_202401_v12/u_add_result/Yes:label')"/>
            <value>Yes</value>
          </item>
          <item>
            <label ref="jr:itext('/data/lr_u_202401_v12/u_add_result/No:label')"/>
            <value>No</value>
          </item>
        </select1>
        <input ref="/data/lr_u_202401_v12/u_code_id">
          <label ref="jr:itext('/data/lr_u_202401_v12/u_code_id:label')"/>
        </input>
        <select1 ref="/data/lr_u_202401_v12/u_dipstick">
          <label ref="jr:itext('/data/lr_u_202401_v12/u_dipstick:label')"/>
          <item>
            <label ref="jr:itext('/data/lr_u_202401_v12/u_dipstick/None:label')"/>
            <value>None</value>
          </item>
          <item>
            <label ref="jr:itext('/data/lr_u_202401_v12/u_dipstick/Trace non-haemolysed:label')"/>
            <value>Trace non-haemolysed</value>
          </item>
          <item>
            <label ref="jr:itext('/data/lr_u_202401_v12/u_dipstick/Trace haemolysed:label')"/>
            <value>Trace haemolysed</value>
          </item>
          <item>
            <label ref="jr:itext('/data/lr_u_202401_v12/u_dipstick/+:label')"/>
            <value>+</value>
          </item>
          <item>
            <label ref="jr:itext('/data/lr_u_202401_v12/u_dipstick/++:label')"/>
            <value>++</value>
          </item>
          <item>
            <label ref="jr:itext('/data/lr_u_202401_v12/u_dipstick/+++:label')"/>
            <value>+++</value>
          </item>
        </select1>
        <input ref="/data/lr_u_202401_v12/u_urine_vol">
          <label ref="jr:itext('/data/lr_u_202401_v12/u_urine_vol:label')"/>
        </input>
        <input ref="/data/lr_u_202401_v12/u_sh_egg">
          <label ref="jr:itext('/data/lr_u_202401_v12/u_sh_egg:label')"/>
        </input>
        <input ref="/data/lr_u_202401_v12/u_sh_egg_per_10ml">
          <label ref="jr:itext('/data/lr_u_202401_v12/u_sh_egg_per_10ml:label')"/>
        </input>
        <input ref="/data/lr_u_202401_v12/u_remarks">
          <label ref="jr:itext('/data/lr_u_202401_v12/u_remarks:label')"/>
        </input>
      </repeat>
    </group>
  </h:body>
</h:html>
