<?xml version="1.0" encoding="UTF-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms"
  xmlns:ev="http://www.w3.org/2001/xml-events"
  xmlns:h="http://www.w3.org/1999/xhtml"
  xmlns:jr="http://openrosa.org/javarosa"
  xmlns:odk="http://www.opendatakit.org/xforms"
  xmlns:orx="http://openrosa.org/xforms"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>(2024 Jan) - 1. SCH/STH â€“ Site Level (School or Community) Form</h:title>
    <model>

      <instance>
        <data id="lr_sch_sth_impact_202401_1_school">
          <w_recorder/>
          <w_region/>
          <w_district/>
          <w_sub_district/>
          <w_school_name/>
          <w_school_id/>
          <w_school_community/>
          <w_gps/>
          <w_headteacher/>
          <w_headteacher_phone/>
          <w_treatment/>
          <w_enrolment/>
          <w_treated/>
          <w_treated_logiccheck/>
          <w_remarks/>
          <w_Start/>
          <w_End/>
          <meta>
            <instanceID/>
          </meta>
        </data>
      </instance>

      <itext>
        <translation default="true()" lang="English">
          <text id="/data/w_headteacher_phone:label">
            <value>Contact number of head teacher</value>
          </text>
          <text id="/data/w_district:label">
            <value>Select the county</value>
          </text>
          <text id="/data/w_treatment/Never:label">
            <value>Never</value>
          </text>
          <text id="/data/w_gps:hint">
            <value>Works best outside buildings</value>
          </text>
          <text id="/data/w_region:label">
            <value>Select region</value>
          </text>
          <text id="/data/w_treatment/12 - 24 moths:label">
            <value>12 - 24 moths</value>
          </text>
          <text id="/data/w_sub_district:label">
            <value>Select the sub-district</value>
          </text>
          <text id="/data/w_school_id:label">
            <value>Select the school ID</value>
          </text>
          <text id="/data/w_school_community:label">
            <value>Community school is in</value>
          </text>
          <text id="/data/w_headteacher:label">
            <value>Name of head teacher</value>
          </text>
          <text id="/data/w_treated:label">
            <value>How many children were treated?</value>
          </text>
          <text id="/data/w_remarks:label">
            <value>Any additional notes:</value>
          </text>
          <text id="/data/w_enrolment:label">
            <value>How many children were enrolled at the time of deworming</value>
          </text>
          <text id="/data/w_treatment/1 month:label">
            <value>1 month</value>
          </text>
          <text id="/data/w_treatment/2 - 6 months:label">
            <value>2 - 6 months</value>
          </text>
          <text id="/data/w_recorder:jr:constraintMsg">
            <value>Must be two digit between 9 and 100</value>
          </text>
          <text id="/data/w_treatment/6 - 12 months:label">
            <value>6 - 12 months</value>
          </text>
          <text id="/data/w_treated_logiccheck:label">
            <value>IF the number (treated/enrolment) is &lt;20% or &gt;100%, check both 10 &amp; 11.</value>
          </text>
          <text id="/data/w_recorder:hint">
            <value>The two digit code assigned to you</value>
          </text>
          <text id="/data/w_treatment/Over two years ago:label">
            <value>Over two years ago</value>
          </text>
          <text id="/data/w_treatment:label">
            <value>How long ago did pupils in your school last receive deworming treatment (PZQ or ALB/MBD or both)?</value>
          </text>
          <text id="/data/w_recorder:label">
            <value>Recorder code</value>
          </text>
          <text id="/data/w_school_name:label">
            <value>Select the school name</value>
          </text>
          <text id="/data/w_treatment/Don not know:label">
            <value>Don't know</value>
          </text>
          <text id="/data/w_gps:label">
            <value>Collect GPS Coordinates</value>
          </text>
        </translation>
      </itext>

      <bind constraint="regex(., '^[0-9]{2}$')" jr:constraintMsg="jr:itext('/data/w_recorder:jr:constraintMsg')" nodeset="/data/w_recorder" required="true()" type="int"/>
      <bind nodeset="/data/w_region" required="true()" type="string"/>
      <bind nodeset="/data/w_district" required="true()" type="string" db_add_col_1="."/>
      <bind nodeset="/data/w_sub_district" required="true()" type="string" db_add_col_1="/data/w_district" db_add_col_2="."/>
      <bind nodeset="/data/w_school_name" required="true()" type="string" db_add_col_2="/data/w_sub_district" db_add_col_3="."/>
      <bind nodeset="/data/w_school_id" required="true()" type="string" db_add_col_3="/data/w_school_name" db_add_col_4="."/>
      <bind nodeset="/data/w_school_community" required="true()" type="string"/>
      <bind nodeset="/data/w_gps" required="true()" type="geopoint"/>
      <bind nodeset="/data/w_headteacher" required="true()" type="string"/>
      <bind nodeset="/data/w_headteacher_phone" required="true()" type="int"/>
      <bind nodeset="/data/w_treatment" required="true()" type="select1"/>
      <bind nodeset="/data/w_enrolment" required="true()" type="int"/>
      <bind nodeset="/data/w_treated" required="true()" type="int"/>
      <bind nodeset="/data/w_treated_logiccheck" required="true()" type="int"/>
      <bind nodeset="/data/w_remarks" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/w_Start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/w_End" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/data/w_recorder">
      <label ref="jr:itext('/data/w_recorder:label')"/>
      <hint ref="jr:itext('/data/w_recorder:hint')"/>
    </input>
    <input ref="/data/w_region">
      <label ref="jr:itext('/data/w_region:label')"/>
    </input>
    <input ref="/data/w_district">
      <label ref="jr:itext('/data/w_district:label')"/>
    </input>
    <input ref="/data/w_sub_district">
      <label ref="jr:itext('/data/w_sub_district:label')"/>
    </input>
    <input ref="/data/w_school_name">
      <label ref="jr:itext('/data/w_school_name:label')"/>
    </input>
    <input ref="/data/w_school_id">
      <label ref="jr:itext('/data/w_school_id:label')"/>
    </input>
    <input ref="/data/w_school_community">
      <label ref="jr:itext('/data/w_school_community:label')"/>
    </input>
    <input ref="/data/w_gps">
      <label ref="jr:itext('/data/w_gps:label')"/>
      <hint ref="jr:itext('/data/w_gps:hint')"/>
    </input>
    <input ref="/data/w_headteacher">
      <label ref="jr:itext('/data/w_headteacher:label')"/>
    </input>
    <input ref="/data/w_headteacher_phone">
      <label ref="jr:itext('/data/w_headteacher_phone:label')"/>
    </input>
    <select1 ref="/data/w_treatment">
      <label ref="jr:itext('/data/w_treatment:label')"/>
      <item>
        <label ref="jr:itext('/data/w_treatment/Never:label')"/>
        <value>Never</value>
      </item>
      <item>
        <label ref="jr:itext('/data/w_treatment/Don not know:label')"/>
        <value>Don not know</value>
      </item>
      <item>
        <label ref="jr:itext('/data/w_treatment/1 month:label')"/>
        <value>1 month</value>
      </item>
      <item>
        <label ref="jr:itext('/data/w_treatment/2 - 6 months:label')"/>
        <value>2 - 6 months</value>
      </item>
      <item>
        <label ref="jr:itext('/data/w_treatment/6 - 12 months:label')"/>
        <value>6 - 12 months</value>
      </item>
      <item>
        <label ref="jr:itext('/data/w_treatment/12 - 24 moths:label')"/>
        <value>12 - 24 moths</value>
      </item>
      <item>
        <label ref="jr:itext('/data/w_treatment/Over two years ago:label')"/>
        <value>Over two years ago</value>
      </item>
    </select1>
    <input ref="/data/w_enrolment">
      <label ref="jr:itext('/data/w_enrolment:label')"/>
    </input>
    <input ref="/data/w_treated">
      <label ref="jr:itext('/data/w_treated:label')"/>
    </input>
    <input ref="/data/w_treated_logiccheck">
      <label ref="jr:itext('/data/w_treated_logiccheck:label')"/>
    </input>
    <input ref="/data/w_remarks">
      <label ref="jr:itext('/data/w_remarks:label')"/>
    </input>
  </h:body>
</h:html>
