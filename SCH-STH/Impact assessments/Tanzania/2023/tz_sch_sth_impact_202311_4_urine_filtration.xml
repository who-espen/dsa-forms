      <bind constraint="regex(., '^[0-9]{2}$')" jr:constraintMsg="jr:itext('/data/u_recorder:jr:constraintMsg')" nodeset="/data/u_recorder" required="true()" type="int"/>
      <bind nodeset="/data/u_district" required="true()" type="string" db_get="col_1"/>
      <bind nodeset="/data/u_ward" required="true()" type="string" db_get="col_2" db_filter_by_col_1="/data/u_district"/>
      <bind nodeset="/data/u_site_name" required="true()" type="string" db_get="col_3" db_filter_by_col_2="/data/u_ward"/>
      <bind nodeset="/data/u_site_id" required="true()" type="string" db_get="col_4" db_filter_by_col_3="/data/u_site_name"/>
      <bind calculate="join(' ',  /data/tz_u_202401_v4_1/u_code_id )" nodeset="/data/C1" type="string"/>
      <bind calculate="position(..)" nodeset="/data/tz_u_202401_v4_1/C2" type="string"/>
      <bind calculate="if ( ../C2  = 1,'',substring-after( /data/C1 , ../u_code_id ))" nodeset="/data/tz_u_202401_v4_1/C3" type="string"/>
      <bind nodeset="/data/tz_u_202401_v4_1/u_add_result" required="true()" type="select1"/>
      <bind constraint="not(selected( ../C3 ,  ../u_code_id ))" jr:constraintMsg="jr:itext('/data/tz_u_202401_v4_1/u_code_id:jr:constraintMsg')" nodeset="/data/tz_u_202401_v4_1/u_code_id" relevant=" ../u_add_result  = 'Yes'" required="true()" type="string" db_get="col_5" db_filter_by_col_4="/data/u_site_id"/>
      <bind constraint=". = 10" jr:constraintMsg="jr:itext('/data/tz_u_202401_v4_1/u_urine_vol:jr:constraintMsg')" nodeset="/data/tz_u_202401_v4_1/u_urine_vol" relevant=" ../u_add_result  = 'Yes'" required="true()" type="int"/>
      <bind nodeset="/data/tz_u_202401_v4_1/u_macroscopique_hemat" relevant=" ../u_add_result  = 'Yes'" required="true()" type="select1"/>
      <bind nodeset="/data/tz_u_202401_v4_1/u_sh_egg" relevant=" ../u_add_result  = 'Yes'" required="true()" type="int"/>
      <bind calculate="round( 10 *  ../u_sh_egg  div  ../u_urine_vol  )" nodeset="/data/tz_u_202401_v4_1/u_sh_egg_per_10ml" readonly="true()" relevant=" ../u_add_result  = 'Yes'" type="string"/>
      <bind nodeset="/data/tz_u_202401_v4_1/u_sch_man" relevant=" ../u_add_result  = 'Yes'" type="int"/>
      <bind calculate="round( 10 *  ../u_sch_man  div  ../u_urine_vol  )" nodeset="/data/tz_u_202401_v4_1/u_sch_man_per_10ml" readonly="true()" relevant=" ../u_add_result  = 'Yes' and  ../u_sch_man &gt;0" type="string"/>
      <bind nodeset="/data/tz_u_202401_v4_1/note_mansoni" readonly="true()" relevant=" ../u_sch_man &gt;0" type="string"/>
      <bind nodeset="/data/tz_u_202401_v4_1/u_dipstick" relevant=" ../u_add_result  = 'Yes'" required="true()" type="select1"/>
      <bind nodeset="/data/tz_u_202401_v4_1/u_remarks" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/u_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/u_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
