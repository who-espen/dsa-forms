      <bind nodeset="/data/tz_st_p_2405/p_ending_survey_note" readonly="true()" relevant=" ../p_consent  = 'Refused' or  ../p_assent  = 'Refused'" type="string"/>
      <bind nodeset="/data/tz_st_p_2405/p_sample_note" readonly="true()" relevant=" ../p_collected_stool  = 'No' or  ../p_collected_urine  = 'No'" type="string"/>
      <bind nodeset="/data/tz_st_p_2405/p_remarks" type="text"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/p_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/p_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
