      <bind calculate="position(..)" nodeset="/data/tz_k_202401_v3_1/C2" type="string"/>
      <bind calculate="if ( ../C2  = 1,'',substring-after( /data/C1 , ../k_barcode_id ))" nodeset="/data/tz_k_202401_v3_1/C3" type="string"/>
      <bind nodeset="/data/tz_k_202401_v3_1/k_add_result" required="true()" type="select1"/>
      <bind constraint="not(selected( ../C3 ,  ../k_barcode_id ))" jr:constraintMsg="jr:itext('/data/tz_k_202401_v3_1/k_barcode_id:jr:constraintMsg')" nodeset="/data/tz_k_202401_v3_1/k_barcode_id" relevant=" ../k_add_result  = 'Yes'" required="true()" type="string"  db_get="col_5" db_filter_by_col_4="/data/k_site_id"/>
      <bind nodeset="/data/tz_k_202401_v3_1/k_hookworm_sa" relevant=" ../k_add_result  = 'Yes'" required="true()" type="int"/>
      <bind nodeset="/data/tz_k_202401_v3_1/k_hookworm_sb" relevant=" ../k_add_result  = 'Yes'" required="true()" type="int"/>
      <bind calculate="( ../k_hookworm_sa  +  ../k_hookworm_sb ) *12" nodeset="/data/tz_k_202401_v3_1/k_hookworm_intensity" relevant=" ../k_add_result  = 'Yes'" type="string"/>
      <bind calculate="if( ../k_hookworm_intensity &gt;= 400, 1, 0)" nodeset="/data/tz_k_202401_v3_1/k_hookworm_heavy_intensity" relevant=" ../k_add_result  = 'Yes'" type="string"/>
      <bind calculate="if( ../k_hookworm_intensity &gt;= 100 and  ../k_hookworm_intensity  &lt; 400, 1, 0)" nodeset="/data/tz_k_202401_v3_1/k_hookworm_moderate_intensity" relevant=" ../k_add_result  = 'Yes'" type="string"/>
      <bind nodeset="/data/tz_k_202401_v3_1/k_ascaris_lumb_sa" relevant=" ../k_add_result  = 'Yes'" required="true()" type="int"/>
      <bind nodeset="/data/tz_k_202401_v3_1/k_ascaris_lumb_sb" relevant=" ../k_add_result  = 'Yes'" required="true()" type="int"/>
      <bind calculate="( ../k_ascaris_lumb_sa  +  ../k_ascaris_lumb_sb ) *12" nodeset="/data/tz_k_202401_v3_1/k_ascaris_lumb_intensity" relevant=" ../k_add_result  = 'Yes'" type="string"/>
      <bind calculate="if( ../k_ascaris_lumb_intensity &gt;= 50000, 1, 0)" nodeset="/data/tz_k_202401_v3_1/k_ascaris_lumb_heavy_intensity" relevant=" ../k_add_result  = 'Yes'" type="string"/>
      <bind calculate="if( ../k_ascaris_lumb_intensity &gt;= 5000 and  ../k_ascaris_lumb_intensity  &lt; 50000, 1, 0)" nodeset="/data/tz_k_202401_v3_1/k_ascaris_lumb_moderate_intensity" relevant=" ../k_add_result  = 'Yes'" type="string"/>
      <bind nodeset="/data/tz_k_202401_v3_1/k_trichuris_sa" relevant=" ../k_add_result  = 'Yes'" required="true()" type="int"/>
      <bind nodeset="/data/tz_k_202401_v3_1/k_trichuris_sb" relevant=" ../k_add_result  = 'Yes'" required="true()" type="int"/>
      <bind calculate="( ../k_trichuris_sa  +  ../k_trichuris_sb ) *12" nodeset="/data/tz_k_202401_v3_1/k_trichuris_intensity" relevant=" ../k_add_result  = 'Yes'" type="string"/>
      <bind calculate="if( ../k_trichuris_intensity &gt;= 400, 1, 0)" nodeset="/data/tz_k_202401_v3_1/k_trichuris_heavy_intensity" relevant=" ../k_add_result  = 'Yes'" type="string"/>
      <bind calculate="if( ../k_trichuris_intensity &gt;= 100 and  ../k_trichuris_intensity  &lt; 400, 1, 0)" nodeset="/data/tz_k_202401_v3_1/k_trichuris_moderate_intensity" relevant=" ../k_add_result  = 'Yes'" type="string"/>
      <bind nodeset="/data/tz_k_202401_v3_1/k_sch_man_sa" relevant=" ../k_add_result  = 'Yes'" required="true()" type="int"/>
      <bind nodeset="/data/tz_k_202401_v3_1/k_sch_man_sb" relevant=" ../k_add_result  = 'Yes'" required="true()" type="int"/>
      <bind calculate="( ../k_sch_man_sa  +  ../k_sch_man_sb ) *12" nodeset="/data/tz_k_202401_v3_1/k_sch_man_intensity" relevant=" ../k_add_result  = 'Yes'" type="string"/>
      <bind calculate="if( ../k_sch_man_intensity &gt;= 400, 1, 0)" nodeset="/data/tz_k_202401_v3_1/k_sch_man_heavy_intensity" relevant=" ../k_add_result  = 'Yes'" type="string"/>
      <bind calculate="if( ../k_sch_man_intensity &gt;= 100 and  ../k_sch_man_intensity  &lt; 400, 1, 0)" nodeset="/data/tz_k_202401_v3_1/k_sch_man_moderate_intensity" relevant=" ../k_add_result  = 'Yes'" type="string"/>
      <bind nodeset="/data/tz_k_202401_v3_1/k_sch_hae_sa" relevant=" ../k_add_result  = 'Yes'" type="int"/>
      <bind nodeset="/data/tz_k_202401_v3_1/k_sch_hae_sb" relevant=" ../k_add_result  = 'Yes'" type="int"/>
      <bind nodeset="/data/tz_k_202401_v3_1/note_haematobium" readonly="true()" relevant=" ../k_sch_hae_sb &gt;0 or  ../k_sch_hae_sa &gt;0" type="string"/>
      <bind nodeset="/data/tz_k_202401_v3_1/k_other_eggs" relevant=" ../k_add_result  = 'Yes'" required="true()" type="select1"/>
      <bind nodeset="/data/tz_k_202401_v3_1/other_eggs_name" relevant=" ../k_add_result  = 'Yes' and  ../k_other_eggs  = 'Yes'" required="true()" type="string"/>
      <bind nodeset="/data/tz_k_202401_v3_1/other_eggs_sa" relevant=" ../k_add_result  = 'Yes' and  ../k_other_eggs  = 'Yes'" required="true()" type="int"/>
      <bind nodeset="/data/tz_k_202401_v3_1/other_eggs_sb" relevant=" ../k_add_result  = 'Yes' and  ../k_other_eggs  = 'Yes'" required="true()" type="int"/>
      <bind nodeset="/data/tz_k_202401_v3_1/k_remarks" relevant=" ../k_add_result  = 'Yes'" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/k_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/k_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
