<?xml version="1.0" encoding="UTF-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms"
  xmlns:ev="http://www.w3.org/2001/xml-events"
  xmlns:h="http://www.w3.org/1999/xhtml"
  xmlns:jr="http://openrosa.org/javarosa"
  xmlns:odk="http://www.opendatakit.org/xforms"
  xmlns:orx="http://openrosa.org/xforms"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>5. SCH/STH - POC CCA V2</h:title>
    <model >

      <instance>
        <data id="cd_sch_sth_baseline_5_cca_202202_v2">
          <c_recorder_id/>
          <c_zone_sante/>
          <c_village/>
          <c_ecole/>
          <c_code_ecole/>
          <c_generateID/>
          <c_generateID2/>
          <c_date/>
          <c_resultats/>
          <c_remarks/>
          <c_start/>
          <c_end/>
          <meta>
            <instanceID/>
          </meta>
        </data>
      </instance>

      <itext>
        <translation lang="French">
          <text id="/data/c_recorder_id:jr:constraintMsg">
            <value>Le code doit être un nombre à deux chiffres entre 9 et 91</value>
          </text>
          <text id="/data/c_recorder_id:label">
            <value>Entrer l'identifiant de l'enregistreur</value>
          </text>
          <text id="/data/c_recorder_id:hint">
            <value>Code à deux chiffres qui vous a été assigné</value>
          </text>
          <text id="/data/c_zone_sante:label">
            <value>Zone de Santé (niveau admin 3)</value>
          </text>
          <text id="/data/c_village:label">
            <value>Village (niveau admin 5)</value>
          </text>
          <text id="/data/c_ecole:label">
            <value>Nom de l'école</value>
          </text>
          <text id="/data/c_code_ecole:label">
            <value>Code de l'école</value>
          </text>
          <text id="/data/c_code_ecole:hint">
            <value>Code à trois chiffre. Ex : 001</value>
          </text>
          <text id="/data/c_generateID:jr:constraintMsg">
            <value>Le format doit être 123-1234-567</value>
          </text>
          <text id="/data/c_generateID:label">
            <value>Entrer l'identifiant unique du test de diagnostique (Doit avoir le format 123-1234-456 Majuscule avec des tiret de soulignement seulement)</value>
          </text>
          <text id="/data/c_generateID2:jr:constraintMsg">
            <value>Le code répété doit être le même</value>
          </text>
          <text id="/data/c_generateID2:label">
            <value>Répéter l'identifiant unique</value>
          </text>
          <text id="/data/c_date:label">
            <value>Date</value>
          </text>
          <text id="/data/c_resultats:label">
            <value>Résultat des cassettes</value>
          </text>
          <text id="/data/c_resultats/Négatif:label">
            <value>0 = Négatif</value>
          </text>
          <text id="/data/c_resultats/Positif:label">
            <value>1 = Positif</value>
          </text>
          <text id="/data/c_resultats/Invalide:label">
            <value>2 = Invalide</value>
          </text>
          <text id="/data/c_remarks:label">
            <value>Notes additionnelles</value>
          </text>
        </translation>
      </itext>
      <bind constraint="regex(.,'^[0-9]{2}$')" jr:constraintMsg="jr:itext('/data/c_recorder_id:jr:constraintMsg')" nodeset="/data/c_recorder_id" required="true()" type="int"/>
      <bind nodeset="/data/c_zone_sante" required="true()" type="string" db_get="col_1"/>
      <bind nodeset="/data/c_village" required="true()" type="string" db_get="col_2" db_filter_by_col_1="/data/c_zone_sante"/>
      <bind nodeset="/data/c_ecole" required="true()" type="string" db_get="col_3" db_filter_by_col_2="/data/c_village"/>
      <bind nodeset="/data/c_code_ecole" required="true()" type="string" db_get="col_4" db_filter_by_col_3="/data/c_ecole"/>
      <bind constraint="regex(.,'^[0-9]{3}-[0-9]{4}-[0-9]{3}$')" jr:constraintMsg="jr:itext('/data/c_generateID:jr:constraintMsg')" nodeset="/data/c_generateID" required="true()" type="string"/>
      <bind constraint=". =  /data/c_generateID " jr:constraintMsg="jr:itext('/data/c_generateID2:jr:constraintMsg')" nodeset="/data/c_generateID2" required="true()" type="string"/>
      <bind nodeset="/data/c_date" required="true()" type="date"/>
      <bind nodeset="/data/c_resultats" type="string"/>
      <bind nodeset="/data/c_remarks" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/c_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/c_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/data/c_recorder_id">
      <label ref="jr:itext('/data/c_recorder_id:label')"/>
      <hint ref="jr:itext('/data/c_recorder_id:hint')"/>
    </input>
    <input ref="/data/c_zone_sante">
      <label ref="jr:itext('/data/c_zone_sante:label')"/>
    </input>
    <input ref="/data/c_village">
      <label ref="jr:itext('/data/c_village:label')"/>
    </input>
    <input ref="/data/c_ecole">
      <label ref="jr:itext('/data/c_ecole:label')"/>
    </input>
    <input ref="/data/c_code_ecole">
      <label ref="jr:itext('/data/c_code_ecole:label')"/>
      <hint ref="jr:itext('/data/c_code_ecole:hint')"/>
    </input>
    <input ref="/data/c_generateID">
      <label ref="jr:itext('/data/c_generateID:label')"/>
    </input>
    <input ref="/data/c_generateID2">
      <label ref="jr:itext('/data/c_generateID2:label')"/>
    </input>
    <input ref="/data/c_date">
      <label ref="jr:itext('/data/c_date:label')"/>
    </input>
    <select1 ref="/data/c_resultats">
      <label ref="jr:itext('/data/c_resultats:label')"/>
      <item>
        <label ref="jr:itext('/data/c_resultats/Négatif:label')"/>
        <value>Négatif</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_resultats/Positif:label')"/>
        <value>Positif</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_resultats/Invalide:label')"/>
        <value>Invalide</value>
      </item>
    </select1>
    <input ref="/data/c_remarks">
      <label ref="jr:itext('/data/c_remarks:label')"/>
    </input>
  </h:body>
</h:html>
