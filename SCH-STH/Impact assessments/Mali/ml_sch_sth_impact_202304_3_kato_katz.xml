        <translation lang="English">
          <text id="/data/k_Recorder:label">
            <value>code register</value>
          </text>
          <text id="/data/k_district:label">
            <value>Select the county</value>
          </text>
          <text id="/data/k_subdistrict:label">
            <value>Select the Sub-district</value>
          </text>
          <text id="/data/k_site_name:label">
            <value>Select the village</value>
          </text>
          <text id="/data/k_barcodeid:jr:constraintMsg">
            <value>Please scan the QR code for the correct district. The first three letters of the QR code must match the first three letters of the district name. For example, TCH0001 for TCHAOUDJO.</value>
          </text>
          <text id="/data/k_barcodeid:label">
            <value>Scan QR code now</value>
          </text>
          <text id="/data/k_barcode_ok:label">
            <value>Was the QR code properly scanned</value>
          </text>
          <text id="/data/k_barcode_ok/Oui:label">
            <value>Yes</value>
          </text>
          <text id="/data/k_barcode_ok/Non:label">
            <value>No</value>
          </text>
          <text id="/data/k_manual_code_id:jr:constraintMsg">
            <value>Please enter the code from the QR code sticker. The format is the first three letters of the district followed by four digits. For example, TCH0001 for TCHAOUDJO</value>
          </text>
          <text id="/data/k_manual_code_id:label">
            <value>Enter the QR code manually</value>
          </text>
          <text id="/data/k_sch_man_sa:label">
            <value>Schistosoma mansoni (eggs) – Slide A</value>
          </text>
          <text id="/data/k_sch_man_sb:label">
            <value>Schistosoma mansoni (eggs) – Slide B</value>
          </text>
          <text id="/data/k_ascaris_lumb_sa:label">
            <value>Ascaris lumbricoides (eggs) – Slide A</value>
          </text>
          <text id="/data/k_ascaris_lumb_sb:label">
            <value>Ascaris lumbricoides (eggs) – Slide B</value>
          </text>
          <text id="/data/k_hookworm_sa:label">
            <value>Hookworm – Slide A</value>
          </text>
          <text id="/data/k_hookworm_sb:label">
            <value>Hookworm – Slide B</value>
          </text>
          <text id="/data/k_trichuris_sa:label">
            <value>Trichuris trichura – Slide A</value>
          </text>
          <text id="/data/k_trichuris_sb:label">
            <value>Trichuris trichura – Slide B</value>
          </text>
          <text id="/data/k_sch_hae_sa:label">
            <value>Schistosoma haematobium – Slide A</value>
          </text>
          <text id="/data/k_sch_hae_sb:label">
            <value>Schistosoma haematobium – Slide B</value>
          </text>
          <text id="/data/k_remarks:label">
            <value>Additional comment</value>
          </text>
        </translation>
      </itext>

      <bind nodeset="/data/k_Recorder" required="true()" type="int"/>
      <bind nodeset="/data/k_district" required="true()" type="string" db_get="col_1"/>
      <bind nodeset="/data/k_subdistrict" required="true()" type="string" db_get="col_2" db_filter_by_col_1="/data/k_district"/>
      <bind nodeset="/data/k_site_name" required="true()" type="string" db_get="col_3" db_filter_by_col_2="/data/k_subdistrict"/>
      <bind jr:constraintMsg="jr:itext('/data/k_barcodeid:jr:constraintMsg')" nodeset="/data/k_barcodeid" type="barcode"/>
      <bind nodeset="/data/k_barcode_ok" required="true()" type="string"/>
      <bind jr:constraintMsg="jr:itext('/data/k_manual_code_id:jr:constraintMsg')" nodeset="/data/k_manual_code_id" relevant=" /data/k_barcode_ok  = 'Non'" required="true()" type="string"/>
      <bind nodeset="/data/k_sch_man_sa" required="true()" type="int"/>
      <bind nodeset="/data/k_sch_man_sb" required="true()" type="int"/>
      <bind nodeset="/data/k_ascaris_lumb_sa" required="true()" type="int"/>
      <bind nodeset="/data/k_ascaris_lumb_sb" required="true()" type="int"/>
      <bind nodeset="/data/k_hookworm_sa" required="true()" type="int"/>
      <bind nodeset="/data/k_hookworm_sb" required="true()" type="int"/>
      <bind nodeset="/data/k_trichuris_sa" required="true()" type="int"/>
      <bind nodeset="/data/k_trichuris_sb" required="true()" type="int"/>
      <bind nodeset="/data/k_sch_hae_sa" type="int"/>
      <bind nodeset="/data/k_sch_hae_sb" type="int"/>
      <bind nodeset="/data/k_remarks" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/k_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/k_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
