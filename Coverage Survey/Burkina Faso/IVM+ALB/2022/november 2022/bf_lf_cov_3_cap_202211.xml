      <bind constraint="regex(., '^[A-Z]{1}[0-9]{2}$')" jr:constraintMsg="jr:itext('/data/numero_menage:jr:constraintMsg')" nodeset="/data/numero_menage" required="true()" type="string"/>
      <bind constraint=". &gt;= 18" jr:constraintMsg="jr:itext('/data/Age:jr:constraintMsg')" nodeset="/data/Age" required="true()" type="int"/>
      <bind nodeset="/data/Sexe" required="true()" type="string"/>
      <bind nodeset="/data/Niveau_instruction" required="true()" type="string"/>
      <bind nodeset="/data/duree_resident" required="true()" type="int"/>
      <bind nodeset="/data/duree_resident_mois" relevant=" /data/duree_resident  &lt; 1" required="true()" type="int"/>
      <bind nodeset="/data/Entendu_parler_Oncho" required="true()" type="string"/>
      <bind nodeset="/data/Inquiet" relevant=" /data/Entendu_parler_Oncho  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/Manifestations" relevant=" /data/Entendu_parler_Oncho  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/Autres_manisfestations" relevant=" /data/Entendu_parler_Oncho  = 'Oui' and selected( /data/Manifestations , 'Otherm')" required="true()" type="string"/>
      <bind nodeset="/data/Mode_transmission" relevant=" /data/Entendu_parler_Oncho  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/Autres_transmission" relevant=" /data/Entendu_parler_Oncho  = 'Oui' and selected( /data/Mode_transmission , 'Othert')" required="true()" type="string"/>
      <bind nodeset="/data/prevention" required="true()" type="string"/>
      <bind nodeset="/data/Autres_prevention" relevant="selected( /data/prevention , 'Otherp')" required="true()" type="string"/>
      <bind nodeset="/data/Cnce_Aveugle" relevant=" /data/Entendu_parler_Oncho  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/Nombre_aveugle" relevant=" /data/Cnce_Aveugle  = 'Oui'" required="true()" type="int"/>
      <bind nodeset="/data/Informe" required="true()" type="string"/>
      <bind nodeset="/data/Canaux_information" relevant=" /data/Informe  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/Autres_canaux_information" relevant=" /data/Informe  = 'Oui' and selected( /data/Canaux_information , 'Otherc')" required="true()" type="string"/>
      <bind nodeset="/data/nbre_resident_menage" required="true()" type="int"/>
      <bind constraint=". &lt;=  /data/nbre_resident_menage " jr:constraintMsg="jr:itext('/data/nbre_resident_moins5ans:jr:constraintMsg')" nodeset="/data/nbre_resident_moins5ans" required="true()" type="int"/>
      <bind nodeset="/data/resident_non_traite" required="true()" type="string"/>
      <bind nodeset="/data/nbre_resident_non_traite" relevant=" /data/resident_non_traite  = 'Oui'" required="true()" type="int"/>
      <bind nodeset="/data/satisfaction" required="true()" type="string"/>
      <bind nodeset="/data/insatisfaction" relevant=" /data/satisfaction  = 'Passatisfaitdutout' and  /data/satisfaction = 'Passatisfait'" required="true()" type="string"/>
      <bind nodeset="/data/Perception_DC" required="true()" type="string"/>
      <bind nodeset="/data/Suggestions" required="false()" type="string"/>
      <bind jr:constraintMsg="jr:itext('/data/gps:jr:constraintMsg')" nodeset="/data/gps" required="true()" type="geopoint"/>
      <bind nodeset="/data/Commentaires" required="false()" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
