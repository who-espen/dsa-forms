			<bind nodeset="/data/v_gaoua_cat" relevant=" /data/v_district ='GAOUA'" required="true()" type="string"/>
			<bind calculate=" /data/v_gaoua_cat " nodeset="/data/v_categorie" required="true()" type="string"/>
			<bind nodeset="/data/v_csps" required="true()" type="string" db_add_col_1="/data/v_district" db_add_col_2="."/>
			<bind nodeset="/data/v_grappe" required="true()" type="string" db_add_col_2="/data/v_csps" db_add_col_3="."/>
			<bind constraint=". &gt; 0 and . &lt;= 30" jr:constraintMsg="jr:itext('/data/v_num_grappe:jr:constraintMsg')" nodeset="/data/v_num_grappe" db_add_col_4="." required="true()" type="int"/>
			<bind calculate="if(  /data/v_district  = 'BATIE', 'BAT', if( /data/v_district  = 'BITTOU', 'BIT', 'GAO'))" nodeset="/data/v_prefix_district" required="true()" type="string"/>
			<bind calculate="if(  /data/v_num_grappe  &lt; 10, concat( /data/v_prefix_district , '_0',  /data/v_num_grappe ), concat( /data/v_prefix_district , '_',  /data/v_num_grappe ))" constraint="regex(.,'^[A-Z]{3}_[0-9]{2}$')" jr:constraintMsg="jr:itext('/data/v_code_site:jr:constraintMsg')" nodeset="/data/v_code_site" readonly="true()" required="true()" type="string"/>
			<bind constraint="regex(.,'^[0-9]{3},[0-9]{2}$')" jr:constraintMsg="jr:itext('/data/v_Coverage_village22:jr:constraintMsg')" nodeset="/data/v_Coverage_village22" required="true()" type="string"/>
			<bind nodeset="/data/v_gps" required="true()" type="geopoint"/>
			<bind nodeset="/data/v_commentaires" type="string"/>
			<bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/v_debut_enregistrement" type="dateTime"/>
			<bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/v_fin_enregistrement" type="dateTime"/>
			<bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
