				<value>GAOUA</value>
			</item>
		</select1>
		<select1 ref="/data/v_gaoua_cat">
			<label ref="jr:itext('/data/v_gaoua_cat:label')"/>
			<item>
				<label ref="jr:itext('/data/v_gaoua_cat/GAOUA [CSPS B]:label')"/>
				<value>GAOUA [CSPS B]</value>
			</item>
			<item>
				<label ref="jr:itext('/data/v_gaoua_cat/GAOUA [CSPS C - N]:label')"/>
				<value>GAOUA [CSPS C - N]</value>
			</item>
			<item>
				<label ref="jr:itext('/data/v_gaoua_cat/GAOUA [CSPS P - T]:label')"/>
				<value>GAOUA [CSPS P - T]</value>
			</item>
		</select1>
		<select1 ref="/data/v_csps">
			<label ref="jr:itext('/data/v_csps:label')"/>
			<itemset nodeset="instance('csps_list')/root/item[if(  /data/v_district  = 'GAOUA', district_list =  /data/v_district  and categorie_list =  /data/v_categorie , district_list =  /data/v_district  and categorie_list = 'notgaoua')]">
				<value ref="name"/>
				<label ref="jr:itext(itextId)"/>
			</itemset>
		</select1>
		<select1 ref="/data/v_grappe">
			<label ref="jr:itext('/data/v_grappe:label')"/>
			<itemset nodeset="instance('village_grappe_list')/root/item[csps_list =  /data/v_csps ]">
				<value ref="name"/>
				<label ref="jr:itext(itextId)"/>
			</itemset>
		</select1>
		<input ref="/data/v_num_grappe">
			<label ref="jr:itext('/data/v_num_grappe:label')"/>
		</input>
		<input ref="/data/v_code_site">
			<label ref="jr:itext('/data/v_code_site:label')"/>
			<hint ref="jr:itext('/data/v_code_site:hint')"/>
		</input>
		<input ref="/data/v_Coverage_village22">
			<label ref="jr:itext('/data/v_Coverage_village22:label')"/>
			<hint ref="jr:itext('/data/v_Coverage_village22:hint')"/>
		</input>
		<input ref="/data/v_gps">
			<label ref="jr:itext('/data/v_gps:label')"/>
			<hint ref="jr:itext('/data/v_gps:hint')"/>
		</input>
		<input ref="/data/v_commentaires">
			<label ref="jr:itext('/data/v_commentaires:label')"/>
		</input>
	</h:body>
