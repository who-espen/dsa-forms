      <bind nodeset="/data/d_code_enquete" type="string" required="true()" calculate="concat( /data/d_code_grappe ,'_', /data/d_numero_menage ,'_',if( /data/d_numero_enquete &lt;10, concat('0',  /data/d_numero_enquete ),  /data/d_numero_enquete ))" readonly="true()"/>
      <bind nodeset="/data/d_age" type="int" required="true()" constraint=". &gt;= 18" jr:constraintMsg="jr:itext('/data/d_age:jr:constraintMsg')"/>
      <bind nodeset="/data/d_sexe" type="string" required="true()"/>
      <bind nodeset="/data/d_niveau_instruction" type="string" required="true()"/>
      <bind nodeset="/data/d_duree_resident" type="int" required="true()" constraint=". &lt;= 120" jr:constraintMsg="jr:itext('/data/d_duree_resident:jr:constraintMsg')"/>
      <bind nodeset="/data/d_duree_resident_mois" type="int" required="true()" constraint=". &gt;= 2 and . &lt;= 11" jr:constraintMsg="jr:itext('/data/d_duree_resident_mois:jr:constraintMsg')" relevant=" /data/d_duree_resident  = 0"/>
      <bind nodeset="/data/d_entendu_parler_oncho" type="string" required="true()"/>
      <bind nodeset="/data/d_inquiet" type="string" required="true()" relevant=" /data/d_entendu_parler_oncho  = 'Oui'"/>
      <bind nodeset="/data/d_manifestations" type="string" required="true()" relevant=" /data/d_entendu_parler_oncho  = 'Oui'"/>
      <bind nodeset="/data/d_autres_manisfestations" type="string" required="true()" relevant=" /data/d_entendu_parler_oncho  = 'Oui' and selected( /data/d_manifestations , 'Otherm')"/>
      <bind nodeset="/data/d_mode_transmission" type="string" required="true()" relevant=" /data/d_entendu_parler_oncho  = 'Oui'"/>
      <bind nodeset="/data/d_autres_transmission" type="string" required="true()" relevant=" /data/d_entendu_parler_oncho  = 'Oui' and selected( /data/d_mode_transmission , 'Othert')"/>
      <bind nodeset="/data/d_prevention" type="string" required="true()"/>
      <bind nodeset="/data/d_autres_prevention" type="string" required="true()" relevant="selected( /data/d_prevention , 'Otherp')"/>
      <bind nodeset="/data/d_cnce_aveugle" type="string" required="true()" relevant=" /data/d_entendu_parler_oncho  = 'Oui'"/>
      <bind nodeset="/data/d_nombre_aveugle" type="int" required="true()" relevant=" /data/d_cnce_aveugle  = 'Oui'"/>
      <bind nodeset="/data/d_nbre_resident_menage" type="int" required="true()"/>
      <bind nodeset="/data/d_nbre_resident_moins5ans" type="int" required="true()"/>
      <bind nodeset="/data/d_resident_non_traite" type="string" required="true()"/>
      <bind nodeset="/data/d_nbre_resident_non_traite" type="int" required="true()" relevant=" /data/d_resident_non_traite  = 'Oui'"/>
      <bind nodeset="/data/d_satisfaction" type="string" required="true()"/>
      <bind nodeset="/data/d_raison_insatisf" type="string" required="true()" relevant=" /data/d_satisfaction  = 'Passatisfaitdutout' or  /data/d_satisfaction = 'Passatisfait'"/>
      <bind nodeset="/data/d_raison_insatisfa" type="string" required="true()" relevant="selected( /data/d_raison_insatisf , 'autreinsatisfaction')"/>
      <bind nodeset="/data/d_rsatisf" type="string" required="true()" relevant=" /data/d_satisfaction  = 'Satisfait' or  /data/d_satisfaction = 'Tressatisfait'"/>
      <bind nodeset="/data/d_raison_satisfa" type="string" required="true()" relevant="selected( /data/d_rsatisf , 'autresatisfaction' )"/>
      <bind nodeset="/data/d_suggestions" type="string"/>
      <bind nodeset="/data/d_gps" type="geopoint" required="true()" jr:constraintMsg="jr:itext('/data/d_gps:jr:constraintMsg')"/>
      <bind nodeset="/data/d_notes" type="text"/>
      <bind nodeset="/data/d_debut_enreg" jr:preload="timestamp" type="dateTime" jr:preloadParams="start"/>
      <bind nodeset="/data/d_fin_enreg" jr:preload="timestamp" type="dateTime" jr:preloadParams="end"/>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" jr:preload="uid"/>
    </model>
  </h:head>
