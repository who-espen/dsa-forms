      <bind nodeset="/data/d_equipe" required="true()" type="select1"/>
      <bind db_get="col_1" nodeset="/data/d_district" required="true()" type="string"/>
      <bind db_filter_by_col_1="/data/d_district" db_get="col_2" nodeset="/data/d_code_grappe" required="true()" type="string"/>
      <bind constraint="regex(.,'^[A-Z]{1}[0-9]{2}$')" db_filter_by_col_2="/data/d_code_grappe" db_get="col_3" jr:constraintMsg="jr:itext('/data/d_numero_menage:jr:constraintMsg')" nodeset="/data/d_numero_menage" required="true()" type="string"/>
      <bind constraint=".&lt;100" jr:constraintMsg="jr:itext('/data/d_numero_enquete:jr:constraintMsg')" nodeset="/data/d_numero_enquete" required="true()" type="int"/>
      <bind calculate="concat( /data/d_code_grappe ,'_', /data/d_numero_menage ,'_',if( /data/d_numero_enquete &lt;10, concat('0',  /data/d_numero_enquete ),  /data/d_numero_enquete ))" nodeset="/data/d_code_enquete" readonly="true()" required="true()" type="string"/>
      <bind constraint=". &gt;= 18" jr:constraintMsg="jr:itext('/data/d_age:jr:constraintMsg')" nodeset="/data/d_age" required="true()" type="int"/>
      <bind nodeset="/data/d_sexe" required="true()" type="select1"/>
      <bind nodeset="/data/d_niveau_instruction" required="true()" type="select1"/>
      <bind constraint=". &lt;= 120" jr:constraintMsg="jr:itext('/data/d_duree_resident:jr:constraintMsg')" nodeset="/data/d_duree_resident" required="true()" type="int"/>
      <bind constraint=". &gt;= 2 and . &lt;= 11" jr:constraintMsg="jr:itext('/data/d_duree_resident_mois:jr:constraintMsg')" nodeset="/data/d_duree_resident_mois" relevant=" /data/d_duree_resident  = 0" required="true()" type="int"/>
      <bind nodeset="/data/d_entendu_parler_oncho" required="true()" type="select1"/>
      <bind nodeset="/data/d_inquiet" relevant=" /data/d_entendu_parler_oncho  = 'Oui'" required="true()" type="select1"/>
      <bind nodeset="/data/d_manifestations" relevant=" /data/d_entendu_parler_oncho  = 'Oui'" required="true()" type="select"/>
      <bind nodeset="/data/d_autres_manisfestations" relevant=" /data/d_entendu_parler_oncho  = 'Oui' and selected( /data/d_manifestations , 'Otherm')" required="true()" type="string"/>
      <bind nodeset="/data/d_mode_transmission" relevant=" /data/d_entendu_parler_oncho  = 'Oui'" required="true()" type="select"/>
      <bind nodeset="/data/d_autres_transmission" relevant=" /data/d_entendu_parler_oncho  = 'Oui' and selected( /data/d_mode_transmission , 'Othert')" required="true()" type="string"/>
      <bind nodeset="/data/d_prevention" required="true()" type="select"/>
      <bind nodeset="/data/d_autres_prevention" relevant="selected( /data/d_prevention , 'Otherp')" required="true()" type="string"/>
      <bind nodeset="/data/d_cnce_aveugle" relevant=" /data/d_entendu_parler_oncho  = 'Oui'" required="true()" type="select1"/>
      <bind nodeset="/data/d_nombre_aveugle" relevant=" /data/d_cnce_aveugle  = 'Oui'" required="true()" type="int"/>
      <bind nodeset="/data/d_nbre_resident_menage" required="true()" type="int"/>
      <bind constraint=". &lt;=  /data/d_nbre_resident_menage " nodeset="/data/d_nbre_resident_moins5ans" required="true()" type="int"/>
      <bind nodeset="/data/d_resident_non_traite" required="true()" type="select1"/>
      <bind nodeset="/data/d_nbre_resident_non_traite" relevant=" /data/d_resident_non_traite  = 'Oui'" required="true()" type="int"/>
      <bind nodeset="/data/d_satisfaction" required="true()" type="select1"/>
      <bind nodeset="/data/d_raison_insatisf" relevant=" /data/d_satisfaction  = 'Passatisfaitdutout' or  /data/d_satisfaction = 'Passatisfait'" required="true()" type="select"/>
      <bind nodeset="/data/d_raison_insatisfa" relevant="selected( /data/d_raison_insatisf , 'autreinsatisfaction')" required="true()" type="string"/>
      <bind nodeset="/data/d_rsatisf" relevant=" /data/d_satisfaction  = 'Satisfait' or  /data/d_satisfaction = 'Tressatisfait'" required="true()" type="select"/>
      <bind nodeset="/data/d_raison_satisfa" relevant="selected( /data/d_rsatisf , 'autresatisfaction' )" required="true()" type="string"/>
      <bind nodeset="/data/d_suggestions" type="string"/>
      <bind jr:constraintMsg="jr:itext('/data/d_gps:jr:constraintMsg')" nodeset="/data/d_gps" required="true()" type="geopoint"/>
      <bind nodeset="/data/d_notes" type="text"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/d_debut_enreg" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/d_fin_enreg" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
