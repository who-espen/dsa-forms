      <bind nodeset="/data/c_csps" type="string" required="true()"/>
      <bind nodeset="/data/c_grappe" type="string" required="true()"/>
      <bind nodeset="/data/c_numero_grappe" type="int" constraint=". &gt;= 1 and . &lt;= 30" jr:constraintMsg="jr:itext('/data/c_numero_grappe:jr:constraintMsg')" required="true()"/>
      <bind nodeset="/data/c_code_grappe" type="string" required="true()" calculate="concat( /data/c_numero_grappe ,'_', /data/c_grappe )" readonly="true()" db_add_col_1="/data/c_district" db_add_col_2="."/>
      <bind nodeset="/data/c_nombre_menage" type="int" required="true()"/>
      <bind nodeset="/data/c_nombre_segment" type="int" required="true()"/>
      <bind nodeset="/data/c_gps" type="geopoint" required="true()"/>
      <bind nodeset="/data/c_notes" type="text"/>
      <bind nodeset="/data/c_debut_enreg" jr:preload="timestamp" type="dateTime" jr:preloadParams="start"/>
      <bind nodeset="/data/c_fin_enreg" jr:preload="timestamp" type="dateTime" jr:preloadParams="end"/>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" jr:preload="uid"/>
    </model>
  </h:head>
  <h:body>
    <select1 ref="/data/c_equipe">
      <label ref="jr:itext('/data/c_equipe:label')"/>
      <hint ref="jr:itext('/data/c_equipe:hint')"/>
      <itemset nodeset="instance('equipe')/root/item">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <select1 ref="/data/c_district">
      <label ref="jr:itext('/data/c_district:label')"/>
      <hint ref="jr:itext('/data/c_district:hint')"/>
      <itemset nodeset="instance('district_list')/root/item">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <select1 ref="/data/c_commune">
      <label ref="jr:itext('/data/c_commune:label')"/>
      <itemset nodeset="instance('commune_list')/root/item[district_list =  /data/c_district ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <select1 ref="/data/c_csps">
      <label ref="jr:itext('/data/c_csps:label')"/>
      <itemset nodeset="instance('csps_list')/root/item[commune_list =  /data/c_commune ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <select1 ref="/data/c_grappe">
      <label ref="jr:itext('/data/c_grappe:label')"/>
      <itemset nodeset="instance('grappe_list')/root/item[csps_list =  /data/c_csps ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <input ref="/data/c_numero_grappe">
      <label ref="jr:itext('/data/c_numero_grappe:label')"/>
    </input>
    <input ref="/data/c_code_grappe">
      <label ref="jr:itext('/data/c_code_grappe:label')"/>
    </input>
    <input ref="/data/c_nombre_menage">
      <label ref="jr:itext('/data/c_nombre_menage:label')"/>
    </input>
    <input ref="/data/c_nombre_segment">
      <label ref="jr:itext('/data/c_nombre_segment:label')"/>
    </input>
    <input ref="/data/c_gps">
      <label ref="jr:itext('/data/c_gps:label')"/>
      <hint ref="jr:itext('/data/c_gps:hint')"/>
    </input>
    <input ref="/data/c_notes">
      <label ref="jr:itext('/data/c_notes:label')"/>
    </input>
  </h:body>
</h:html>
