      <bind nodeset="/data/Numero_menage" required="true()" type="string"/>
      <bind constraint=". &gt;= 0 and .&lt;120" jr:constraintMsg="jr:itext('/data/Age:jr:constraintMsg')" nodeset="/data/Age" required="true()" type="int"/>
      <bind constraint=". &gt;= 0 and . &lt;= 11" jr:constraintMsg="jr:itext('/data/Age_en_mois:jr:constraintMsg')" nodeset="/data/Age_en_mois" relevant=" /data/Age  = 0" required="true()" type="int"/>
      <bind nodeset="/data/Sexe" required="true()" type="string"/>
      <bind nodeset="/data/Repondant" required="true()" type="string"/>
      <bind constraint=". &lt;=  /data/Age " jr:constraintMsg="jr:itext('/data/Duree_sejour:jr:constraintMsg')" nodeset="/data/Duree_sejour" required="true()" type="int"/>
      <bind nodeset="/data/Recu_IVM" required="true()" type="string"/>
      <bind nodeset="/data/list_lieu_menage" relevant=" /data/Recu_IVM  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/Pourquoi_Pas_recu_IVM" relevant=" /data/Recu_IVM  = 'Non'" required="true()" type="string"/>
      <bind nodeset="/data/Autres_raisons" relevant=" /data/Pourquoi_Pas_recu_IVM  = 'Other'" required="false()" type="string"/>
      <bind nodeset="/data/Avale_IVM" relevant=" /data/Recu_IVM  = 'Oui'" required="true()" type="string"/>
      <bind constraint=". &lt;= 10" nodeset="/data/Nb_Comprime_IVM_Avale" relevant=" /data/Avale_IVM  = 'Oui'" required="true()" type="int"/>
      <bind nodeset="/data/Prise_Supervisee" relevant=" /data/Avale_IVM  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/Pourquoi_pas_Avale" relevant=" /data/Avale_IVM  = 'Non'" required="true()" type="string"/>
      <bind nodeset="/data/Effet_indesirable" relevant=" /data/Avale_IVM  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/Type_Effet_indesirable" relevant=" /data/Avale_IVM  = 'Oui' and  /data/Effet_indesirable  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/Autre_effet_indesirable" relevant="selected( /data/Type_Effet_indesirable , 'Other')" required="false()" type="string"/>
      <bind nodeset="/data/Combien_de_temps" relevant=" /data/Effet_indesirable  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/Prise_taille" relevant=" /data/Recu_IVM  = 'Oui'" required="true()" type="string"/>
      <bind jr:constraintMsg="jr:itext('/data/Taille:jr:constraintMsg')" nodeset="/data/Taille" relevant=" /data/Repondant  = 'Personne_interrogee'" required="true()" type="string"/>
      <bind jr:constraintMsg="jr:itext('/data/Traitement_par_lequipe:jr:constraintMsg')" nodeset="/data/Traitement_par_lequipe" relevant=" /data/Recu_IVM  = 'Non'" required="true()" type="string"/>
      <bind nodeset="/data/Notes" required="false()" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/debut_edition" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/fin_edition" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
