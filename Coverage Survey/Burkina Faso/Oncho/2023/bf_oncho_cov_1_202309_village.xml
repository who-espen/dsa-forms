			<bind nodeset="/data/gps" required="true()" type="geopoint"/>
			<bind nodeset="/data/notes" type="string"/>
			<bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/start" type="dateTime"/>
			<bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/end" type="dateTime"/>
			<bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
		</model>
	</h:head>
	<h:body>
		<select1 ref="/data/code_equipe">
			<label ref="jr:itext('/data/code_equipe:label')"/>
			<hint ref="jr:itext('/data/code_equipe:hint')"/>
			<item>
				<label ref="jr:itext('/data/code_equipe/1:label')"/>
				<value>1</value>
			</item>
			<item>
				<label ref="jr:itext('/data/code_equipe/2:label')"/>
				<value>2</value>
			</item>
			<item>
				<label ref="jr:itext('/data/code_equipe/3:label')"/>
				<value>3</value>
			</item>
			<item>
				<label ref="jr:itext('/data/code_equipe/4:label')"/>
				<value>4</value>
			</item>
			<item>
				<label ref="jr:itext('/data/code_equipe/5:label')"/>
				<value>5</value>
			</item>
		</select1>
		<select1 ref="/data/district">
			<label ref="jr:itext('/data/district:label')"/>
			<hint ref="jr:itext('/data/district:hint')"/>
			<item>
				<label ref="jr:itext('/data/district/DANO:label')"/>
				<value>DANO</value>
			</item>
		</select1>
		<select1 ref="/data/cat">
			<label ref="jr:itext('/data/cat:label')"/>
			<hint ref="jr:itext('/data/cat:hint')"/>
			<itemset nodeset="instance('category_list')/root/item[district_list =  /data/district ]">
				<value ref="name"/>
				<label ref="jr:itext(itextId)"/>
			</itemset>
		</select1>
		<select1 ref="/data/csps">
			<label ref="jr:itext('/data/csps:label')"/>
			<hint ref="jr:itext('/data/csps:hint')"/>
			<itemset nodeset="instance('csps_list')/root/item[category_list =  /data/cat ]">
				<value ref="name"/>
				<label ref="jr:itext(itextId)"/>
			</itemset>
		</select1>
		<select1 ref="/data/grappe">
			<label ref="jr:itext('/data/grappe:label')"/>
			<hint ref="jr:itext('/data/grappe:hint')"/>
			<itemset nodeset="instance('grappe_list')/root/item[csps_list =  /data/csps ]">
				<value ref="name"/>
				<label ref="jr:itext(itextId)"/>
			</itemset>
		</select1>
		<input ref="/data/csps_autre">
			<label ref="jr:itext('/data/csps_autre:label')"/>
		</input>
		<input ref="/data/grappe_autre">
			<label ref="jr:itext('/data/grappe_autre:label')"/>
		</input>
		<input ref="/data/num_grappe">
			<label ref="jr:itext('/data/num_grappe:label')"/>
		</input>
		<input ref="/data/gps">
			<label ref="jr:itext('/data/gps:label')"/>
			<hint ref="jr:itext('/data/gps:hint')"/>
		</input>
		<input ref="/data/notes">
			<label ref="jr:itext('/data/notes:label')"/>
		</input>
	</h:body>
</h:html>