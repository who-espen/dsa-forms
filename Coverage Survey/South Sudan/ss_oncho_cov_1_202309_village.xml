      </item>
      <item>
        <label ref="jr:itext('/data/c_county/Warrap:label')"/>
        <value>Warrap</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_county/Wesstern Equatoria:label')"/>
        <value>Wesstern Equatoria</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_county/Western Bahr El Ghazal:label')"/>
        <value>Western Bahr El Ghazal</value>
      </item>
    </select1>
    <select1 ref="/data/c_payam">
      <label ref="jr:itext('/data/c_payam:label')"/>
      <itemset nodeset="instance('payam')/root/item[county =  /data/c_county ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <input ref="/data/c_boma">
      <label ref="jr:itext('/data/c_boma:label')"/>
    </input>
    <input ref="/data/c_village">
      <label ref="jr:itext('/data/c_village:label')"/>
    </input>
    <input ref="/data/c_village_id">
      <label ref="jr:itext('/data/c_village_id:label')"/>
    </input>
    <input ref="/data/gps">
      <label ref="jr:itext('/data/gps:label')"/>
      <hint ref="jr:itext('/data/gps:hint')"/>
    </input>
    <input ref="/data/notes">
      <label ref="jr:itext('/data/notes:label')"/>
    </input>
  </h:body>
