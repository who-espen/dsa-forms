      <bind nodeset="/data/c_payam" required="true()" type="select1" db_add_col_1="/data/c_county" db_add_col_2="."/>
      <bind nodeset="/data/c_boma" required="true()" type="string" db_add_col_2="/data/c_payam" db_add_col_3="."/>
      <bind nodeset="/data/c_village" required="true()" type="string" db_add_col_3="/data/c_boma" db_add_col_4="."/>
      <bind constraint=". &gt; 99 and . &lt; 1000" jr:constraintMsg="jr:itext('/data/c_village_id:jr:constraintMsg')" nodeset="/data/c_village_id" required="true()" type="int" db_add_col_4="/data/c_village" db_add_col_5="."/>
      <bind nodeset="/data/gps" required="true()" type="geopoint"/>
      <bind nodeset="/data/notes" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
