      <bind nodeset="/data/h_region" required="true()" type="string" db_get="col_1"/>
      <bind nodeset="/data/h_woreda" required="true()" type="string" db_get="col_2" db_filter_by_col_1="/data/h_region"/>
      <bind nodeset="/data/h_kebele" required="true()" type="string" db_get="col_3" db_filter_by_col_2="/data/h_woreda"/>
      <bind nodeset="/data/h_village" type="string" db_get="col_4" db_filter_by_col_3="/data/h_kebele"/>
      <bind nodeset="/data/h_head_hh" type="string"/>
      <bind nodeset="/data/h_hh_number" required="true()" type="int"/>
      <bind nodeset="/data/h_interviewed_house" required="true()" type="string"/>
      <bind nodeset="/data/h_reason_not_interviewed" relevant=" /data/h_interviewed_house  = 'No'" required="true()" type="string"/>
      <bind nodeset="/data/h_reason_not_interviewed_other" relevant=" /data/h_reason_not_interviewed  = 'Other'" required="true()" type="string"/>
      <bind nodeset="/data/h_consent" relevant=" /data/h_interviewed_house != 'No'" required="true()" type="string"/>
      <bind nodeset="/data/h_gps" relevant=" /data/h_consent  = 'Yes'" required="true()" type="geopoint"/>
      <bind nodeset="/data/h_ads1" readonly="true()" relevant=" /data/h_consent  = 'Yes'" type="string"/>
      <bind nodeset="/data/h_male_member" relevant=" /data/h_consent  = 'Yes'" required="true()" type="int"/>
      <bind nodeset="/data/h_female_member" relevant=" /data/h_consent  = 'Yes'" required="true()" type="int"/>
      <bind calculate=" /data/h_male_member  +  /data/h_female_member " nodeset="/data/h_total_member" readonly="true()" relevant=" /data/h_consent  = 'Yes'" required="true()" type="int"/>
      <bind nodeset="/data/h_boys_5_14_member" relevant=" /data/h_consent  = 'Yes'" required="true()" type="int"/>
      <bind nodeset="/data/h_girls_5_14_member" relevant=" /data/h_consent  = 'Yes'" required="true()" type="int"/>
      <bind nodeset="/data/h_boys_2_4_member" relevant=" /data/h_consent  = 'Yes'" required="true()" type="int"/>
