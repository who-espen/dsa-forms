      <bind nodeset="/data/h_ads2" readonly="true()" type="string"/>
      <bind nodeset="/data/e_electricity" required="true()" type="string"/>
      <bind nodeset="/data/e_radio" required="true()" type="string"/>
      <bind nodeset="/data/e_tv" required="true()" type="string"/>
      <bind nodeset="/data/e_freeser" required="true()" type="string"/>
      <bind nodeset="/data/e_mitad" required="true()" type="string"/>
      <bind nodeset="/data/e_table" required="true()" type="string"/>
      <bind nodeset="/data/e_chair" required="true()" type="string"/>
      <bind nodeset="/data/e_bed" required="true()" type="string"/>
