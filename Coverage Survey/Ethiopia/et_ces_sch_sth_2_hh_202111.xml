      <bind nodeset="/data/e_water_src" required="true()" type="string"/>
      <bind nodeset="/data/e_toilet" required="true()" type="string"/>
      <bind nodeset="/data/e_fuel_cooking" required="true()" type="string"/>
      <bind nodeset="/data/e_floor_material" required="true()" type="string"/>
      <bind nodeset="/data/e_wall_material" required="true()" type="string"/>
      <bind nodeset="/data/e_roof_material" required="true()" type="string"/>
      <bind nodeset="/data/h_notes" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/h_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/h_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
