            <bind nodeset="/data/p_end_survey_note" readonly="true()" relevant=" /data/p_enrollment !=&quot;Consentement&quot; or  /data/p_lived_in_village  &lt; 10" type="string"/>
            <bind nodeset="/data/p_remarks" required="false()" type="string"/>
            <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/start_at" type="dateTime"/>
            <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/end_at" type="dateTime"/>
            <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
