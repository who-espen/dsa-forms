            <bind nodeset="/data/p_district" required="true()" type="select1"/>
            <bind nodeset="/data/p_village_id" required="true()" type="string"/>
            <bind nodeset="/data/p_particiant_id" required="true()" type="string" calculate="concat(if(string-length(/data/p_district) > 3, substr(/data/p_district,0,3), ''),'_', /data/meta/randomnum)" readonly="true()" />
            <bind nodeset="/data/p_enrollment" required="true()" type="select1"/>
            <bind nodeset="/data/p_sex" relevant=" /data/p_enrollment =&quot;Consentement&quot;" required="true()" type="select1"/>
            <bind constraint=".&gt;=15 and .&lt;135" jr:constraintMsg="jr:itext('/data/p_age:jr:constraintMsg')" nodeset="/data/p_age" relevant=" /data/p_enrollment =&quot;Consentement&quot;" required="true()" type="int"/>
            <bind constraint=".&lt;= /data/p_age " jr:constraintMsg="jr:itext('/data/p_lived_in_village:jr:constraintMsg')" nodeset="/data/p_lived_in_village" relevant=" /data/p_enrollment =&quot;Consentement&quot;" required="true()" type="int"/>
