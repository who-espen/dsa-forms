      <bind nodeset="/data/code_site" relevant=" /data/site != 'Autre'" required="true()" type="string" db_get="col_4" db_filter_by_col_1="/data/ue"/>
      <bind nodeset="/data/site2" relevant="selected( /data/site ,'Autre')" required="true()" type="string" db_get="col_5" db_filter_by_col_1="/data/ue"/>
      <bind nodeset="/data/code_site2" relevant="selected( /data/site ,'Autre')" required="true()" type="string" db_get="col_6" db_filter_by_col_1="/data/ue"/>
      <bind nodeset="/data/consentement" required="true()" type="string"/>
      <bind calculate="concat( /data/code_site ,  /data/code_site2 , '-', substr(random(),3,9))" jr:constraintMsg="jr:itext('/data/code_id:jr:constraintMsg')" nodeset="/data/code_id" readonly="true()" relevant=" /data/consentement  = 'Oui'" required="true()" type="string"/>
      <bind constraint=".&gt;4 and .&lt;120" jr:constraintMsg="jr:itext('/data/age:jr:constraintMsg')" nodeset="/data/age" relevant=" /data/consentement  = 'Oui'" required="true()" type="int"/>
