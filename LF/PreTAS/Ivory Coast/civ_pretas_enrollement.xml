      <bind nodeset="/data/ivm_alb" relevant=" /data/consentement  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/avm_alb_derniere_campagne" relevant=" /data/ivm_alb ='Oui' and  /data/consentement  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/nb_fois_ivm_alb" relevant=" /data/ivm_alb ='Oui' and  /data/consentement  = 'Oui'" required="true()" type="int"/>
      <bind nodeset="/data/migration" relevant=" /data/consentement  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/migration_origine" relevant=" /data/migration ='Non' and  /data/consentement  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/migration_rigine_autre" relevant="selected( /data/migration_origine , 'Autre.region.de.la.cote.d.ivoire') and  /data/consentement  = 'Oui'" type="string"/>
