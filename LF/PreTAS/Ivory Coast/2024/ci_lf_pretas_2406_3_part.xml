      <bind constraint=". &gt; 9 and . &lt; 100" jr:constraintMsg="jr:itext('/data/p_recorder:jr:constraintMsg')" nodeset="/data/p_recorder" required="true()" type="int"/>
      <bind nodeset="/data/p_region" required="true()" type="string" db_get="col_1"/>
      <bind nodeset="/data/p_district" required="true()" type="string" db_get="col_2" db_filter_by_col_1="/data/p_region"/>
      <bind nodeset="/data/p_site" required="true()" type="string" db_get="col_3" db_filter_by_col_2="/data/p_district"/>
      <bind nodeset="/data/p_site_id" required="true()" type="string" db_get="col_4" db_filter_by_col_3="/data/p_site"/>
      <bind nodeset="/data/p_numero_menage" required="true()" type="string" db_get="col_5" db_filter_by_col_4="/data/p_site_id"/>
      <bind nodeset="/data/p_consent" required="true()" type="select1"/>
      <bind nodeset="/data/p_sex" relevant=" /data/p_consent  = 'Oui'" required="true()" type="select1"/>
      <bind constraint=". &gt;= 20" jr:constraintMsg="jr:itext('/data/p_age_yrs:jr:constraintMsg')" nodeset="/data/p_age_yrs" relevant=" /data/p_consent  = 'Oui'" required="true()" type="int"/>
      <bind constraint=". &lt;=  /data/p_age_yrs " jr:constraintMsg="jr:itext('/data/p_how_long_lived:jr:constraintMsg')" nodeset="/data/p_how_long_lived" relevant=" /data/p_consent  = 'Oui'" required="true()" type="int"/>
      <bind constraint=". &gt; 0 and . &lt;= 410" jr:constraintMsg="jr:itext('/data/p_num:jr:constraintMsg')" nodeset="/data/p_num" relevant=" /data/p_consent  = 'Oui'" required="true()" type="string"/>
      <bind calculate="concat( /data/p_recorder , '_',  /data/p_site_id , '_',  /data/p_num )" nodeset="/data/p_code_id" readonly="true()" relevant=" /data/p_consent  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/p_tps_ds_village" relevant=" /data/p_consent  = 'Oui'" required="true()" type="select1"/>
      <bind nodeset="/data/p_tps_annee" relevant=" /data/p_consent  = 'Oui' and  /data/p_tps_ds_village  = 'Plus d.une année'" required="true()" type="int"/>
      <bind nodeset="/data/p_tps_mois" relevant=" /data/p_consent  = 'Oui' and  /data/p_tps_ds_village  = 'Moins d.une année'" required="true()" type="int"/>
      <bind nodeset="/data/p_niveau_etude" relevant=" /data/p_consent  = 'Oui'" required="true()" type="select1"/>
      <bind nodeset="/data/p_profession" relevant=" /data/p_consent  = 'Oui'" required="true()" type="select1"/>
      <bind nodeset="/data/p_nb_fois_pris_med_lf" relevant=" /data/p_consent  = 'Oui'" required="true()" type="select1"/>
      <bind nodeset="/data/p_raison_pas_pris_med" relevant=" /data/p_nb_fois_pris_med_lf  = 'Jamais'" required="true()" type="select"/>
      <bind nodeset="/data/p_ending_survey_note" readonly="true()" relevant=" /data/p_consent  = 'Non'" type="string"/>
      <bind nodeset="/data/p_notes" relevant=" /data/p_consent  = 'Oui'" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/p_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/p_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
