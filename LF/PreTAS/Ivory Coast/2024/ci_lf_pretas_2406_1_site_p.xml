      <bind calculate="if( /data/c_site1  = 'Autre',  /data/c_site2 ,  /data/c_site1 )" nodeset="/data/c_site" readonly="true()" type="string" db_add_col_3="." db_add_col_2="/data/c_district"/>
      <bind calculate="if( /data/c_site1  = 'Autre',  /data/c_site_id2 ,  /data/c_site_id1 )" nodeset="/data/c_site_id" readonly="true()" type="string" db_add_col_4="." db_add_col_3="/data/c_site"/>
