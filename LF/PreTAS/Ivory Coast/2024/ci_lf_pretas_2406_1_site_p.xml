      <bind calculate="position(..)" nodeset="/data/lr_p_2406_1/C2" type="string"/>
      <bind calculate="if ( ../C2  = 1,'',substring-after( /data/C1 , ../p_num ))" nodeset="/data/lr_p_2406_1/C3" type="string"/>
      <bind nodeset="/data/lr_p_2406_1/p_consent" required="true()" type="select1"/>
      <bind nodeset="/data/lr_p_2406_1/p_sex" relevant=" ../p_consent  = 'Oui' and  /data/c_consent  = 'Oui'" required="true()" type="select1"/>
