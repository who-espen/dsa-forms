      <bind constraint="not(selected( ../C3 ,  ../p_num )) and . &gt; 0 and . &lt;= 410" jr:constraintMsg="jr:itext('/data/lr_p_2406_1/p_num:jr:constraintMsg')" nodeset="/data/lr_p_2406_1/p_num" relevant=" ../p_consent  = 'Oui' and  /data/c_consent  = 'Oui'" required="true()" type="string"/>
      <bind calculate="if( /data/c_site  = 'Autre', concat( /data/c_recorder , '-',  /data/c_site_id2 , '-' , ../p_num ), concat( /data/c_recorder , '-',  /data/c_site_id , '-' , ../p_num ))" nodeset="/data/lr_p_2406_1/p_code_id" readonly="true()" relevant=" ../p_consent  = 'Oui' and  /data/c_consent  = 'Oui'" required="true()" type="string"/>
