      <bind nodeset="/data/ci_p_2406_3/p_ending_survey_note" readonly="true()" relevant=" ../p_consent  = 'Non'" type="string"/>
      <bind nodeset="/data/ci_p_2406_3/p_notes" relevant=" ../p_consent  = 'Oui'" type="text"/>
