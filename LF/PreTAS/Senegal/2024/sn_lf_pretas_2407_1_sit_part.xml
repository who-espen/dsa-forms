          <item>
            <itextId>static_instance-site_list-0</itextId>
            <commune_list>Doumga Lao</commune_list>
            <name>Wordé</name>
          </item>
          <item>
            <itextId>static_instance-site_list-1</itextId>
            <commune_list>Galoya</commune_list>
            <name>Galoya</name>
          </item>
        </root>
      </instance>
