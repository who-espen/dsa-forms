        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <input ref="/data/c_cluster_leader_name">
      <label ref="jr:itext('/data/c_cluster_leader_name:label')"/>
    </input>
    <select1 ref="/data/c_consent">
