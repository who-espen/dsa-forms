      </item>
    </select1>
    <input ref="/data/c_population">
      <label ref="jr:itext('/data/c_population:label')"/>
    </input>
    <input ref="/data/c_present">
      <label ref="jr:itext('/data/c_present:label')"/>
    </input>
    <input ref="/data/c_gps">
      <label ref="jr:itext('/data/c_gps:label')"/>
      <hint ref="jr:itext('/data/c_gps:hint')"/>
    </input>
    <input ref="/data/p_ending_survey_note">
      <label ref="jr:itext('/data/p_ending_survey_note:label')"/>
    </input>
    <input ref="/data/p_nb_part">
      <label ref="jr:itext('/data/p_nb_part:label')"/>
    </input>
    <group ref="/data/sn_lf_p_2407_3">
      <label ref="jr:itext('/data/sn_lf_p_2407_3:label')"/>
      <repeat jr:count=" /data/sn_lf_p_2407_3_count " nodeset="/data/sn_lf_p_2407_3">
        <select1 ref="/data/sn_lf_p_2407_3/p_consent">
          <label ref="jr:itext('/data/sn_lf_p_2407_3/p_consent:label')"/>
          <item>
            <label ref="jr:itext('/data/sn_lf_p_2407_3/p_consent/Oui:label')"/>
            <value>Oui</value>
          </item>
          <item>
            <label ref="jr:itext('/data/sn_lf_p_2407_3/p_consent/Non:label')"/>
            <value>Non</value>
          </item>
        </select1>
        <select1 ref="/data/sn_lf_p_2407_3/p_IDMethod">
          <label ref="jr:itext('/data/sn_lf_p_2407_3/p_IDMethod:label')"/>
          <item>
            <label ref="jr:itext('/data/sn_lf_p_2407_3/p_IDMethod/Scanner:label')"/>
            <value>Scanner</value>
          </item>
          <item>
            <label ref="jr:itext('/data/sn_lf_p_2407_3/p_IDMethod/Manuel:label')"/>
            <value>Manuel</value>
          </item>
        </select1>
        <input ref="/data/sn_lf_p_2407_3/p_Barcode_id">
          <label ref="jr:itext('/data/sn_lf_p_2407_3/p_Barcode_id:label')"/>
        </input>
        <input ref="/data/sn_lf_p_2407_3/p_manual_code_id">
          <label ref="jr:itext('/data/sn_lf_p_2407_3/p_manual_code_id:label')"/>
        </input>
        <input ref="/data/sn_lf_p_2407_3/p_manual_code_id2">
          <label ref="jr:itext('/data/sn_lf_p_2407_3/p_manual_code_id2:label')"/>
        </input>
        <input ref="/data/sn_lf_p_2407_3/p_code_id">
          <label ref="jr:itext('/data/sn_lf_p_2407_3/p_code_id:label')"/>
        </input>
        <select1 ref="/data/sn_lf_p_2407_3/p_sex">
          <label ref="jr:itext('/data/sn_lf_p_2407_3/p_sex:label')"/>
          <item>
            <label ref="jr:itext('/data/sn_lf_p_2407_3/p_sex/Masculin:label')"/>
            <value>Masculin</value>
          </item>
          <item>
            <label ref="jr:itext('/data/sn_lf_p_2407_3/p_sex/Féminin:label')"/>
