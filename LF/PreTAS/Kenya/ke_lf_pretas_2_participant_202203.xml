<?xml version="1.0" encoding="UTF-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms"
  xmlns:ev="http://www.w3.org/2001/xml-events"
  xmlns:h="http://www.w3.org/1999/xhtml"
  xmlns:jr="http://openrosa.org/javarosa"
  xmlns:odk="http://www.opendatakit.org/xforms"
  xmlns:orx="http://openrosa.org/xforms"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>2. Kenya - Pre TAS LF Participants Form V4</h:title>
    <model >

      <instance>
        <data id="ke_lf_pretas_2_participant_202203_v4">
          <p_enumerator/>
          <p_county/>
          <p_subcounty/>
          <p_cluster_name/>
          <p_cluster_id/>
          <p_consent/>
          <p_IDMethod/>
          <p_code_id/>
          <p_code_id2/>
          <p_BarcodeID/>
          <p_sex/>
          <p_age_yrs/>
          <p_dob_>
            <year/>
            <month/>
            <date/>
          </p_dob_>
          <p_lived_last_1yr/>
          <p_oth_recent_subcounties/>
          <p_length_reside/>
          <p_bednet/>
          <p_bednet_lastnight/>
          <p_previous_trt/>
          <p_ending_survey_note/>
          <p_notes/>
          <p_start/>
          <p_end/>
          <meta>
            <instanceID/>
          </meta>
        </data>
      </instance>

      <itext>
        <translation default="true()" lang="English">
          <text id="/data/p_enumerator:label">
            <value>Enumerator</value>
          </text>
          <text id="/data/p_enumerator:hint">
            <value>Write first and last name</value>
          </text>
          <text id="/data/p_county:label">
            <value>Select your county</value>
          </text>
          <text id="/data/p_subcounty:label">
            <value>Select your subcounty</value>
          </text>
          <text id="/data/p_cluster_name:label">
            <value>Select the site</value>
          </text>
          <text id="/data/p_cluster_id:label">
            <value>Select the site ID</value>
          </text>
          <text id="/data/p_consent:label">
            <value>Does respondent provide consent?</value>
          </text>
          <text id="/data/p_consent/Yes:label">
            <value>Yes</value>
          </text>
          <text id="/data/p_consent/No:label">
            <value>No</value>
          </text>
          <text id="/data/p_IDMethod:label">
            <value>How will the individual's unique ID be generated?</value>
          </text>
          <text id="/data/p_IDMethod/Scanner:label">
            <value>Scanner</value>
          </text>
          <text id="/data/p_IDMethod/ID_generation:label">
            <value>Manual</value>
          </text>
          <text id="/data/p_code_id:label">
            <value>Please record the following unique ID code for the respondent on a separate list and on each diagnostic test(s) administered</value>
          </text>
          <text id="/data/p_code_id2:jr:constraintMsg">
            <value>The repeated code must be hte same</value>
          </text>
          <text id="/data/p_code_id2:label">
            <value>Please repeat the unique ID</value>
          </text>
          <text id="/data/p_BarcodeID:label">
            <value>Scan barcode now</value>
          </text>
          <text id="/data/p_sex:label">
            <value>Sex of respondent</value>
          </text>
          <text id="/data/p_sex/Male:label">
            <value>Male</value>
          </text>
          <text id="/data/p_sex/Female:label">
            <value>Female</value>
          </text>
          <text id="/data/p_age_yrs:jr:constraintMsg">
            <value>The age must be between 5 and 120 years</value>
          </text>
          <text id="/data/p_age_yrs:label">
            <value>Enter age in years</value>
          </text>
          <text id="/data/p_dob_:label">
            <value>Date of birth</value>
          </text>
          <text id="/data/p_dob_/year:jr:constraintMsg">
            <value>The year must be between 2017 and 1902</value>
          </text>
          <text id="/data/p_dob_/year:label">
            <value>Year of birth</value>
          </text>
          <text id="/data/p_dob_/month:label">
            <value>Month of birth</value>
          </text>
          <text id="/data/p_dob_/month/January:label">
            <value>January</value>
          </text>
          <text id="/data/p_dob_/month/February:label">
            <value>February</value>
          </text>
          <text id="/data/p_dob_/month/March:label">
            <value>March</value>
          </text>
          <text id="/data/p_dob_/month/April:label">
            <value>April</value>
          </text>
          <text id="/data/p_dob_/month/May:label">
            <value>May</value>
          </text>
          <text id="/data/p_dob_/month/June:label">
            <value>June</value>
          </text>
          <text id="/data/p_dob_/month/July:label">
            <value>July</value>
          </text>
          <text id="/data/p_dob_/month/August:label">
            <value>August</value>
          </text>
          <text id="/data/p_dob_/month/September:label">
            <value>September</value>
          </text>
          <text id="/data/p_dob_/month/October:label">
            <value>October</value>
          </text>
          <text id="/data/p_dob_/month/November:label">
            <value>November</value>
          </text>
          <text id="/data/p_dob_/month/December:label">
            <value>December</value>
          </text>
          <text id="/data/p_dob_/date:jr:constraintMsg">
            <value>Please enter a valide day</value>
          </text>
          <text id="/data/p_dob_/date:label">
            <value>Day on Birth</value>
          </text>
          <text id="/data/p_lived_last_1yr:label">
            <value>A1. Have you lived in this Sub-County for the last 1 year?</value>
          </text>
          <text id="/data/p_lived_last_1yr/Yes:label">
            <value>Yes</value>
          </text>
          <text id="/data/p_lived_last_1yr/No:label">
            <value>No</value>
          </text>
          <text id="/data/p_oth_recent_subcounties:label">
            <value>A2. If no, which is the last recent other Sub-County you lived in?</value>
          </text>
          <text id="/data/p_length_reside:jr:constraintMsg">
            <value>The value must not be greater than the age</value>
          </text>
          <text id="/data/p_length_reside:label">
            <value>A3. For how long did you reside in that recent last Sub-County?</value>
          </text>
          <text id="/data/p_bednet:label">
            <value>B1. Do you normally sleep under a bed net?</value>
          </text>
          <text id="/data/p_bednet/Yes:label">
            <value>Yes</value>
          </text>
          <text id="/data/p_bednet/No:label">
            <value>No</value>
          </text>
          <text id="/data/p_bednet_lastnight:label">
            <value>B2. Did you sleep under a bed net last night?</value>
          </text>
          <text id="/data/p_bednet_lastnight/Yes:label">
            <value>Yes</value>
          </text>
          <text id="/data/p_bednet_lastnight/No:label">
            <value>No</value>
          </text>
          <text id="/data/p_previous_trt:label">
            <value>C1. Including this year, how many times have you taken pills for LF?</value>
          </text>
          <text id="/data/p_ending_survey_note:label">
            <value>You just entered a value that will end the survey</value>
          </text>
          <text id="/data/p_notes:label">
            <value>Additional Notes</value>
          </text>
        </translation>
      </itext>

      <bind nodeset="/data/p_enumerator" required="true()" type="string"/>
      <bind nodeset="/data/p_county" required="true()" type="string" db_get="col_1"/>
      <bind nodeset="/data/p_subcounty" required="true()" type="string" db_get="col_2" db_filter_by_col_1="/data/p_county"/>
      <bind nodeset="/data/p_cluster_name" required="true()" type="string" db_get="col_3" db_filter_by_col_2="/data/p_subcounty"/>
      <bind nodeset="/data/p_cluster_id" required="true()" type="string" db_get="col_4" db_filter_by_col_3="/data/p_cluster_name"/>
      <bind nodeset="/data/p_consent" required="true()" type="string"/>
      <bind nodeset="/data/p_IDMethod" relevant=" /data/p_consent  = 'Yes'" required="true()" type="string"/>
      <bind nodeset="/data/p_code_id" relevant=" /data/p_consent  = 'Yes' and  /data/p_IDMethod != 'Scanner'" required="true()" type="string"/>
      <bind constraint=". =  /data/p_code_id " jr:constraintMsg="jr:itext('/data/p_code_id2:jr:constraintMsg')" nodeset="/data/p_code_id2" relevant=" /data/p_consent  = 'Yes' and  /data/p_IDMethod != 'Scanner'" required="true()" type="string"/>
      <bind nodeset="/data/p_BarcodeID" relevant=" /data/p_consent  = 'Yes' and  /data/p_IDMethod  = 'Scanner'" required="true()" type="barcode"/>
      <bind nodeset="/data/p_sex" relevant=" /data/p_consent  = 'Yes'" required="true()" type="string"/>
      <bind constraint=". &gt;= 5 and . &lt;= 120" jr:constraintMsg="jr:itext('/data/p_age_yrs:jr:constraintMsg')" nodeset="/data/p_age_yrs" relevant=" /data/p_consent  = 'Yes'" required="true()" type="int"/>
      <bind nodeset="/data/p_dob_" relevant=" /data/p_consent  = 'Yes'"/>
      <bind constraint=". &gt;= 1902 and . &lt;= 2017" jr:constraintMsg="jr:itext('/data/p_dob_/year:jr:constraintMsg')" nodeset="/data/p_dob_/year" required="true()" type="int"/>
      <bind nodeset="/data/p_dob_/month" required="true()" type="string"/>
      <bind constraint=". &gt; 0 and . &lt; 32" jr:constraintMsg="jr:itext('/data/p_dob_/date:jr:constraintMsg')" nodeset="/data/p_dob_/date" required="true()" type="int"/>
      <bind nodeset="/data/p_lived_last_1yr" relevant=" /data/p_consent  = 'Yes'" required="true()" type="string"/>
      <bind nodeset="/data/p_oth_recent_subcounties" relevant=" /data/p_consent  = 'Yes' and  /data/p_lived_last_1yr  = 'No'" required="true()" type="string"/>
      <bind constraint=". &lt;=  /data/p_age_yrs " jr:constraintMsg="jr:itext('/data/p_length_reside:jr:constraintMsg')" nodeset="/data/p_length_reside" relevant=" /data/p_consent  = 'Yes' and  /data/p_lived_last_1yr  = 'No'" required="true()" type="int"/>
      <bind nodeset="/data/p_bednet" relevant=" /data/p_consent  = 'Yes'" required="true()" type="string"/>
      <bind nodeset="/data/p_bednet_lastnight" relevant=" /data/p_consent  = 'Yes'" required="true()" type="string"/>
      <bind nodeset="/data/p_previous_trt" relevant=" /data/p_consent  = 'Yes'" required="true()" type="int"/>
      <bind nodeset="/data/p_ending_survey_note" readonly="true()" relevant=" /data/p_consent  = 'No'" type="string"/>
      <bind nodeset="/data/p_notes" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/p_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/p_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/data/p_enumerator">
      <label ref="jr:itext('/data/p_enumerator:label')"/>
      <hint ref="jr:itext('/data/p_enumerator:hint')"/>
    </input>
    <input ref="/data/p_county">
      <label ref="jr:itext('/data/p_county:label')"/>
    </input>
    <input ref="/data/p_subcounty">
      <label ref="jr:itext('/data/p_subcounty:label')"/>
    </input>
    <input ref="/data/p_cluster_name">
      <label ref="jr:itext('/data/p_cluster_name:label')"/>
    </input>
    <input ref="/data/p_cluster_id">
      <label ref="jr:itext('/data/p_cluster_id:label')"/>
    </input>
    <select1 ref="/data/p_consent">
      <label ref="jr:itext('/data/p_consent:label')"/>
      <item>
        <label ref="jr:itext('/data/p_consent/Yes:label')"/>
        <value>Yes</value>
      </item>
      <item>
        <label ref="jr:itext('/data/p_consent/No:label')"/>
        <value>No</value>
      </item>
    </select1>
    <select1 ref="/data/p_IDMethod">
      <label ref="jr:itext('/data/p_IDMethod:label')"/>
      <item>
        <label ref="jr:itext('/data/p_IDMethod/Scanner:label')"/>
        <value>Scanner</value>
      </item>
      <item>
        <label ref="jr:itext('/data/p_IDMethod/ID_generation:label')"/>
        <value>ID_generation</value>
      </item>
    </select1>
    <input ref="/data/p_code_id">
      <label ref="jr:itext('/data/p_code_id:label')"/>
    </input>
    <input ref="/data/p_code_id2">
      <label ref="jr:itext('/data/p_code_id2:label')"/>
    </input>
    <input ref="/data/p_BarcodeID">
      <label ref="jr:itext('/data/p_BarcodeID:label')"/>
    </input>
    <select1 ref="/data/p_sex">
      <label ref="jr:itext('/data/p_sex:label')"/>
      <item>
        <label ref="jr:itext('/data/p_sex/Male:label')"/>
        <value>Male</value>
      </item>
      <item>
        <label ref="jr:itext('/data/p_sex/Female:label')"/>
        <value>Female</value>
      </item>
    </select1>
    <input ref="/data/p_age_yrs">
      <label ref="jr:itext('/data/p_age_yrs:label')"/>
    </input>
    <group appearance="field-list" ref="/data/p_dob_">
      <label ref="jr:itext('/data/p_dob_:label')"/>
      <input ref="/data/p_dob_/year">
        <label ref="jr:itext('/data/p_dob_/year:label')"/>
      </input>
      <select1 ref="/data/p_dob_/month">
        <label ref="jr:itext('/data/p_dob_/month:label')"/>
        <item>
          <label ref="jr:itext('/data/p_dob_/month/January:label')"/>
          <value>January</value>
        </item>
        <item>
          <label ref="jr:itext('/data/p_dob_/month/February:label')"/>
          <value>February</value>
        </item>
        <item>
          <label ref="jr:itext('/data/p_dob_/month/March:label')"/>
          <value>March</value>
        </item>
        <item>
          <label ref="jr:itext('/data/p_dob_/month/April:label')"/>
          <value>April</value>
        </item>
        <item>
          <label ref="jr:itext('/data/p_dob_/month/May:label')"/>
          <value>May</value>
        </item>
        <item>
          <label ref="jr:itext('/data/p_dob_/month/June:label')"/>
          <value>June</value>
        </item>
        <item>
          <label ref="jr:itext('/data/p_dob_/month/July:label')"/>
          <value>July</value>
        </item>
        <item>
          <label ref="jr:itext('/data/p_dob_/month/August:label')"/>
          <value>August</value>
        </item>
        <item>
          <label ref="jr:itext('/data/p_dob_/month/September:label')"/>
          <value>September</value>
        </item>
        <item>
          <label ref="jr:itext('/data/p_dob_/month/October:label')"/>
          <value>October</value>
        </item>
        <item>
          <label ref="jr:itext('/data/p_dob_/month/November:label')"/>
          <value>November</value>
        </item>
        <item>
          <label ref="jr:itext('/data/p_dob_/month/December:label')"/>
          <value>December</value>
        </item>
      </select1>
      <input ref="/data/p_dob_/date">
        <label ref="jr:itext('/data/p_dob_/date:label')"/>
      </input>
    </group>
    <select1 ref="/data/p_lived_last_1yr">
      <label ref="jr:itext('/data/p_lived_last_1yr:label')"/>
      <item>
        <label ref="jr:itext('/data/p_lived_last_1yr/Yes:label')"/>
        <value>Yes</value>
      </item>
      <item>
        <label ref="jr:itext('/data/p_lived_last_1yr/No:label')"/>
        <value>No</value>
      </item>
    </select1>
    <input ref="/data/p_oth_recent_subcounties">
      <label ref="jr:itext('/data/p_oth_recent_subcounties:label')"/>
    </input>
    <input ref="/data/p_length_reside">
      <label ref="jr:itext('/data/p_length_reside:label')"/>
    </input>
    <select1 ref="/data/p_bednet">
      <label ref="jr:itext('/data/p_bednet:label')"/>
      <item>
        <label ref="jr:itext('/data/p_bednet/Yes:label')"/>
        <value>Yes</value>
      </item>
      <item>
        <label ref="jr:itext('/data/p_bednet/No:label')"/>
        <value>No</value>
      </item>
    </select1>
    <select1 ref="/data/p_bednet_lastnight">
      <label ref="jr:itext('/data/p_bednet_lastnight:label')"/>
      <item>
        <label ref="jr:itext('/data/p_bednet_lastnight/Yes:label')"/>
        <value>Yes</value>
      </item>
      <item>
        <label ref="jr:itext('/data/p_bednet_lastnight/No:label')"/>
        <value>No</value>
      </item>
    </select1>
    <input ref="/data/p_previous_trt">
      <label ref="jr:itext('/data/p_previous_trt:label')"/>
    </input>
    <input ref="/data/p_ending_survey_note">
      <label ref="jr:itext('/data/p_ending_survey_note:label')"/>
    </input>
    <input ref="/data/p_notes">
      <label ref="jr:itext('/data/p_notes:label')"/>
    </input>
  </h:body>
</h:html>
