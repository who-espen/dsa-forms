    <bind nodeset="/data/p_consent" type="select1"/>
    <bind nodeset="/data/p_id_method" relevant=" /data/p_consent  = '1_oui'" required="true()" type="select1"/>
    <bind nodeset="/data/p_barcode_id" relevant=" /data/p_id_method ='scanner' and  /data/p_consent  = '1_oui'" required="true()" type="barcode"/>
    <bind constraint="string-length(.)=9" jr:constraintMsg="jr:itext('/data/p_code_id:jr:constraintMsg')" nodeset="/data/p_code_id" relevant=" /data/p_id_method ='manuel' and  /data/p_consent  = '1_oui'" required="true()" type="string"/>
    <bind constraint=". =  /data/p_code_id " jr:constraintMsg="jr:itext('/data/p_code_id2:jr:constraintMsg')" nodeset="/data/p_code_id2" relevant=" /data/p_id_method ='manuel' and  /data/p_consent  = '1_oui'" required="true()" type="string"/>
    <bind constraint="regex(., '^[0-9]{3}$')" jr:constraintMsg="jr:itext('/data/p_Numero_famille:jr:constraintMsg')" nodeset="/data/p_Numero_famille" relevant=" /data/p_consent  = '1_oui'" required="true()" type="string"/>
    <bind constraint="regex(., '^[0-9]{2}$')" jr:constraintMsg="jr:itext('/data/p_Numero_Enquete:jr:constraintMsg')" nodeset="/data/p_Numero_Enquete" relevant=" /data/p_consent  = '1_oui'" required="true()" type="string"/>
    <bind calculate="concat( /data/p_cluster_id , '-', /data/p_recorder_id ,'-',  /data/p_Numero_famille ,'-', /data/p_Numero_Enquete )" nodeset="/data/p_Numero_Enquete_complet" readonly="true()" relevant=" /data/p_consent  = '1_oui'" type="string"/>
    <bind constraint=".&gt;4 and .&lt;110" jr:constraintMsg="jr:itext('/data/p_age_yrs:jr:constraintMsg')" nodeset="/data/p_age_yrs" relevant=" /data/p_consent  = '1_oui'" required="true()" type="int"/>
    <bind nodeset="/data/p_sex" relevant=" /data/p_consent  = '1_oui'" required="true()" type="select1"/>
    <bind nodeset="/data/p_ancien_positif" relevant=" /data/p_consent  = '1_oui'" required="true()" type="select1"/>
    <bind nodeset="/data/p_scolarisation" relevant=" /data/p_consent  = '1_oui'" required="true()" type="select1"/>
    <bind nodeset="/data/p_profession" relevant=" /data/p_consent  = '1_oui'" required="true()" type="select1"/>
    <bind nodeset="/data/p_dormi_sous_moustiquaire" relevant=" /data/p_consent  = '1_oui'" required="true()" type="select1"/>
    <bind constraint=". &lt;  /data/p_age_yrs " jr:constraintMsg="jr:itext('/data/p_annee_vecu_dans_village:jr:constraintMsg')" nodeset="/data/p_annee_vecu_dans_village" relevant=" /data/p_consent  = '1_oui'" required="true()" type="int"/>
    <bind constraint=". &lt;= 12" jr:constraintMsg="jr:itext('/data/p_mois_vecu_dans_village:jr:constraintMsg')" nodeset="/data/p_mois_vecu_dans_village" relevant=" /data/p_consent  = '1_oui' and  /data/p_annee_vecu_dans_village  = 0" required="true()" type="int"/>
    <bind nodeset="/data/p_deja_ete_traiter_par_ivm_alb" relevant=" /data/p_consent  = '1_oui'" required="true()" type="select1"/>
    <bind nodeset="/data/p_nb_fois_ivm_alb" relevant=" /data/p_consent  = '1_oui'" required="true()" type="select1"/>
    <bind nodeset="/data/p_jamais_ivm" relevant=" /data/p_deja_ete_traiter_par_ivm_alb ='Non' and  /data/p_consent  = '1_oui'" required="true()" type="select1"/>
    <bind nodeset="/data/p_migration" relevant=" /data/p_consent  = '1_oui'" required="true()" type="select1"/>
    <bind nodeset="/data/p_migration_autre" relevant=" /data/p_migration ='13_autre_pays' and  /data/p_consent  = '1_oui'" type="string"/>
    <bind nodeset="/data/p_lymphoedeme" relevant=" /data/p_consent  = '1_oui'" required="true()" type="select1"/>
    <bind nodeset="/data/p_hydrocele" relevant=" /data/p_sex ='M' and  /data/p_consent  = '1_oui'" required="true()" type="select1"/>
    <bind nodeset="/data/p_ending_survey_note" readonly="true()" relevant=" /data/p_consent  = '2_non'" type="string"/>
    <bind nodeset="/data/p_commentaires" type="string"/>
    <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/p_start" type="dateTime"/>
    <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/p_end" type="dateTime"/>
    <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
  </model>
