      <bind nodeset="/data/c_district" required="true()" type="string" db_add_col_1="/data/c_iu" db_add_col_2="."/>
      <bind nodeset="/data/c_cluster_name" required="true()" type="string" db_add_col_2="/data/c_district" db_add_col_3="."/>
      <bind constraint=". &gt; 99 and . &lt; 1000" jr:constraintMsg="jr:itext('/data/c_cluster_id:jr:constraintMsg')" nodeset="/data/c_cluster_id" required="true()" type="string" db_add_col_3="/data/c_cluster_name" db_add_col_4="."/>
      <bind nodeset="/data/c_site_type" required="true()" type="string"/>
      <bind nodeset="/data/c_cluster_leader" required="true()" type="string"/>
      <bind nodeset="/data/c_consent" required="true()" type="string"/>
      <bind nodeset="/data/c_gps" relevant=" /data/c_consent  = 'Yes'" required="true()" type="geopoint"/>
      <bind nodeset="/data/c_ending_survey_note" readonly="true()" relevant=" /data/c_consent  = 'No'" type="string"/>
      <bind nodeset="/data/c_ClusterNotes" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/c_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/c_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/data/c_recorder">
      <label ref="jr:itext('/data/c_recorder:label')"/>
      <hint ref="jr:itext('/data/c_recorder:hint')"/>
    </input>
    <select1 ref="/data/c_iu">
      <label ref="jr:itext('/data/c_iu:label')"/>
      <item>
        <label ref="jr:itext('/data/c_iu/Bong:label')"/>
        <value>Bong</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_iu/Grand Cape Mount:label')"/>
        <value>Grand Cape Mount</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_iu/Grand Kru:label')"/>
        <value>Grand Kru</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_iu/Margibi:label')"/>
        <value>Margibi</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_iu/Maryland:label')"/>
        <value>Maryland</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_iu/Montserrado:label')"/>
        <value>Montserrado</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_iu/Sinoe:label')"/>
        <value>Sinoe</value>
      </item>
    </select1>
    <select1 ref="/data/c_district">
      <label ref="jr:itext('/data/c_district:label')"/>
      <itemset nodeset="instance('district')/root/item[iu =  /data/c_iu ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <select1 ref="/data/c_cluster_name">
      <label ref="jr:itext('/data/c_cluster_name:label')"/>
      <itemset nodeset="instance('site')/root/item[district =  /data/c_district ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <select1 ref="/data/c_cluster_id">
      <label ref="jr:itext('/data/c_cluster_id:label')"/>
      <itemset nodeset="instance('site_id')/root/item[site=  /data/c_cluster_name ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <select1 ref="/data/c_site_type">
      <label ref="jr:itext('/data/c_site_type:label')"/>
      <itemset nodeset="instance('site_type')/root/item[site=  /data/c_cluster_name ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <input ref="/data/c_cluster_leader">
      <label ref="jr:itext('/data/c_cluster_leader:label')"/>
    </input>
    <select1 ref="/data/c_consent">
      <label ref="jr:itext('/data/c_consent:label')"/>
      <item>
        <label ref="jr:itext('/data/c_consent/Yes:label')"/>
        <value>Yes</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_consent/No:label')"/>
        <value>No</value>
      </item>
    </select1>
    <input ref="/data/c_gps">
      <label ref="jr:itext('/data/c_gps:label')"/>
      <hint ref="jr:itext('/data/c_gps:hint')"/>
    </input>
    <input ref="/data/c_ending_survey_note">
      <label ref="jr:itext('/data/c_ending_survey_note:label')"/>
    </input>
    <input ref="/data/c_ClusterNotes">
      <label ref="jr:itext('/data/c_ClusterNotes:label')"/>
    </input>
  </h:body>
</h:html>
