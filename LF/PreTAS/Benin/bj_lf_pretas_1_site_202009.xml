    <input ref="/data/c_recorder">
      <label ref="jr:itext('/data/c_recorder:label')"/>
      <hint ref="jr:itext('/data/c_recorder:hint')"/>
    </input>
    <select1 ref="/data/c_district">
      <label ref="jr:itext('/data/c_district:label')"/>
      <item>
        <label ref="jr:itext('/data/c_district/Cove:label')"/>
        <value>Cove</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_district/Ouinhi:label')"/>
        <value>Ouinhi</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_district/Zagnanado:label')"/>
        <value>Zagnanado</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_district/Zakpota:label')"/>
        <value>Zakpota</value>
      </item>
    </select1>
    <select1 ref="/data/c_cluster_name">
      <label ref="jr:itext('/data/c_cluster_name:label')"/>
      <hint ref="jr:itext('/data/c_cluster_name:hint')"/>
      <itemset nodeset="instance('site_list')/root/item[district_list =  /data/c_district ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <select1 ref="/data/c_cluster_id">
      <label ref="jr:itext('/data/c_cluster_id:label')"/>
      <itemset nodeset="instance('code_site_list')/root/item[site_list =  /data/c_cluster_name ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <input ref="/data/c_cluster_leader">
      <label ref="jr:itext('/data/c_cluster_leader:label')"/>
    </input>
    <select1 ref="/data/c_consent">
      <label ref="jr:itext('/data/c_consent:label')"/>
      <item>
        <label ref="jr:itext('/data/c_consent/Yes:label')"/>
        <value>Yes</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_consent/No:label')"/>
        <value>No</value>
      </item>
    </select1>
    <input ref="/data/c_gps">
      <label ref="jr:itext('/data/c_gps:label')"/>
      <hint ref="jr:itext('/data/c_gps:hint')"/>
    </input>
    <input ref="/data/c_ending_survey_note">
      <label ref="jr:itext('/data/c_ending_survey_note:label')"/>
    </input>
    <input ref="/data/c_ClusterNotes">
      <label ref="jr:itext('/data/c_ClusterNotes:label')"/>
    </input>
  </h:body>
</h:html>
