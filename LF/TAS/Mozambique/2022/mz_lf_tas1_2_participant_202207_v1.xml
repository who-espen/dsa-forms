			</item>
			<item>
				<label ref="jr:itext('/data/p_sex/Feminino:label')"/>
				<value>Feminino</value>
			</item>
		</select1>
		<input ref="/data/p_age_yrs">
			<label ref="jr:itext('/data/p_age_yrs:label')"/>
		</input>
		<input ref="/data/p_how_long_lived">
			<label ref="jr:itext('/data/p_how_long_lived:label')"/>
		</input>
		<select1 ref="/data/p_ID_method">
			<label ref="jr:itext('/data/p_ID_method:label')"/>
			<item>
				<label ref="jr:itext('/data/p_ID_method/Scanner:label')"/>
				<value>Scanner</value>
			</item>
			<item>
				<label ref="jr:itext('/data/p_ID_method/Manual:label')"/>
				<value>Manual</value>
			</item>
		</select1>
		<input ref="/data/p_BarcodeID1">
			<label ref="jr:itext('/data/p_BarcodeID1:label')"/>
			<hint ref="jr:itext('/data/p_BarcodeID1:hint')"/>
		</input>
		<input ref="/data/p_BarcodeID2">
			<label ref="jr:itext('/data/p_BarcodeID2:label')"/>
		</input>
		<input ref="/data/p_mosquito_prevent">
			<label ref="jr:itext('/data/p_mosquito_prevent:label')"/>
		</input>
		<select1 ref="/data/p_possess_net">
			<label ref="jr:itext('/data/p_possess_net:label')"/>
			<item>
				<label ref="jr:itext('/data/p_possess_net/Sim:label')"/>
				<value>Sim</value>
			</item>
			<item>
				<label ref="jr:itext('/data/p_possess_net/Nao:label')"/>
				<value>Nao</value>
			</item>
		</select1>
		<select1 ref="/data/p_use_net">
			<label ref="jr:itext('/data/p_use_net:label')"/>
			<item>
				<label ref="jr:itext('/data/p_use_net/Sim:label')"/>
				<value>Sim</value>
			</item>
			<item>
				<label ref="jr:itext('/data/p_use_net/Nao:label')"/>
				<value>Nao</value>
			</item>
		</select1>
		<select1 ref="/data/p_building_type">
			<label ref="jr:itext('/data/p_building_type:label')"/>
			<item>
				<label ref="jr:itext('/data/p_building_type/Construção convencional:label')"/>
				<value>Construção convencional</value>
			</item>
			<item>
				<label ref="jr:itext('/data/p_building_type/Vara e pique:label')"/>
				<value>Vara e pique</value>
			</item>
			<item>
				<label ref="jr:itext('/data/p_building_type/Madeira e zinco:label')"/>
				<value>Madeira e zinco</value>
			</item>
			<item>
				<label ref="jr:itext('/data/p_building_type/Material local:label')"/>
				<value>Material local</value>
			</item>
			<item>
				<label ref="jr:itext('/data/p_building_type/Adobe:label')"/>
				<value>Adobe</value>
			</item>
		</select1>
		<input ref="/data/p_ending_survey_note">
			<label ref="jr:itext('/data/p_ending_survey_note:label')"/>
		</input>
		<input ref="/data/p_notes">
			<label ref="jr:itext('/data/p_notes:label')"/>
		</input>
	</h:body>
</h:html>