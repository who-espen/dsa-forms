      <bind constraint="regex(.,'^[0-9]{2}$')" jr:constraintMsg="jr:itext('/data/d_recorderID:jr:constraintMsg')" nodeset="/data/d_recorderID" required="true()" type="int"/>
      <bind nodeset="/data/d_eu_name" required="true()" type="string" db_get="col_1"/>
      <bind nodeset="/data/d_district" required="true()" type="string" db_get="col_2" db_filter_by_col_1="/data/d_eu_name"/>
      <bind nodeset="/data/d_district_id" required="true()" type="string" db_get="col_3" db_filter_by_col_2="/data/d_district"/>
      <bind nodeset="/data/d_cluster_name" required="true()" type="string" db_get="col_4" db_filter_by_col_2="/data/d_district"/>
      <bind constraint=". &gt; 99 and .&lt; 1000" jr:constraintMsg="jr:itext('/data/d_cluster_id:jr:constraintMsg')" nodeset="/data/d_cluster_id" relevant=" /data/d_cluster_name != 'Autre'" required="true()" type="string" db_get="col_5" db_filter_by_col_3="/data/d_cluster_name"/>
      <bind nodeset="/data/d_cluster_name2" relevant=" /data/d_cluster_name  = 'Autre'" required="true()" type="string" db_get="col_6" db_filter_by_col_2="/data/d_district"/>
      <bind nodeset="/data/d_cluster_id2" relevant=" /data/d_cluster_name  = 'Autre'" required="true()" type="string" db_get="col_7" db_filter_by_col_3="/data/d_cluster_name"/>
      <bind constraint=". &gt; 0 and . &lt;= 410" jr:constraintMsg="jr:itext('/data/d_num:jr:constraintMsg')" nodeset="/data/d_num" required="true()" type="int"/>
      <bind constraint=". =  /data/d_num " jr:constraintMsg="jr:itext('/data/d_num2:jr:constraintMsg')" nodeset="/data/d_num2" required="true()" type="int"/>
      <bind calculate="concat( /data/d_district_id , '-',  /data/d_cluster_id , '-' , /data/d_num )" nodeset="/data/d_code_id" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/data/d_sex" required="true()" type="string"/>
      <bind constraint=". &gt;= 5 and . &lt;= 9" jr:constraintMsg="jr:itext('/data/d_age_yrs:jr:constraintMsg')" nodeset="/data/d_age_yrs" required="true()" type="int"/>
      <bind nodeset="/data/d_lotnumber1" required="true()" type="string"/>
      <bind nodeset="/data/d_result1" required="true()" type="string"/>
      <bind nodeset="/data/d_why_invalid1" relevant=" /data/d_result1  = 'Invalide'" required="true()" type="string"/>
      <bind nodeset="/data/d_why_invalid_other1" relevant="selected( /data/d_why_invalid1 ,'Other')" required="true()" type="string"/>
      <bind calculate=" /data/d_result1 " nodeset="/data/d_display_result1" required="true()" type="string"/>
      <bind nodeset="/data/d_image_1" relevant=" /data/d_result1  = 'Positif'" required="true()" type="binary"/>
      <bind nodeset="/data/d_need_second_test" readonly="true()" relevant=" /data/d_result1 != 'Negatif'" type="string"/>
      <bind nodeset="/data/d_lotnumber2" relevant=" /data/d_result1 != 'Negatif'" required="true()" type="string"/>
      <bind nodeset="/data/d_result2" relevant=" /data/d_result1 != 'Negatif'" required="true()" type="string"/>
      <bind nodeset="/data/d_why_Invalid2" relevant=" /data/d_result2  = 'Invalide'" required="true()" type="string"/>
      <bind nodeset="/data/d_why_invalid_other2" relevant="selected( /data/d_why_Invalid2 ,'Other')" required="true()" type="string"/>
      <bind calculate=" /data/d_result1 " nodeset="/data/d_display_result2" relevant=" /data/d_result1 != 'Negatif'" type="string"/>
      <bind calculate="if( /data/d_result1 =&quot;Positif&quot; and  /data/d_result2 =&quot;Positif&quot;, &quot;Positif&quot;,
 if( /data/d_result1 =&quot;Positif&quot; and  /data/d_result2 =&quot;Negatif&quot;, &quot;Indeterminate&quot;,
 if( /data/d_result1 =&quot;Positif&quot; and  /data/d_result2 =&quot;Invalide&quot;, &quot;Positif&quot;,
 if( /data/d_result1 =&quot;Invalide&quot; and  /data/d_result2 =&quot;Positif&quot;, &quot;Positif&quot;,
 if( /data/d_result1 =&quot;Invalide&quot; and  /data/d_result2 =&quot;Invalide&quot;, &quot;Indeterminate&quot;,
 if( /data/d_result1 =&quot;Invalide&quot; and  /data/d_result2 =&quot;Negatif&quot;, &quot;Negatif&quot;, &quot;Negatif&quot;)
 )
 )
 )
 )
)" nodeset="/data/d_final_result" type="string"/>
      <bind nodeset="/data/d_display_final_result" readonly="true()" type="string"/>
      <bind nodeset="/data/d_image_2" relevant=" /data/d_result2  = 'Positif'" required="true()" type="binary"/>
      <bind nodeset="/data/d_positive_why_no_tx" relevant=" /data/d_final_result  = 'Positif'" type="string"/>
      <bind nodeset="/data/notes_part" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/c_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/c_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
