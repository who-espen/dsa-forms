      <bind nodeset="/data/c_code_operateur" required="true()" type="string"/>
      <bind nodeset="/data/c_type_tas" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/data/c_site_type" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/data/c_ue" required="true()" type="string" db_add_col_1="."/>
      <bind nodeset="/data/c_ds" required="true()" type="string" db_add_col_1="/data/c_ue" db_add_col_2="."/>
      <bind nodeset="/data/c_commune" required="true()" type="string" db_add_col_2="/data/c_ds" db_add_col_3="."/>
      <bind nodeset="/data/c_siteDansListe" required="true()" type="string"/>
      <bind nodeset="/data/c_village" relevant=" /data/c_siteDansListe  = 'Yes'" required="true()" type="string"/>
      <bind nodeset="/data/c_nom_CSPS" relevant=" /data/c_siteDansListe  = 'Yes'" required="true()" type="string"/>
      <bind nodeset="/data/c_village2" relevant=" /data/c_siteDansListe  = 'No'" required="true()" type="string"/>
      <bind nodeset="/data/c_nom_CSPS2" relevant=" /data/c_siteDansListe  = 'No'" required="true()" type="string"/>
      <bind constraint=". &gt; 0 and . &lt;= 50" jr:constraintMsg="jr:itext('/data/c_num_grappe:jr:constraintMsg')" nodeset="/data/c_num_grappe" required="true()" type="int"/>
      <bind calculate="if( /data/c_siteDansListe  = 'Yes', concat( /data/c_num_grappe ,'_', /data/c_village ), concat( /data/c_num_grappe ,'_', /data/c_village2 ))" nodeset="/data/c_nom_grappe" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/data/c_gps" required="true()" type="geopoint"/>
      <bind nodeset="/data/c_commentaires" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/c_debut_enreg" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/c_fin_enreg" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
