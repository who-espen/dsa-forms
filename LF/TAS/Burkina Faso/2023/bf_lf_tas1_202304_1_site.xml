  <h:body>
    <select1 ref="/data/c_code_operateur">
      <label ref="jr:itext('/data/c_code_operateur:label')"/>
      <item>
        <label ref="jr:itext('/data/c_code_operateur/01:label')"/>
        <value>01</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_code_operateur/02:label')"/>
        <value>02</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_code_operateur/03:label')"/>
        <value>03</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_code_operateur/04:label')"/>
        <value>04</value>
      </item>
    </select1>
    <input ref="/data/c_type_tas">
      <label ref="jr:itext('/data/c_type_tas:label')"/>
      <hint ref="jr:itext('/data/c_type_tas:hint')"/>
    </input>
    <input ref="/data/c_site_type">
      <label ref="jr:itext('/data/c_site_type:label')"/>
      <hint ref="jr:itext('/data/c_site_type:hint')"/>
    </input>
    <select1 ref="/data/c_ue">
      <label ref="jr:itext('/data/c_ue:label')"/>
      <item>
        <label ref="jr:itext('/data/c_ue/BOGODOGO:label')"/>
        <value>BOGODOGO</value>
      </item>
    </select1>
    <select1 ref="/data/c_ds">
      <label ref="jr:itext('/data/c_ds:label')"/>
      <itemset nodeset="instance('ds_list')/root/item[ue_list =  /data/c_ue ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <select1 ref="/data/c_commune">
      <label ref="jr:itext('/data/c_commune:label')"/>
      <itemset nodeset="instance('commune_list')/root/item[ds_list =  /data/c_ds ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <select1 ref="/data/c_siteDansListe">
      <label ref="jr:itext('/data/c_siteDansListe:label')"/>
      <hint ref="jr:itext('/data/c_siteDansListe:hint')"/>
      <item>
        <label ref="jr:itext('/data/c_siteDansListe/Yes:label')"/>
        <value>Yes</value>
      </item>
      <item>
        <label ref="jr:itext('/data/c_siteDansListe/No:label')"/>
        <value>No</value>
      </item>
    </select1>
    <select1 ref="/data/c_village">
      <label ref="jr:itext('/data/c_village:label')"/>
      <itemset nodeset="instance('village_list')/root/item[commune_list =  /data/c_commune ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <select1 ref="/data/c_nom_CSPS">
      <label ref="jr:itext('/data/c_nom_CSPS:label')"/>
      <itemset nodeset="instance('csps_list')/root/item[commune_list =  /data/c_commune ]">
        <value ref="name"/>
        <label ref="jr:itext(itextId)"/>
      </itemset>
    </select1>
    <input ref="/data/c_village2">
      <label ref="jr:itext('/data/c_village2:label')"/>
    </input>
    <input ref="/data/c_nom_CSPS2">
      <label ref="jr:itext('/data/c_nom_CSPS2:label')"/>
    </input>
    <input ref="/data/c_num_grappe">
      <label ref="jr:itext('/data/c_num_grappe:label')"/>
    </input>
    <input ref="/data/c_nom_grappe">
      <label ref="jr:itext('/data/c_nom_grappe:label')"/>
      <hint ref="jr:itext('/data/c_nom_grappe:hint')"/>
    </input>
    <input ref="/data/c_gps">
      <label ref="jr:itext('/data/c_gps:label')"/>
      <hint ref="jr:itext('/data/c_gps:hint')"/>
    </input>
    <input ref="/data/c_commentaires">
      <label ref="jr:itext('/data/c_commentaires:label')"/>
    </input>
  </h:body>
</h:html>
