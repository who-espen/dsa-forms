      <bind constraint=". &gt;= 0 and . &lt;= 7" jr:constraintMsg="jr:itext('/data/d_duree_sejour:jr:constraintMsg')" nodeset="/data/d_duree_sejour" relevant="( /data/d_resultat_fts1  = '1_Positif' or  /data/d_resultat_fts2  = '1_Positif')" type="int"/>
      <bind constraint=". &gt;= 6 and .&lt; 12" jr:constraintMsg="jr:itext('/data/d_duree_sejour_mois:jr:constraintMsg')" nodeset="/data/d_duree_sejour_mois" relevant="( /data/d_duree_sejour  = 0) and ( /data/d_resultat_fts1  = '1_Positif' or  /data/d_resultat_fts2  = '1_Positif')" type="int"/>
