      <bind nodeset="/data/d_defis_operationnels_autre" relevant=" /data/d_defis_operationnels ='S_Autre (expliquer)'" required="true()" type="string"/>
      <bind nodeset="/data/d_observations" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/d_debut_enreg" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/d_fin_enreg" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
