      <bind constraint="regex(.,'^[A-Z]{2}[0-9]{7}$')" jr:constraintMsg="jr:itext('/data/d_barcodeID2:jr:constraintMsg')" nodeset="/data/d_barcodeID2" relevant=" /data/d_id_methode  = 'Manuel'" required="true()" type="string"/>
      <bind constraint=". =  /data/d_barcodeID2 " nodeset="/data/d_barcodeID2_bis" relevant=" /data/d_id_methode  = 'Manuel'" required="true()" type="string"/>
      <bind constraint=".&gt;=1 and .&lt;=2" nodeset="/data/d_nb_bandelette" required="true()" type="string"/>
      <bind nodeset="/data/d_lot_fts1" required="true()" type="string"/>
      <bind nodeset="/data/d_resultat_fts1" required="true()" type="string"/>
      <bind constraint="true()" nodeset="/data/d_fts1_invalid_description" relevant=" /data/d_resultat_fts1  = '3_Invalide'" required="true()" type="string"/>
      <bind constraint="true()" nodeset="/data/d_autre_expliquer_fts1" relevant=" /data/d_fts1_invalid_description ='G_Autre_expliquer'" required="true()" type="string"/>
      <bind nodeset="/data/d_photo1" orx:max-pixels="2000" relevant=" /data/d_resultat_fts1  = '1_Positif' or  /data/d_resultat_fts1  = '3_Invalide'" required="true()" type="binary"/>
      <bind nodeset="/data/d_lot_fts2" relevant=" /data/d_nb_bandelette  &gt; 1" required="true()" type="string"/>
      <bind nodeset="/data/d_resultat_fts2" relevant=" /data/d_nb_bandelette  &gt; 1" required="true()" type="string"/>
      <bind constraint="true()" nodeset="/data/d_fts2_invalid_description" relevant=" /data/d_resultat_fts2  = '3_Invalide'" required="true()" type="string"/>
