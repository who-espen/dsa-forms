    <bind nodeset="/data/p_site_name" required="true()" type="string" db_get="col_3" db_filter_by_col_2="/data/csps"/>
    <bind nodeset="/data/p_site_id" required="true()" type="string" db_get="col_4" db_filter_by_col_3="/data/p_site_name"/>
    <bind nodeset="/data/p_consent" type="select1"/>
    <bind nodeset="/data/p_id_method" relevant=" /data/p_consent  = '1_oui'" required="true()" type="select1"/>
    <bind nodeset="/data/p_barcode_id" relevant=" /data/p_id_method ='scanner' and  /data/p_consent  = '1_oui'" required="true()" type="barcode"/>
    <bind constraint="string-length(.)=10" jr:constraintMsg="jr:itext('/data/manual_code:jr:constraintMsg')" nodeset="/data/manual_code" relevant=" /data/p_id_method ='manuel' and  /data/p_consent  = '1_oui'" required="true()" type="string"/>
    <bind constraint=". =  /data/manual_code " jr:constraintMsg="jr:itext('/data/manual_code2:jr:constraintMsg')" nodeset="/data/manual_code2" relevant=" /data/p_id_method ='manuel' and  /data/p_consent  = '1_oui'" required="true()" type="string"/>
    <bind nodeset="/data/Enrolement" readonly="true()" type="string"/>
    <bind constraint="regex(., '^[0-9]{3}$')" jr:constraintMsg="jr:itext('/data/Numero_famille:jr:constraintMsg')" nodeset="/data/Numero_famille" relevant=" /data/p_consent  = '1_oui'" required="true()" type="string"/>
    <bind constraint="regex(., '^[0-9]{3}$')" jr:constraintMsg="jr:itext('/data/Numero_Enquete:jr:constraintMsg')" nodeset="/data/Numero_Enquete" relevant=" /data/p_consent  = '1_oui'" required="true()" type="string"/>
    <bind calculate="concat( /data/p_site_id , '-', /data/p_recorder_id ,'-',  /data/Numero_famille ,'-', /data/Numero_Enquete )" nodeset="/data/Numero_Enquete_complet" readonly="true()" relevant=" /data/p_consent  = '1_oui'" required="true()" type="string"/>
    <bind constraint=".&gt;4 and .&lt;110" jr:constraintMsg="jr:itext('/data/p_age_yrs:jr:constraintMsg')" nodeset="/data/p_age_yrs" relevant=" /data/p_consent  = '1_oui'" required="true()" type="int"/>
    <bind nodeset="/data/p_sex" relevant=" /data/p_consent  = '1_oui'" required="true()" type="select1"/>
    <bind nodeset="/data/Scolarisation" relevant=" /data/p_consent  = '1_oui'" required="true()" type="select1"/>
    <bind constraint=". &lt;=  /data/p_age_yrs " jr:constraintMsg="jr:itext('/data/Duree_sejour:jr:constraintMsg')" nodeset="/data/Duree_sejour" relevant=" /data/p_consent  = '1_oui'" required="true()" type="int"/>
    <bind constraint=". &lt; 12" jr:constraintMsg="jr:itext('/data/Duree_sejour_mois:jr:constraintMsg')" nodeset="/data/Duree_sejour_mois" relevant=" /data/Duree_sejour  = 0" required="true()" type="int"/>
    <bind nodeset="/data/status" relevant=" /data/p_consent  = '1_oui'" required="true()" type="select1"/>
