      <bind nodeset="/data/d_need_second_test" readonly="true()" relevant=" /data/d_result1 != 'Negative'" type="string"/>
      <bind nodeset="/data/d_result2" relevant=" /data/d_result1 != 'Negative'" required="true()" type="string"/>
      <bind nodeset="/data/d_why_invalid2" relevant=" /data/d_result2  = 'Invalid'" required="true()" type="string"/>
      <bind nodeset="/data/d_why_invalid_other2" relevant="selected( /data/d_why_invalid2 ,'Other')" required="true()" type="string"/>
      <bind calculate=" /data/d_result1 " nodeset="/data/d_display_result2" relevant=" /data/d_result1 != 'Negative'" type="string"/>
      <bind calculate="if( /data/d_result1 =&quot;Positive&quot; and  /data/d_result2 =&quot;Positive&quot;, &quot;Positive&quot;,
 if( /data/d_result1 =&quot;Positive&quot; and  /data/d_result2 =&quot;Negative&quot;, &quot;Indeterminate&quot;,
 if( /data/d_result1 =&quot;Positive&quot; and  /data/d_result2 =&quot;Invalid&quot;, &quot;Positive&quot;,
 if( /data/d_result1 =&quot;Invalid&quot; and  /data/d_result2 =&quot;Positive&quot;, &quot;Positive&quot;,
 if( /data/d_result1 =&quot;Invalid&quot; and  /data/d_result2 =&quot;Invalid&quot;, &quot;Indeterminate&quot;,
 if( /data/d_result1 =&quot;Invalid&quot; and  /data/d_result2 =&quot;Negative&quot;, &quot;Negative&quot;, &quot;Negative&quot;)
 )
 )
 )
 )
)" nodeset="/data/d_final_result" type="string"/>
      <bind nodeset="/data/d_display_final_result" readonly="true()" type="string"/>
