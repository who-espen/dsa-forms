					<text id="/data/r_generatedid:jr:constraintMsg">
						<value>Format: 4 chiffres (le code du site) suivis de 3 chiffres (le numéro d'ordre du répondant), séparés par un tiret. Sous la forme: XXXX-XXX</value>
					</text>
					<text id="/data/r_generatedid:label">
						<value>Sélectionnez le code d'identification unique du participant</value>
					</text>
					<text id="/data/r_generatedid2:jr:constraintMsg">
						<value>Ce code est différent</value>
					</text>
					<text id="/data/r_generatedid2:label">
						<value>Veuillez répéter le code d'identification unique du participant</value>
					</text>
					<text id="/data/r_generatedid2:hint">
						<value>Format: 4 chiffres (le code du site) suivis de 3 chiffres (le numéro d'ordre du répondant), séparés par un tiret. Sous la forme: XXXX-XXX</value>
					</text>
					<text id="/data/r_note2:label">
						<value>Résultats Kato Katz</value>
					</text>
					<text id="/data/r_ascaris_lumb_sa:label">
						<value>Ascaris nombre d'œufs lame A</value>
					</text>
					<text id="/data/r_ascaris_lumb_gram:label">
						<value>Ascaris nombre d'œufs par gramme de selles</value>
					</text>
					<text id="/data/r_ascaris_lumb_gram:hint">
						<value>Formule utilisée: (nombre d'œufs lame A) x24</value>
					</text>
					<text id="/data/r_hookworm_sa:label">
						<value>Ankylostome nombre d'œufs lame A</value>
					</text>
					<text id="/data/r_hookworm_gram:label">
						<value>Ankylostome nombre d'œufs par gramme de selles</value>
					</text>
					<text id="/data/r_hookworm_gram:hint">
						<value>Formule utilisée: (nombre d'œufs lame A) x24</value>
					</text>
					<text id="/data/r_trichuris_sa:label">
						<value>Trichocéphale nombre d'œufs lame A</value>
					</text>
					<text id="/data/r_trichuris_gram:label">
						<value>Trichocéphale nombre d'œufs par gramme de selles</value>
					</text>
					<text id="/data/r_trichuris_gram:hint">
						<value>Formule utilisée: (nombre d'œufs lame A) x24</value>
					</text>
					<text id="/data/r_autre:label">
						<value>Autres?</value>
					</text>
					<text id="/data/r_autre/Oui:label">
						<value>Oui</value>
					</text>
					<text id="/data/r_autre/Non:label">
						<value>Non</value>
					</text>
					<text id="/data/r_nom_autre:label">
						<value>Préciser autre:</value>
					</text>
					<text id="/data/r_autre_sa:label">
						<value>12.b. Nombre d'œufs lame A (<output value=" /data/r_nom_autre "/>)</value>
					</text>
					<text id="/data/r_autre_gram:label">
						<value>12.d. Nombre d'œufs par gramme de selles (<output value=" /data/r_nom_autre "/>)</value>
					</text>
					<text id="/data/r_autre_gram:hint">
						<value>Formule utilisée: (nombre d'œufs lame A + nombre d'œufs lame B) x12</value>
					</text>
					<text id="/data/r_remarks:label">
						<value>Notes additionnelles</value>
					</text>
				</translation>
				<translation lang="English">
					<text id="/data/r_recorderID:jr:constraintMsg">
						<value>Please enter your 2 digits code</value>
					</text>
					<text id="/data/r_recorderID:label">
						<value>Investigator code</value>
					</text>
					<text id="/data/r_recorderID:hint">
						<value>2 digits</value>
					</text>
					<text id="/data/r_province:label">
						<value>Select the province</value>
					</text>
					<text id="/data/r_eu_name:label">
						<value>Select the evaluation unit (EU)</value>
					</text>
					<text id="/data/r_commune:label">
						<value>Select the evaluation unit (EU)</value>
					</text>
					<text id="/data/r_cluster_name:label">
						<value>Enter the school Name</value>
					</text>
					<text id="/data/r_cluster_id:label">
						<value>Enter the cluster ID</value>
					</text>
					<text id="/data/r_cluster_id:hint">
						<value>The four-digit code associated with each school</value>
					</text>
					<text id="/data/r_generatedid:jr:constraintMsg">
						<value>The format should be XXXX-XXX</value>
					</text>
					<text id="/data/r_generatedid:label">
						<value>Select participant's unique ID from the diagnostic test</value>
					</text>
					<text id="/data/r_generatedid2:label">
						<value>Repeat the participant's unique ID from the diagnostic test</value>
					</text>
					<text id="/data/r_note2:label">
						<value>Kato Katz results</value>
					</text>
					<text id="/data/r_ascaris_lumb_sa:label">
						<value>Ascaris lumbricoides (eggs) – Slide A</value>
					</text>
					<text id="/data/r_ascaris_lumb_gram:label">
						<value>Ascaris - number of eggs per gram of stool</value>
					</text>
					<text id="/data/r_hookworm_sa:label">
						<value>Hookworm – Slide A</value>
					</text>
					<text id="/data/r_hookworm_gram:label">
						<value>Hookworm - number of eggs per gram of stool</value>
					</text>
					<text id="/data/r_trichuris_sa:label">
						<value>Trichuris trichura – Slide A</value>
					</text>
					<text id="/data/r_trichuris_gram:label">
						<value>Trichuris - number of eggs per gram of stool</value>
					</text>
					<text id="/data/r_autre:label">
						<value>Other?</value>
					</text>
					<text id="/data/r_autre/Oui:label">
						<value>Yes</value>
					</text>
					<text id="/data/r_autre/Non:label">
						<value>No</value>
					</text>
					<text id="/data/r_nom_autre:label">
						<value>Precise other:</value>
					</text>
					<text id="/data/r_autre_sa:label">
						<value>Number of eggs – Slide A</value>
					</text>
					<text id="/data/r_autre_gram:label">
						<value>Number of eggs per gram of stool</value>
					</text>
					<text id="/data/r_remarks:label">
						<value>Additional comment</value>
					</text>
					<text id="/data/r_generatedid2:jr:constraintMsg">
						<value>-</value>
					</text>
					<text id="/data/r_generatedid2:hint">
						<value>-</value>
					</text>
					<text id="/data/r_ascaris_lumb_gram:hint">
						<value>-</value>
					</text>
					<text id="/data/r_hookworm_gram:hint">
						<value>-</value>
					</text>
					<text id="/data/r_trichuris_gram:hint">
						<value>-</value>
					</text>
					<text id="/data/r_autre_gram:hint">
						<value>-</value>
					</text>
				</translation>
			</itext>
			<instance>
				<data id="cd_lf_tas_4_Result_kato_katz_202210_v1">
					<r_recorderID/>
					<r_province/>
					<r_eu_name/>
					<r_commune/>
					<r_cluster_name/>
					<r_cluster_id/>
					<r_generatedid/>
					<r_generatedid2/>
					<r_note2/>
					<r_ascaris_lumb_sa/>
					<r_ascaris_lumb_gram/>
					<r_hookworm_sa/>
					<r_hookworm_gram/>
					<r_trichuris_sa/>
					<r_trichuris_gram/>
					<r_autre/>
					<r_nom_autre/>
					<r_autre_sa/>
					<r_autre_gram/>
					<r_remarks/>
					<r_start/>
					<r_end/>
					<meta>
						<instanceID/>
					</meta>
				</data>
			</instance>
			<bind constraint="regex(.,'^[0-9]{2}$')" jr:constraintMsg="jr:itext('/data/r_recorderID:jr:constraintMsg')" nodeset="/data/r_recorderID" required="true()" type="string"/>
			<bind nodeset="/data/r_province" required="true()" type="string" db_get="col_1"/>
			<bind nodeset="/data/r_eu_name" required="true()" type="string" db_get="col_2" db_filter_by_col_1="/data/r_province"/>
			<bind nodeset="/data/r_commune" required="true()" type="string" db_get="col_3" db_filter_by_col_2="/data/r_eu_name"/>
			<bind nodeset="/data/r_cluster_name" required="true()" type="string" db_get="col_4" db_filter_by_col_3="/data/r_commune"/>
			<bind constraint="regex(.,'^[0-9]{4}$')" nodeset="/data/r_cluster_id" required="true()" type="string" db_get="col_5" db_filter_by_col_4="/data/r_cluster_name"/>
			<bind constraint="regex(.,'^[0-9]{4}-[0-9]{3}$')" jr:constraintMsg="jr:itext('/data/r_generatedid:jr:constraintMsg')" nodeset="/data/r_generatedid" db_get="col_6" db_filter_by_col_7="/data/r_cluster_id" required="true()" type="string"/>
			<bind constraint=". =  /data/r_generatedid " jr:constraintMsg="jr:itext('/data/r_generatedid2:jr:constraintMsg')" nodeset="/data/r_generatedid2" required="true()" type="string"/>
			<bind nodeset="/data/r_note2" readonly="true()" type="string"/>
			<bind nodeset="/data/r_ascaris_lumb_sa" required="true()" type="int"/>
			<bind calculate=" /data/r_ascaris_lumb_sa  *24" nodeset="/data/r_ascaris_lumb_gram" readonly="true()" required="true()" type="int"/>
			<bind nodeset="/data/r_hookworm_sa" required="true()" type="int"/>
			<bind calculate=" /data/r_hookworm_sa  * 24" nodeset="/data/r_hookworm_gram" readonly="true()" required="true()" type="int"/>
			<bind nodeset="/data/r_trichuris_sa" required="true()" type="int"/>
			<bind calculate=" /data/r_trichuris_sa  * 24" nodeset="/data/r_trichuris_gram" readonly="true()" required="true()" type="int"/>
			<bind nodeset="/data/r_autre" required="true()" type="string"/>
			<bind nodeset="/data/r_nom_autre" relevant=" /data/r_autre ='Oui'" required="true()" type="string"/>
			<bind nodeset="/data/r_autre_sa" relevant=" /data/r_autre ='Oui'" required="true()" type="int"/>
			<bind calculate=" /data/r_autre_sa  * 24" nodeset="/data/r_autre_gram" readonly="true()" relevant=" /data/r_autre ='Oui'" required="true()" type="int"/>
			<bind nodeset="/data/r_remarks" type="string"/>
			<bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/r_start" type="dateTime"/>
			<bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/r_end" type="dateTime"/>
			<bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
		</model>
	</h:head>
	<h:body>
		<input ref="/data/r_recorderID">
			<label ref="jr:itext('/data/r_recorderID:label')"/>
			<hint ref="jr:itext('/data/r_recorderID:hint')"/>
		</input>
		<input ref="/data/r_province">
			<label ref="jr:itext('/data/r_province:label')"/>
		</input>
		<input ref="/data/r_eu_name">
			<label ref="jr:itext('/data/r_eu_name:label')"/>
		</input>
		<input ref="/data/r_commune">
			<label ref="jr:itext('/data/r_commune:label')"/>
		</input>
		<input ref="/data/r_cluster_name">
			<label ref="jr:itext('/data/r_cluster_name:label')"/>
		</input>
		<input ref="/data/r_cluster_id">
			<label ref="jr:itext('/data/r_cluster_id:label')"/>
			<hint ref="jr:itext('/data/r_cluster_id:hint')"/>
		</input>
		<input ref="/data/r_generatedid">
			<label ref="jr:itext('/data/r_generatedid:label')"/>
		</input>
		<input ref="/data/r_generatedid2">
			<label ref="jr:itext('/data/r_generatedid2:label')"/>
			<hint ref="jr:itext('/data/r_generatedid2:hint')"/>
		</input>
		<input ref="/data/r_note2">
			<label ref="jr:itext('/data/r_note2:label')"/>
		</input>
		<input ref="/data/r_ascaris_lumb_sa">
			<label ref="jr:itext('/data/r_ascaris_lumb_sa:label')"/>
		</input>
		<input ref="/data/r_ascaris_lumb_gram">
			<label ref="jr:itext('/data/r_ascaris_lumb_gram:label')"/>
			<hint ref="jr:itext('/data/r_ascaris_lumb_gram:hint')"/>
		</input>
		<input ref="/data/r_hookworm_sa">
			<label ref="jr:itext('/data/r_hookworm_sa:label')"/>
		</input>
		<input ref="/data/r_hookworm_gram">
			<label ref="jr:itext('/data/r_hookworm_gram:label')"/>
			<hint ref="jr:itext('/data/r_hookworm_gram:hint')"/>
		</input>
		<input ref="/data/r_trichuris_sa">
			<label ref="jr:itext('/data/r_trichuris_sa:label')"/>
		</input>
		<input ref="/data/r_trichuris_gram">
			<label ref="jr:itext('/data/r_trichuris_gram:label')"/>
			<hint ref="jr:itext('/data/r_trichuris_gram:hint')"/>
		</input>
		<select1 ref="/data/r_autre">
			<label ref="jr:itext('/data/r_autre:label')"/>
			<item>
				<label ref="jr:itext('/data/r_autre/Oui:label')"/>
				<value>Oui</value>
			</item>
			<item>
				<label ref="jr:itext('/data/r_autre/Non:label')"/>
				<value>Non</value>
			</item>
		</select1>
		<input ref="/data/r_nom_autre">
			<label ref="jr:itext('/data/r_nom_autre:label')"/>
		</input>
		<input ref="/data/r_autre_sa">
			<label ref="jr:itext('/data/r_autre_sa:label')"/>
		</input>
		<input ref="/data/r_autre_gram">
			<label ref="jr:itext('/data/r_autre_gram:label')"/>
			<hint ref="jr:itext('/data/r_autre_gram:hint')"/>
		</input>
		<input ref="/data/r_remarks">
			<label ref="jr:itext('/data/r_remarks:label')"/>
		</input>
	</h:body>
</h:html>