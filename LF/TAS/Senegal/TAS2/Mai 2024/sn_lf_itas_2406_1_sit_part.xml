      <bind nodeset="/data/c_cluster_name" required="true()" type="select1" db_add_col_2="/data/c_commune" db_add_col_3="."/>
      <bind constraint=". &gt; 99 and .&lt; 1000" jr:constraintMsg="jr:itext('/data/c_cluster_id:jr:constraintMsg')" nodeset="/data/c_cluster_id" required="true()" type="select1" db_add_col_3="/data/c_cluster_name" db_add_col_4="."/>
      <bind nodeset="/data/c_cluster_leader_name" required="true()" type="string"/>
      <bind nodeset="/data/c_consent" required="true()" type="select1"/>
      <bind nodeset="/data/c_population" relevant=" /data/c_consent  = 'Oui'" required="false()" type="int"/>
      <bind constraint=". &lt;=  /data/c_population " jr:constraintMsg="jr:itext('/data/c_present:jr:constraintMsg')" nodeset="/data/c_present" relevant=" /data/c_consent  = 'Oui'" required="false()" type="int"/>
      <bind nodeset="/data/c_gps" relevant=" /data/c_consent  = 'Oui'" required="true()" type="geopoint"/>
      <bind nodeset="/data/p_ending_survey_note" readonly="true()" relevant=" /data/c_consent  = 'Non'" type="string"/>
      <bind nodeset="/data/p_nb_part" relevant=" /data/c_consent  = 'Oui'" required="true()" type="int"/>
      <bind calculate="join(' ',  /data/sn_lf_p_2406/p_code_id )" nodeset="/data/C1" type="text"/>
      <bind calculate=" /data/p_nb_part " nodeset="/data/sn_lf_p_2406_count" readonly="true()" type="string"/>
      <bind nodeset="/data/sn_lf_p_2406" relevant=" /data/c_consent  = 'Oui'"/>
      <bind calculate="position(..)" nodeset="/data/sn_lf_p_2406/C2" type="text"/>
      <bind calculate="if ( ../C2  = 1,'',substring-after( /data/C1 , ../p_code_id ))" nodeset="/data/sn_lf_p_2406/C3" type="text"/>
      <bind nodeset="/data/sn_lf_p_2406/p_consent" required="true()" type="select1"/>
      <bind nodeset="/data/sn_lf_p_2406/p_IDMethod" relevant=" ../p_consent  = 'Oui' and  /data/c_eu_name  = 'SARAYA'" required="true()" type="select1"/>
      <bind nodeset="/data/sn_lf_p_2406/p_Barcode_id" relevant=" ../p_IDMethod  = 'Scanner' and  ../p_consent  = 'Oui' and  /data/c_eu_name  = 'SARAYA'" required="true()" type="barcode"/>
      <bind constraint="if( /data/c_eu_name  = 'SARAYA', regex(., '^(SENITAS)\d{4}$'), true())" jr:constraintMsg="jr:itext('/data/sn_lf_p_2406/p_manual_code_id:jr:constraintMsg')" nodeset="/data/sn_lf_p_2406/p_manual_code_id" relevant="( ../p_IDMethod  = 'Manuel' or  /data/c_eu_name != 'SARAYA') and  ../p_consent  = 'Oui'" required="true()" type="string"/>
      <bind constraint=". =  ../p_manual_code_id " jr:constraintMsg="jr:itext('/data/sn_lf_p_2406/p_manual_code_id2:jr:constraintMsg')" nodeset="/data/sn_lf_p_2406/p_manual_code_id2" relevant="( ../p_IDMethod  = 'Manuel' or  /data/c_eu_name != 'SARAYA') and  ../p_consent  = 'Oui'" required="true()" type="string"/>
      <bind calculate="if( ../p_IDMethod  = 'Scanner', concat( ../p_Barcode_id ),
 if( /data/c_eu_name  = 'SARAYA',  ../p_manual_code_id , concat( /data/c_recorder , '_',  /data/c_cluster_id , '_',  ../p_manual_code_id ))
)" constraint="not(selected( ../C3 ,  ../p_code_id ))" jr:constraintMsg="jr:itext('/data/sn_lf_p_2406/p_code_id:jr:constraintMsg')" nodeset="/data/sn_lf_p_2406/p_code_id" readonly="true()" relevant=" ../p_consent  = 'Oui'" required="true()" type="string"/>
      <bind nodeset="/data/sn_lf_p_2406/p_sex" relevant=" ../p_consent  = 'Oui'" required="true()" type="select1"/>
      <bind constraint=". &gt;= 5 and . &lt;= 9" jr:constraintMsg="jr:itext('/data/sn_lf_p_2406/p_age_yrs:jr:constraintMsg')" nodeset="/data/sn_lf_p_2406/p_age_yrs" relevant=" ../p_consent  = 'Oui'" required="true()" type="int"/>
      <bind constraint=". &lt;=  ../p_age_yrs " jr:constraintMsg="jr:itext('/data/sn_lf_p_2406/p_how_long_lived:jr:constraintMsg')" nodeset="/data/sn_lf_p_2406/p_how_long_lived" relevant=" ../p_consent  = 'Oui'" required="true()" type="int"/>
      <bind nodeset="/data/sn_lf_p_2406/p_class" relevant=" ../p_consent  = 'Oui'" required="true()" type="select1"/>
      <bind nodeset="/data/sn_lf_p_2406/p_depist_lf" relevant=" ../p_consent  = 'Oui'" required="true()" type="select1"/>
      <bind constraint="if( /data/c_eu_name  = 'SARAYA', . = 'Oui', . = 'Non')" jr:constraintMsg="jr:itext('/data/sn_lf_p_2406/p_depist_oncho:jr:constraintMsg')" nodeset="/data/sn_lf_p_2406/p_depist_oncho" relevant=" ../p_consent  = 'Oui'" required="true()" type="select1"/>
      <bind nodeset="/data/sn_lf_p_2406/p_ending_survey_note2" readonly="true()" relevant=" ../p_consent  = 'Non'" type="string"/>
      <bind nodeset="/data/sn_lf_p_2406/p_notes" type="text"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/c_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/c_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
    
