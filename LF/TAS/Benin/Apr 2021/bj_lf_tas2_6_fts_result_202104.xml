      <bind nodeset="/data/d_need_third_test" readonly="true()" relevant="( /data/d_result1  = 'Positif' and  /data/d_result2  = 'Négatif')
or ( /data/d_result1  = 'Positif' and  /data/d_result2  = 'Invalide')
or ( /data/d_result1  = 'Invalide' and  /data/d_result2  = 'Positif')" type="string"/>
      <bind nodeset="/data/d_lotnumber3" relevant="( /data/d_result1  = 'Positif' and  /data/d_result2  = 'Négatif')
or ( /data/d_result1  = 'Positif' and  /data/d_result2  = 'Invalide')
or ( /data/d_result1  = 'Invalide' and  /data/d_result2  = 'Positif')" required="true()" type="string"/>
