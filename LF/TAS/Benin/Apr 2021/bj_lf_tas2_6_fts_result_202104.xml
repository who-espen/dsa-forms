or ( /data/d_result1  = 'Positif' and  /data/d_result2  = 'Invalide')
or ( /data/d_result1  = 'Invalide' and  /data/d_result2  = 'Positif')" type="string"/>
      <bind calculate="if( /data/d_result1 =&quot;Positif&quot; and  /data/d_result2 =&quot;Positif&quot;, &quot;Positif&quot;,
 if( /data/d_result1 =&quot;Positif&quot; and  /data/d_result2 =&quot;Négatif&quot; and  /data/d_result3 =&quot;Négatif&quot;, &quot;Négatif&quot;,
 if( /data/d_result1 =&quot;Positif&quot; and  /data/d_result2 =&quot;Négatif&quot; and  /data/d_result3 =&quot;Positif&quot;, &quot;Positif&quot;,
 if( /data/d_result1 =&quot;Positif&quot; and  /data/d_result2 =&quot;Invalide&quot; and  /data/d_result3 =&quot;Négatif&quot;, &quot;Négatif&quot;,
 if( /data/d_result1 =&quot;Positif&quot; and  /data/d_result2 =&quot;Invalide&quot; and  /data/d_result3 =&quot;Positif&quot;, &quot;Positif&quot;,
 if( /data/d_result1 =&quot;Invalide&quot; and  /data/d_result2 =&quot;Positif&quot; and  /data/d_result3 =&quot;Positif&quot;, &quot;Positif&quot;,
 if( /data/d_result1 =&quot;Invalide&quot; and  /data/d_result2 =&quot;Positif&quot; and  /data/d_result3 =&quot;Négatif&quot;, &quot;Négatif&quot;,
 if( /data/d_result1 =&quot;Invalide&quot; and  /data/d_result2 =&quot;Invalide&quot;, &quot;Invalide&quot;,
 if( /data/d_result1 =&quot;Invalide&quot; and  /data/d_result2 =&quot;Négatif&quot;, &quot;Négatif&quot;,
 if( /data/d_result1 =&quot;Négatif&quot; , &quot;Négatif&quot;, &quot;Indéterminé&quot; )
 )
 )
 )
 )
 )
 )
 )
 )
)" nodeset="/data/d_final_result" relevant=" /data/d_result1 != 'Negative'" type="string"/>
      <bind nodeset="/data/d_display_final_result" readonly="true()" relevant=" /data/d_result1 != 'Negative'" type="string"/>
      <bind nodeset="/data/d_positive_why_no_tx" relevant=" /data/d_final_result  = 'Positif'" type="string"/>
      <bind nodeset="/data/notes_part" type="string"/>
