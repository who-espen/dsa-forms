      <bind nodeset="/data/gw_fts_202402_v1/d_add_results" required="true()" type="select1"/>
      <bind constraint="not(selected( ../C3 ,  ../d_code_id ))" jr:constraintMsg="jr:itext('/data/gw_fts_202402_v1/d_code_id:jr:constraintMsg')" nodeset="/data/gw_fts_202402_v1/d_code_id" relevant=" ../d_add_results  = 'Sim'" required="true()" type="string" db_get="col_6" db_filter_by_col_5="/data/d_cluster_id"/>
      <bind constraint=". =  ../d_code_id " jr:constraintMsg="jr:itext('/data/gw_fts_202402_v1/d_code_id2:jr:constraintMsg')" nodeset="/data/gw_fts_202402_v1/d_code_id2" relevant=" ../d_add_results  = 'Sim'" required="true()" type="string"/>
