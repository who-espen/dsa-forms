      <bind nodeset="/data/gw_fts_202402_v1/d_result1" relevant=" ../d_add_results  = 'Sim'" required="true()" type="select1"/>
      <bind nodeset="/data/gw_fts_202402_v1/d_why_invalid1" relevant=" ../d_result1  = 'Invalid'" required="true()" type="select"/>
      <bind nodeset="/data/gw_fts_202402_v1/d_why_invalid_other1" relevant="selected( ../d_why_invalid1 ,'Other') and  ../d_add_results  = 'Sim'" required="true()" type="string"/>
      <bind calculate=" ../d_result1 " nodeset="/data/gw_fts_202402_v1/d_display_result1" required="true()" type="string"/>
      <bind nodeset="/data/gw_fts_202402_v1/d_need_second_test" readonly="true()" relevant=" ../d_result1 != 'Negativo' and  ../d_add_results  = 'Sim'" type="string"/>
      <bind constraint="regex(.,'^[0-9]{6}$')" jr:constraintMsg="jr:itext('/data/gw_fts_202402_v1/d_lotnumber2:jr:constraintMsg')" nodeset="/data/gw_fts_202402_v1/d_lotnumber2" relevant=" ../d_result1 != 'Negativo' and  ../d_add_results  = 'Sim'" required="true()" type="string"/>
      <bind nodeset="/data/gw_fts_202402_v1/d_result2" relevant=" ../d_result1 != 'Negativo' and  ../d_add_results  = 'Sim'" required="true()" type="select1"/>
      <bind nodeset="/data/gw_fts_202402_v1/d_why_invalid2" relevant=" ../d_result2  = 'Invalid' and  ../d_add_results  = 'Sim'" required="true()" type="select"/>
      <bind nodeset="/data/gw_fts_202402_v1/d_why_invalid_other2" relevant="selected( ../d_why_invalid2 ,'Other') and  ../d_add_results  = 'Sim'" required="true()" type="string"/>
      <bind calculate=" ../d_result1 " nodeset="/data/gw_fts_202402_v1/d_display_result2" relevant=" ../d_result1 != 'Negativo' and  ../d_add_results  = 'Sim'" type="string"/>
      <bind calculate="if( ../d_result1 =&quot;Positivo&quot; and  ../d_result2 =&quot;Positivo&quot;, &quot;Positivo&quot;,
 if( ../d_result1 =&quot;Positivo&quot; and  ../d_result2 =&quot;Negativo&quot;, &quot;Indeterminate&quot;,
 if( ../d_result1 =&quot;Positivo&quot; and  ../d_result2 =&quot;Invalid&quot;, &quot;Positivo&quot;,
 if( ../d_result1 =&quot;Invalid&quot; and  ../d_result2 =&quot;Positivo&quot;, &quot;Positivo&quot;,
 if( ../d_result1 =&quot;Invalid&quot; and  ../d_result2 =&quot;Invalid&quot;, &quot;Indeterminate&quot;,
 if( ../d_result1 =&quot;Invalid&quot; and  ../d_result2 =&quot;Negativo&quot;, &quot;Negativo&quot;, &quot;Negativo&quot;)
 )
 )
 )
 )
)" nodeset="/data/gw_fts_202402_v1/d_final_result" relevant=" ../d_add_results  = 'Sim'" type="string"/>
      <bind nodeset="/data/gw_fts_202402_v1/d_display_final_result" readonly="true()" relevant=" ../d_add_results  = 'Sim'" type="string"/>
      <bind nodeset="/data/gw_fts_202402_v1/d_Positivo_why_no_tx" relevant=" ../d_final_result  = 'Positivo' and  ../d_add_results  = 'Sim'" type="string"/>
      <bind nodeset="/data/gw_fts_202402_v1/notes_part" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/c_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/c_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
