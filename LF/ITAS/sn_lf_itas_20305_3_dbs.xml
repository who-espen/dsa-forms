      <bind constraint="regex(.,'^[0-9]{2}$')" jr:constraintMsg="jr:itext('/data/o_recorderID:jr:constraintMsg')" nodeset="/data/o_recorderID" required="true()" type="int"/>
      <bind nodeset="/data/o_eu_name" required="true()" type="string" db_get="col_1"/>
      <bind nodeset="/data/o_commune" required="true()" type="string" db_get="col_2" db_filter_by_col_1="/data/o_eu_name"/>
      <bind nodeset="/data/o_cluster_name" relevant=" /data/o_commune  != 'Saraya'" required="true()" type="string" db_get="col_3" db_filter_by_col_2="/data/o_commune"/>
      <bind constraint=". &gt; 99 and .&lt; 1000" jr:constraintMsg="jr:itext('/data/o_cluster_id:jr:constraintMsg')" nodeset="/data/o_cluster_id" relevant=" /data/o_commune  != 'Saraya'" required="true()" type="string" db_get="col_4" db_filter_by_col_3="/data/o_cluster_name"/>
      <bind nodeset="/data/o_nb_part" relevant=" /data/o_commune  != 'Saraya'" required="true()" type="int"/>
      <bind calculate="join(' ',  /data/sn_lf_o_2405/o_code_id )" nodeset="/data/C1" type="text"/>
      <bind calculate=" /data/o_nb_part " nodeset="/data/sn_lf_o_2405_count" readonly="true()" type="string"/>
      <bind nodeset="/data/sn_lf_o_2405" relevant=" /data/o_commune  != 'Saraya'"/>
      <bind calculate="position(..)" nodeset="/data/sn_lf_o_2405/C2" type="text"/>
      <bind calculate="if ( ../C2  = 1,'',substring-after( /data/C1 , ../o_code_id ))" nodeset="/data/sn_lf_o_2405/C3" type="text"/>
      <bind nodeset="/data/sn_lf_o_2405/o_IDType" required="true()" type="select1"/>
      <bind nodeset="/data/sn_lf_o_2405/o_BarcodeID" relevant=" ../o_IDType  = &quot;Scanner&quot;" required="true()" type="barcode"/>
      <bind nodeset="/data/sn_lf_o_2405/o_manual_code_id" relevant=" ../o_IDType  = &quot;Manuel&quot;" required="true()" type="string"/>
      <bind constraint=". =  ../o_manual_code_id " jr:constraintMsg="jr:itext('/data/sn_lf_o_2405/o_manual_code_id2:jr:constraintMsg')" nodeset="/data/sn_lf_o_2405/o_manual_code_id2" relevant=" ../o_IDType  = &quot;Manuel&quot;" required="true()" type="string"/>
      <bind calculate="if( ../o_IDType  = 'Scanner', concat( ../o_BarcodeID ),concat( /data/o_recorderID , '_',  /data/o_cluster_id , '_',  ../o_manual_code_id ))" constraint="not(selected( ../C3 ,  ../o_code_id ))" jr:constraintMsg="jr:itext('/data/sn_lf_o_2405/o_code_id:jr:constraintMsg')" nodeset="/data/sn_lf_o_2405/o_code_id" required="true()" type="string"/>
      <bind nodeset="/data/sn_lf_o_2405/o_result1" required="true()" type="select1"/>
      <bind nodeset="/data/sn_lf_o_2405/o_why_Invalid1" relevant=" ../o_result1  = 'Invalide'" required="true()" type="select"/>
      <bind nodeset="/data/sn_lf_o_2405/o_why_Invalio_other1" relevant="selected( ../o_why_Invalid1 ,'Other')" required="true()" type="string"/>
      <bind calculate=" ../o_result1 " nodeset="/data/sn_lf_o_2405/o_display_result1" required="true()" type="string"/>
      <bind nodeset="/data/sn_lf_o_2405/o_neeo_secono_test" readonly="true()" relevant=" ../o_result1  = 'Invalide'" type="string"/>
      <bind nodeset="/data/sn_lf_o_2405/o_result2" relevant=" ../o_result1  = 'Invalide'" required="true()" type="select1"/>
      <bind nodeset="/data/sn_lf_o_2405/o_why_Invalid2" relevant=" ../o_result2  = 'Invalide'" required="true()" type="select"/>
      <bind nodeset="/data/sn_lf_o_2405/o_why_Invalio_other2" relevant="selected( ../o_why_Invalid2 ,'Other')" required="true()" type="string"/>
      <bind calculate=" ../o_result1 " nodeset="/data/sn_lf_o_2405/o_display_result2" relevant=" ../o_result1  = 'Invalide'" type="string"/>
      <bind calculate="if( ../o_result1 =&quot;Invalide&quot; and  ../o_result2 =&quot;Positif&quot;, &quot;Positif&quot;,
 if( ../o_result1 =&quot;Invalide&quot; and  ../o_result2 =&quot;Invalide&quot;, &quot;indéterminé&quot;,
 if( ../o_result1 =&quot;Invalide&quot; and  ../o_result2 =&quot;Négatif&quot;, &quot;Négatif&quot;,
 if( ../o_result1 =&quot;Positif&quot;, &quot;Positif&quot;,
 if( ../o_result1 =&quot;Négatif&quot; , &quot;Négatif&quot;,&quot;Négatif&quot;
 )
 )
 )
 )
)" nodeset="/data/sn_lf_o_2405/o_final_result" type="string"/>
      <bind nodeset="/data/sn_lf_o_2405/o_display_final_result" readonly="true()" type="string"/>
      <bind nodeset="/data/sn_lf_o_2405/notes_part" type="text"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/o_start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/o_end" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
